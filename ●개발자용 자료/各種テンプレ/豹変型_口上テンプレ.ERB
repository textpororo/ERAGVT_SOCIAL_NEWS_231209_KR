@KOJO_0_BATTLE_CHARA_TRANSFORMCALL
SIF CHARATALENT_F(TARGET,0,"남자") > 0 || CHARATALENT_F(TARGET,1,"남자") == 0 || TALENT:임신 == 0
	CALL TRANSFORM, 1
TRYCALL KOJO_0_COLOR
;변신 시 구호설정
