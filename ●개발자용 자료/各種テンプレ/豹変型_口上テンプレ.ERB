	PRINTFORML 「%CSTR:TARGET:2%」
ELSE
	SELECTCASE RAND:10
		CASE 1
			PRINTFORML 「チェンジ」
		CASEELSE
			PRINTFORML 「変身」
	ENDSELECT
ENDIF



