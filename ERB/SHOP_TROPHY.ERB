@PRINT_ACHIEVEMENT(num,achievename,hint,bonus,desc,otherA,otherB)
#DIM num,1=0
#DIMS achievename,1=""
;실적解除後のテキスト
#DIMS desc,1=""
;ボーナスポイント
#DIM bonus,1=0
;解除前のヒントテキスト
#DIMS hint,1=""
;解除後テキスト(アンロック要素の通知)
#DIMS otherA,1=""
;解除前の補足テキスト(特定モード限定の場合にその旨を通知)
#DIMS otherB,1=""

;表示上の실적番号(内部のフラグとは無関係、ページ先頭で"init"を呼んで指定する)
#DIM numhead,1
#DIMS numtext


IF achievename=="init"
	PRINTL 실적名　　　　　　　　　　　　内容　　　　　　　　　　　　　　　　周回ボーナス
	CALL SHORTLINE
	numhead=num
	return
ENDIF

numtext'=(numhead<=9 ? "0" # "")+TOSTR(numhead)
numhead+=1

IF GLOBAL:num
	PRINTFORML 　%numtext% %achievename,28,LEFT%%desc,36,LEFT%+{bonus}
ELSE
	SETCOLOR 105,105,105
	PRINTFORML 　%numtext% ？？？　　　　　　　　　　　%hint,36,LEFT%？
	RESETCOLOR
ENDIF
IF GLOBAL:num
	PRINTFORML 　　　　　　　　　　　　　　　　%otherA,36,RIGHT%
ELSE
	PRINTFORML 　　　　　　　　　　　　　　　　%otherB,36,RIGHT%
ENDIF


