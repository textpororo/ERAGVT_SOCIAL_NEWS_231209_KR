;**********************************************************
;幽閉コマンド
;PRISON_COM103
;搾乳攻め　모유체질でなければ모유체질に
;**********************************************************
@PRISON_COM103
#DIM CCOUNT
VARSET LOCAL
TFLAG:10 = 103

;取得する珠
;LOCAL:0 쾌C
;LOCAL:1 쾌V
;LOCAL:2 쾌A
;LOCAL:3 쾌B
;LOCAL:4 윤활 윤활は未使用
;LOCAL:5 공순
;LOCAL:6 습득
;LOCAL:7 욕정
;LOCAL:8 굴복
;LOCAL:9 치정
;LOCAL:10 고통
;LOCAL:11 공포

;経験数の上量値記録用
;LOCAL:120 V経験
;LOCAL:121 A経験
;LOCAL:123 정액경험
;LOCAL:124 펠라경험
;LOCAL:130 노출쾌락경험,
;LOCAL:131 봉사쾌락경험,
;LOCAL:132 고통쾌락경험,
;LOCAL:140 자위경험,
;LOCAL:150 이상경험
;LOCAL:151 V확장경험
;LOCAL:152 A확장경험
;LOCAL:153 사정경험
;LOCAL:154 모유경험
;LOCAL:155 방뇨경험
;LOCAL:156 기생경험
;LOCAL:157 출산경험

;汚染度
CFLAG:30 += 10 + RAND:5

IF TALENT:모유체질 == 0
	;모유체질にする
	TALENT:모유체질 = 1
	
	;공포　固定で
	LOCAL:11 = 800
	
	;지문: 흑화 캐릭터가 幽閉中の操作キャラを모유체질化した
	SIF CFLAG:20 == 2
		CALL MESSAGE_OTHER_PRISON_BONYU
	;지문: 모유체질化
	CALL MESSAGE_PRISON_BONYU


ELSE
;快B
	IF ABL:B감각 == 0
		LOCAL:3 = 100
	ELSEIF ABL:B감각 == 1
		LOCAL:3 = 150
	ELSEIF ABL:B감각 == 2
		LOCAL:3 = 200
	ELSEIF ABL:B감각 == 3
		LOCAL:3 = 600
	ELSEIF ABL:B감각 == 4
		LOCAL:3 = 1000
	ELSEIF ABL:B감각 >= 5
		LOCAL:3 = 1500
	ENDIF

	;メイン部位絶頂見込みの予測
	CALL PALAM_VABCestimate(LOCAL,3,-1)


	
	;습득
	IF ABL:봉사정신 == 0
		LOCAL:6 = 5
	ELSEIF ABL:봉사정신 == 1
		LOCAL:6 = 12
	ELSEIF ABL:봉사정신 == 2
		LOCAL:6 = 25
	ELSEIF ABL:봉사정신 == 3
		LOCAL:6 = 40
	ELSEIF ABL:봉사정신 == 4
		LOCAL:6 = 62
	ELSEIF ABL:봉사정신 >= 5
		LOCAL:6 = 90
	ENDIF

	;지문: 흑화 캐릭터가 幽閉中の操作キャラに搾乳攻めをした
	SIF CFLAG:20 == 2
		CALL MESSAGE_OTHER_PRISON_COM_103
	;지문: 搾乳攻め
	CALL MESSAGE_PRISON_COM_103
ENDIF

;습득基本値
LOCAL:6 += 20

;욕정基本値
LOCAL:7 += 200

;굴복基本値
LOCAL:8 += 100

;치정基本値
LOCAL:9 += 100

;고통基本値
LOCAL:10 += 0

;공포基本値
LOCAL:11 += 0

;幽閉中パラメータ変動共通処理へ
CALL COMMON_PRISON, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11

;経験数の増加
CALL COMMON_PRISON_EXP_SH, LOCAL:120, LOCAL:121, LOCAL:151, LOCAL:152
LOCAL:120 = RESULT:0
LOCAL:121 = RESULT:1
LOCAL:151 = RESULT:2
LOCAL:152 = RESULT:3

;拡張度による拡張経験
CALL PRISON_GAPING, LOCAL:120, LOCAL:121, LOCAL:151, LOCAL:152, LOCAL:152, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3
LOCAL:151 += RESULT:0
LOCAL:152 += RESULT:1

;表示と増加の処理
FOR CCOUNT, 100, 200
	CALL COMMON_PRISON_EXP, CCOUNT, LOCAL:CCOUNT
NEXT

PRINTL 

;珠の取得へ
CALL _ABLUP, 1
