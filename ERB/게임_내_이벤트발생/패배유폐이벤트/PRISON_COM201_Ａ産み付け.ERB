;**********************************************************
;유폐コマンド
;PRISON_COM201
;Aに産卵
;**********************************************************
@PRISON_COM201
#DIM CCOUNT
VARSET LOCAL
TFLAG:10 = 201

;取得する珠
;LOCAL:0 쾌Ｃ
;LOCAL:1 쾌Ｖ
;LOCAL:2 쾌Ａ
;LOCAL:3 쾌Ｂ
;LOCAL:4 윤활 윤활は未使用
;LOCAL:5 공순
;LOCAL:6 습득
;LOCAL:7 욕정
;LOCAL:8 굴복
;LOCAL:9 치정
;LOCAL:10 고통
;LOCAL:11 공포

;経験数の上量値記録用
;LOCAL:120 V経験
;LOCAL:121 A経験
;LOCAL:123 정액경험
;LOCAL:124 펠라경험
;LOCAL:130 노출쾌락경험,
;LOCAL:131 봉사쾌락경험,
;LOCAL:132 고통쾌락경험,
;LOCAL:140 자위경험,
;LOCAL:150 이상경험
;LOCAL:151 Ｖ확장경험
;LOCAL:152 Ａ확장경험
;LOCAL:153 사정경험
;LOCAL:154 모유경험
;LOCAL:155 방뇨경험
;LOCAL:156 기생경험
;LOCAL:157 출산경험

;汚染度
CFLAG:30 += 10 + RAND:10

;근친교배경험
SIF INCEST_F(TARGET, CFLAG:20 * 100 + CFLAG:21, 1) > 0
	LOCAL:141 += 1

;쾌Ａ
IF ABL:Ａ감각 == 0
	LOCAL:121 = 5
	LOCAL:152 = 1
	LOCAL:2 = 300
ELSEIF ABL:Ａ감각 == 1
	LOCAL:121 = 8
	LOCAL:152 = 1
	LOCAL:2 = 450
ELSEIF ABL:Ａ감각 == 2
	LOCAL:121 = 11
	LOCAL:152 = 2
	LOCAL:2 = 600
ELSEIF ABL:Ａ감각 == 3
	LOCAL:121 = 15
	LOCAL:152 = 2
	LOCAL:2 = 800
ELSEIF ABL:Ａ감각 == 4
	LOCAL:121 = 20
	LOCAL:152 = 4
	LOCAL:2 = 1200
ELSEIF ABL:Ａ감각 >= 5
	LOCAL:121 = 30
	LOCAL:152 = 8
	LOCAL:2 = 2000
ENDIF
;출산경험이 少ない場合は쾌Ａが入りにくい
IF EXP:출산경험 < 3
	TIMES LOCAL:2, 0.30
ELSEIF EXP:출산경험 < 5
	TIMES LOCAL:2, 0.50
ELSEIF EXP:출산경험 < 8
	TIMES LOCAL:2, 0.80
ELSE
ENDIF

;메인 부위 절정 예정의 예측
CALL PALAM_VABCestimate(LOCAL,2,-1)



;공순
;출산경험이 多いと공순が入る
IF EXP:출산경험 < 3
ELSEIF EXP:출산경험 < 5
	LOCAL:5 = 200
ELSEIF EXP:출산경험 < 8
	LOCAL:5 = 1000
ELSE
	LOCAL:5 = 5000
ENDIF

;굴복
;출산경험に応じて
IF EXP:출산경험 < 3
	LOCAL:8 = 500
ELSEIF EXP:출산경험 < 5
	LOCAL:8 = 1000
ELSEIF EXP:출산경험 < 8
	LOCAL:8 = 2000
ELSE
	LOCAL:8 = 5000
ENDIF

;치정
;출산경험이 低いと치정が入る
IF EXP:출산경험 < 3
	LOCAL:9 = 800
ELSEIF EXP:출산경험 < 5
	LOCAL:9 = 400
ELSEIF EXP:출산경험 < 8
	LOCAL:9 = 200
ELSE
	LOCAL:9 = 50
ENDIF

;고통
;출산경험이 低いと入る
IF EXP:출산경험 < 3
	LOCAL:10 = 1500
ELSEIF EXP:출산경험 < 5
	LOCAL:10 = 1000
ELSEIF EXP:출산경험 < 8
	LOCAL:10 = 500
ELSE
	LOCAL:10 = 200
ENDIF

;공포
;출산경험이 低いと入る
IF EXP:출산경험 < 3
	LOCAL:11 = 1500
ELSEIF EXP:출산경험 < 5
	LOCAL:11 = 1000
ELSEIF EXP:출산경험 < 8
	LOCAL:11 = 500
ELSE
	LOCAL:11 = 200
ENDIF

;지문: Aに産卵
CALL MESSAGE_PRISON_COM_201

;습득 기본값
LOCAL:6 += 0

;욕정 기본값
LOCAL:7 += 0

;굴복 기본값
LOCAL:8 += 500

;치정 기본값
LOCAL:9 += 0

;고통 기본값
LOCAL:10 += 500

;공포 기본값
LOCAL:11 += 500

;初めてのA出産の場合、이상경험이 入る
IF CFLAG:205 == 0
	CFLAG:205 = 1
	LOCAL:150 = 1
ENDIF

;유폐中パラメータ変動共通処理へ
CALL COMMON_PRISON, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11

;経験数の増加
CALL COMMON_PRISON_EXP_SH, LOCAL:120, LOCAL:121, LOCAL:151, LOCAL:152
LOCAL:120 = RESULT:0
LOCAL:121 = RESULT:1
LOCAL:151 = RESULT:2
LOCAL:152 = RESULT:3

;拡張度による拡張経験
CALL PRISON_GAPING, LOCAL:120, LOCAL:121, LOCAL:151, LOCAL:152, LOCAL:152, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3
LOCAL:151 += RESULT:0
LOCAL:152 += RESULT:1

;表示と増加の処理
FOR CCOUNT, 100, 200
	CALL COMMON_PRISON_EXP, CCOUNT, LOCAL:CCOUNT
NEXT

PRINTL 

;珠の取得へ
CALL _ABLUP, 1
