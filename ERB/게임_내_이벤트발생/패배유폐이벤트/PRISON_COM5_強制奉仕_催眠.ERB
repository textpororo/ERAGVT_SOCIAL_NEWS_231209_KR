;**********************************************************
;유폐コマンド
;PRISON_COM5
;強制奉仕
;**********************************************************
@PRISON_COM5
#DIM CCOUNT
VARSET LOCAL
TFLAG:10 = 5

;取得する珠
;LOCAL:0 쾌Ｃ
;LOCAL:1 쾌Ｖ
;LOCAL:2 쾌Ａ
;LOCAL:3 쾌Ｂ
;LOCAL:4 윤활 윤활は未使用
;LOCAL:5 공순
;LOCAL:6 습득
;LOCAL:7 욕정
;LOCAL:8 굴복
;LOCAL:9 치정
;LOCAL:10 고통
;LOCAL:11 공포

;経験数の上量値記録用
;LOCAL:120 V経験
;LOCAL:121 A経験
;LOCAL:123 정액경험
;LOCAL:124 펠라경험
;LOCAL:130 노출쾌락경험,
;LOCAL:131 봉사쾌락경험,
;LOCAL:132 고통쾌락경험,
;LOCAL:140 자위경험,
;LOCAL:150 이상경험
;LOCAL:151 Ｖ확장경험
;LOCAL:152 Ａ확장경험
;LOCAL:153 사정경험
;LOCAL:154 모유경험
;LOCAL:155 방뇨경험
;LOCAL:156 기생경험
;LOCAL:157 출산경험

;汚染度
CFLAG:30 += 10 + RAND:6

;근친교배경험
SIF INCEST_F(TARGET, CFLAG:20 * 100 + CFLAG:21, 1) > 0
	LOCAL:141 += 1

;쾌Ｃ
IF ABL:Ｃ감각 == 0
	LOCAL:0 = 1
ELSEIF ABL:Ｃ감각 == 1
	LOCAL:0 = 2
ELSEIF ABL:Ｃ감각 == 2
	LOCAL:0 = 20
ELSEIF ABL:Ｃ감각 == 3
	LOCAL:0 = 100
ELSEIF ABL:Ｃ감각 == 4
	LOCAL:0 = 200
ELSEIF ABL:Ｃ감각 >= 5
	LOCAL:0 = 400
ENDIF

;쾌Ｖ
IF ISFEMALE()
	IF ABL:Ｖ감각 == 0
		LOCAL:120 = 2
		LOCAL:1 = 1
	ELSEIF ABL:Ｖ감각 == 1
		LOCAL:120 = 5
		LOCAL:1 = 2
	ELSEIF ABL:Ｖ감각 == 2
		LOCAL:120 = 8
		LOCAL:1 = 20
	ELSEIF ABL:Ｖ감각 == 3
		LOCAL:120 = 11
		LOCAL:1 = 100
	ELSEIF ABL:Ｖ감각 == 4
		LOCAL:120 = 15
		LOCAL:1 = 200
	ELSEIF ABL:Ｖ감각 >= 5
		LOCAL:120 = 20
		LOCAL:1 = 400
	ENDIF
ENDIF

;쾌Ａ
;A経験がないならAには入れない
IF EXP:Ａ경험 > 0
	IF ABL:Ａ감각 == 0
		LOCAL:121 = 2
		LOCAL:2 = 1
	ELSEIF ABL:Ａ감각 == 1
		LOCAL:121 = 5
		LOCAL:2 = 2
	ELSEIF ABL:Ａ감각 == 2
		LOCAL:121 = 8
		LOCAL:2 = 20
	ELSEIF ABL:Ａ감각 == 3
		LOCAL:121 = 11
		LOCAL:2 = 100
	ELSEIF ABL:Ａ감각 == 4
		LOCAL:121 = 15
		LOCAL:2 = 200
	ELSEIF ABL:Ａ감각 >= 5
		LOCAL:121 = 20
		LOCAL:2 = 400
	ENDIF
ENDIF

;쾌Ｂ
IF ABL:Ｂ감각 == 0
	LOCAL:3 = 1
ELSEIF ABL:Ｂ감각 == 1
	LOCAL:3 = 2
ELSEIF ABL:Ｂ감각 == 2
	LOCAL:3 = 20
ELSEIF ABL:Ｂ감각 == 3
	LOCAL:3 = 100
ELSEIF ABL:Ｂ감각 == 4
	LOCAL:3 = 200
ELSEIF ABL:Ｂ감각 >= 5
	LOCAL:3 = 400
ENDIF

;메인 부위 절정 예정의 예측
CALL PALAM_VABCestimate(LOCAL,0,1,2,3,-1)



;공순
IF ABL:순종 == 2
	LOCAL:5 = 200
ELSEIF ABL:순종 == 3
	LOCAL:5 = 500
ELSEIF ABL:순종 == 4
	LOCAL:5 = 1000
ELSEIF ABL:순종 >= 5
	LOCAL:5 = 2000
ENDIF

;습득
IF ABL:봉사정신 == 0
	LOCAL:6 = 10
ELSEIF ABL:봉사정신 == 1
	LOCAL:6 = 25
ELSEIF ABL:봉사정신 == 2
	LOCAL:6 = 50
ELSEIF ABL:봉사정신 == 3
	LOCAL:6 = 80
ELSEIF ABL:봉사정신 == 4
	LOCAL:6 = 125
ELSEIF ABL:봉사정신 >= 5
	LOCAL:6 = 180
ENDIF

;굴복
IF ABL:순종 == 0
	LOCAL:8 = 500
ELSEIF ABL:순종 == 1
	LOCAL:8 = 1000
ELSEIF ABL:순종 == 2
	LOCAL:8 = 1500
ELSEIF ABL:순종 == 3
	LOCAL:8 = 2500
ELSEIF ABL:순종 == 4
	LOCAL:8 = 3500
ELSEIF ABL:순종 >= 5
	LOCAL:8 = 5000
ENDIF

;봉사정신에 따라 보너스
IF ABL:봉사정신 < 3
ELSEIF ABL:봉사정신 < 4
	LOCAL:5 += 500
	LOCAL:6 += 100
ELSEIF ABL:봉사정신 < 5
	LOCAL:5 += 1000
	LOCAL:6 += 500
ELSE
	LOCAL:5 += 2000
	LOCAL:6 += 1000
ENDIF

;순종이 낮으면 공포가 들어간다　일단 고정
SIF ABL:순종 < 3
	LOCAL:11 = 500

;처녀であれば처녀を破る
IF TALENT:처녀 == 1 && CHECK_HOLYVIRGIN_F()==0 && SHIELD:1 <= 0
	LOCAL:10 = 500
	CALL LOSTVIRGIN, 1
ENDIF

;습득 기본값
LOCAL:6 += 20

;욕정 기본값
LOCAL:7 += 0

;굴복 기본값
LOCAL:8 += 100

;치정 기본값
LOCAL:9 += 100

;고통 기본값
LOCAL:10 += 100

;공포 기본값
LOCAL:11 += 100

;정액경험
LOCAL:123 = 25
;펠라경험
LOCAL:124 = 10
;봉사쾌락경험
IF ABL:봉사정신 == 0
	LOCAL:131 = 1
ELSEIF ABL:봉사정신 == 1
	LOCAL:131 = 2
ELSEIF ABL:봉사정신 == 2
	LOCAL:131 = 4
ELSEIF ABL:봉사정신 == 3
	LOCAL:131 = 8
ELSEIF ABL:봉사정신 == 4
	LOCAL:131 = 16
ELSEIF ABL:봉사정신 >= 5
	LOCAL:131 = 32
ENDIF

;지문: 흑화 캐릭터가 유폐 중인 조작 캐릭터에게 강제로 봉사시켰다
SIF CFLAG:20 == 2
	CALL MESSAGE_OTHER_PRISON_COM_5
;지문: 強制奉仕
CALL MESSAGE_PRISON_COM_5

;유폐中パラメータ変動共通処理へ
CALL COMMON_PRISON, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11

;経験数の増加
CALL COMMON_PRISON_EXP_SH, LOCAL:120, LOCAL:121, LOCAL:151, LOCAL:152
LOCAL:120 = RESULT:0
LOCAL:121 = RESULT:1
LOCAL:151 = RESULT:2
LOCAL:152 = RESULT:3

;拡張度による拡張経験
CALL PRISON_GAPING, LOCAL:120, LOCAL:121, LOCAL:151, LOCAL:152, LOCAL:152, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3
LOCAL:151 += RESULT:0
LOCAL:152 += RESULT:1

;表示と増加の処理
FOR CCOUNT, 100, 200
	CALL COMMON_PRISON_EXP, CCOUNT, LOCAL:CCOUNT
NEXT

PRINTL 

;珠の取得へ
CALL _ABLUP, 1
;임신
SIF SHIELD:1 <= 0
	TRYCALL NINSIN_HANTEI,25,30

