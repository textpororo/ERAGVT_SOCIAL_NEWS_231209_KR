;-----------------------------------------------------------------------------------------
;経験増加・임신 판정など	ARG:0 == 部位渡し, ARG:1 == SHOJO(性交の種類), ARG:2 == S_질내精液
;-----------------------------------------------------------------------------------------
@CALC_INKIOKYU, ARG:0, ARG:1 = 0, ARG:2 = 0
#DIM CCOUNT
#DIM SHOJO
#DIM S_回数
#DIM S_질내精液
#DIM C_計算元
#DIM 部位

VARSET LOCAL

;(部位渡し 손 == 1 割れ目 == 2 下着 ==  3 脚 == 4 尻肉 == 5 顔 == 6 双丘 == 7)
部位 = ARG:0
SHOJO = ARG:1
S_질내精液 = ARG:2

;総処置数
S_回数 = 12 + RAND:5
;정액경험
LOCAL:123 = 15 + RAND:16
;ACTION_SEISANを参考に基準値を定義
C_計算元 = BASE:체력 * (10 + RAND:3) / 100

;Ｖ경험	Ａ경험---------------------------------------------
IF SHOJO == 1
	;Ｖ경험
	LOCAL:120 += 1
ELSEIF SHOJO == 2
	;Ａ경험
	LOCAL:121 += 1
ELSEIF SHOJO == 3
	IF ABL:Ｖ감각 >= 4 && ABL:Ａ감각 >= 4
		;Ｖ경험
		LOCAL:120 += 2 + RAND:5
		;Ａ경험
		LOCAL:121 += 2 + RAND:5
	ELSEIF ABL:Ｖ감각 >= ABL:Ａ감각
		;Ｖ경험
		LOCAL:120 += 4 + RAND:5
	ELSE
		;Ａ경험
		LOCAL:121 += 4 + RAND:5
	ENDIF
ELSEIF SHOJO == 4
	;Ａ경험
	LOCAL:121 += 4 + RAND:5
ELSEIF SHOJO == 5
	;Ｖ경험
	LOCAL:120 = S_回数 - 6
ELSEIF SHOJO == 6
	;Ａ경험
	LOCAL:121 = S_回数 - 6
ENDIF

;피간경험--------------------------------------------------
SIF SHOJO == 1 || SHOJO == 2 || SHOJO == 5 || SHOJO == 6
	LOCAL:110 += 1

;絶頂、奉仕快楽、사정、모유、방뇨경험----------------------
IF SHOJO == 1 || SHOJO == 2
	LOCAL:122 += 1
	IF (部位 == 7 && ABL:Ｂ감각 >= 4) || (部位 == 6 && ABL:정액중독 >= 4) || (部位 == 3 && ABL:Ｃ감각 >= 3) || (部位 == 2 && ABL:Ｃ감각 >= 2)
		;절정경험
		LOCAL:122 += C_計算元 * (1 + RAND:2) / 100 + RAND:4
		;봉사쾌락경험
		LOCAL:131 += C_計算元 * (2 + RAND:3) / 100
		SIF ISPENIS()
			LOCAL:153 += C_計算元 * (1 + RAND:3) / 100
		SIF TALENT:모유체질 > 0
			LOCAL:154 += C_計算元 * (1 + RAND:3) / 100
		SIF TALENT:오줌싸개버릇 > 0
			LOCAL:155 += C_計算元 * (1 + RAND:3) / 100
	ENDIF
ELSEIF SHOJO == 3 || SHOJO == 4
	;절정경험
	SIF (部位 == 7 && ABL:Ｂ감각 >= 4) || (部位 == 6 && ABL:정액중독 >= 4) || (部位 == 3 && ABL:Ｃ감각 >= 3) || (部位 == 2 && ABL:Ｃ감각 >= 2)
		LOCAL:122 += C_計算元 * (1 + RAND:2) / 100
	IF ABL:Ｖ감각 >= 4 && ABL:Ａ감각 >= 4
		LOCAL:122 += C_計算元 * (1 + RAND:2) / 100 + RAND:(1 + (ABL:Ｖ감각 + ABL:Ａ감각) / 4)
	ELSEIF ABL:Ｖ감각 >= ABL:Ａ감각
		LOCAL:122 += C_計算元 * (1 + RAND:2) / 100 + RAND:(2 + (ABL:Ｖ감각 + 4) / 4)
	ELSE
		LOCAL:122 += C_計算元 * (1 + RAND:2) / 100 + RAND:(2 + (ABL:Ａ감각 + 4) / 4)
	ENDIF
	;봉사쾌락경험
	LOCAL:131 += C_計算元 * (2 + RAND:3) / 100
	SIF ISPENIS()
		LOCAL:153 += C_計算元 * (1 + RAND:3) / 100
	SIF TALENT:모유체질 > 0
		LOCAL:154 += C_計算元 * (1 + RAND:3) / 100
	SIF TALENT:오줌싸개버릇 > 0
		LOCAL:155 += C_計算元 * (1 + RAND:3) / 100

ELSEIF SHOJO == 5 || SHOJO == 6
	;절정경험
	IF SHOJO == 5
		LOCAL:122 += C_計算元 * (1 + RAND:2) / 100 + RAND:(2 + (ABL:Ｖ감각 + 4) / 4)
	ELSEIF SHOJO == 6
		LOCAL:122 += C_計算元 * (1 + RAND:2) / 100 + RAND:(2 + (ABL:Ａ감각 + 4) / 4)
	ENDIF
	;봉사쾌락경험
	LOCAL:131 += C_計算元 * (2 + RAND:3) / 100
	SIF ISPENIS()
		LOCAL:153 += C_計算元 * (1 + RAND:3) / 100
	SIF TALENT:모유체질 > 0
		LOCAL:154 += C_計算元 * (1 + RAND:3) / 100
	SIF TALENT:오줌싸개버릇 > 0
		LOCAL:155 += C_計算元 * (1 + RAND:3) / 100
ELSE
	IF (部位 == 7 && ABL:Ｂ감각 >= 4) || (部位 == 6 && ABL:정액중독 >= 4) || (部位 == 3 && ABL:Ｃ감각 >= 3) || (部位 == 2 && ABL:Ｃ감각 >= 2)
		;절정경험
		LOCAL:122 += C_計算元 * (1 + RAND:2) / 100 + RAND:4
		;봉사쾌락경험
		LOCAL:131 += C_計算元 * (2 + RAND:3) / 100
		SIF ISPENIS()
			LOCAL:153 += C_計算元 * (1 + RAND:3) / 100
		SIF TALENT:모유체질 > 0
			LOCAL:154 += C_計算元 * (1 + RAND:3) / 100
		SIF TALENT:오줌싸개버릇 > 0
			LOCAL:155 += C_計算元 * (1 + RAND:3) / 100
	ENDIF
ENDIF
;펠라경험------------------------------------------------
;(セックスの分、フェラ回数が減る。フェラ処理以外はさらに減る)
IF 部位 == 6 || SHOJO == 3 || SHOJO == 4
	LOCAL:124 = MAX(S_回数 - (LOCAL:120 + LOCAL:121), 0)
ELSEIF 部位 == 1 || 部位 == 7 || SHOJO == 5 || SHOJO == 6
	LOCAL:124 = MAX(S_回数 - (LOCAL:120 + LOCAL:121 + RAND:3), 0)
ELSE
	LOCAL:124 = MAX(S_回数 - (LOCAL:120 + LOCAL:121 + 4 + RAND:6), 0)
ENDIF

;임신 판정--------------------------------------------------
;(セックス回数＋ランダム대량사정、정액경험でキャップ)
IF SHOJO == 1
	S_질내精液 += MIN(LOCAL:120 + 10 + RAND:6, LOCAL:123)
ELSEIF SHOJO == 3 && ((ABL:Ｖ감각 >= 4 && ABL:Ａ감각 >= 4) || ABL:Ｖ감각 >= ABL:Ａ감각)
	S_질내精液 += MIN((LOCAL:120 + (LOCAL:120 * RAND:3)), LOCAL:123)
ELSEIF SHOJO == 5
	S_질내精液 += MIN((LOCAL:120 + (LOCAL:120 * RAND:4)), LOCAL:123)
ENDIF

