;Urethra(尿道)の淫語
;**********************************************************
@S_SLANG_U(ARG)
#FUNCTIONS
#DIMS S_SLANGWORD_U

;초기화
S_SLANGWORD_U=

;第一形容詞：状況（汁など）
IF ISMALE()
	IF TALENT:음핵
		S_SLANGWORD_U+="精子焼けしている"
	ELSE
		SELECTCASE RAND:4
			CASE 3
				S_SLANGWORD_U+="尿路"
			CASE 2
				S_SLANGWORD_U+="小水道"
			CASE 1
				S_SLANGWORD_U+="おしっこの路"
			CASEELSE
				S_SLANGWORD_U+="尿道"
		ENDSELECT
	ENDIF
ELSEIF TALENT:후타나리 > 0
	IF TALENT:음핵
		S_SLANGWORD_U+="精液の染み付いた"
	ELSE
		SELECTCASE RAND:5
			CASE 4
				S_SLANGWORD_U+="膀胱"
			CASE 3
				S_SLANGWORD_U+="尿路"
			CASE 2
				S_SLANGWORD_U+="小水道"
			CASE 1
				S_SLANGWORD_U+="おしっこの路"
			CASEELSE
				S_SLANGWORD_U+="尿道"
		ENDSELECT
	ENDIF
ELSE
	IF TALENT:음핵
		S_SLANGWORD_U+="精液の染み付いた"
	ELSE
		SELECTCASE RAND:4
			CASE 3
				S_SLANGWORD_U+="尿路"
			CASE 2
				S_SLANGWORD_U+="小水道"
			CASE 1
				S_SLANGWORD_U+="おしっこの路"
			CASEELSE
				S_SLANGWORD_U+="尿道"
		ENDSELECT
	ENDIF
ENDIF
RETURNF S_SLANGWORD_U

