;Anal(애널)の淫語
;**********************************************************
@S_SLANG_A(ARG)
#DIMS S_SLANGWORD_A
#FUNCTIONS

;初期化
S_SLANGWORD_A=


;PALAM:쾌A> 2000

;部位呼称

IF (TALENT:음고>0) || (TALENT:음란>0) || (PALAM:쾌A>2000)
	SELECTCASE RAND:4
		CASE 3
			S_SLANGWORD_A += "尻オナホ"
		CASE 2
			S_SLANGWORD_A += "애널"
		CASEELSE
			;尻マンコ
			SELECTCASE RAND:2
				CASE 1
					S_SLANGWORD_A += "尻"
				CASEELSE
					S_SLANGWORD_A += "ケツ"
			ENDSELECT
			S_SLANGWORD_A += "マンコ"
	ENDSELECT
ELSEIF (TALENT:청순파>0) && (PALAM:쾌A<2000)
	SELECTCASE RAND:4
		CASE 3
			S_SLANGWORD_A += "不浄"
		CASE 2
			S_SLANGWORD_A += "うんち"
		CASE 1
			S_SLANGWORD_A += "後ろ"
		CASEELSE
			S_SLANGWORD_A += "お尻"
	ENDSELECT
	S_SLANGWORD_A += "の穴"

ELSE
	SELECTCASE RAND:5
		CASE 4
			S_SLANGWORD_A += "排泄器官"
		CASE 3
			S_SLANGWORD_A += "애널"
		CASE 2
			S_SLANGWORD_A += "お尻"
		CASE 1
			S_SLANGWORD_A += "菊"
			SELECTCASE RAND:3
				CASE 2
					S_SLANGWORD_A += "花"
				CASE 1
					S_SLANGWORD_A += "門"
				CASEELSE
					S_SLANGWORD_A += "穴"
			ENDSELECT
		CASEELSE
	ENDSELECT
ENDIF

RETURNF S_SLANGWORD_A


