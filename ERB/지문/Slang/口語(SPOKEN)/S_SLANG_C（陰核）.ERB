;Clitoris(클리토리스)の淫語
;**********************************************************
@S_SLANG_C(ARG)
#FUNCTIONS
#DIMS S_SLANGWORD_C

;初期化
S_SLANGWORD_C=

;남자や후타나리は、ペニスをコール
IF ISMALE() ||  TALENT:후타나리 > 0
	S_SLANGWORD_C=%S_SLANG_P(TARGET)%
ELSE
	IF TALENT:음핵
		S_SLANGWORD_C+="음핵"
	ELSE
		SELECTCASE RAND:3
			CASE 2
				S_SLANGWORD_C+="お豆さん"
			CASE 1
				S_SLANGWORD_C+="エッチな突起"
			CASEELSE
				S_SLANGWORD_C+="클리토리스"
		ENDSELECT
	ENDIF
ENDIF
RETURNF S_SLANGWORD_C

