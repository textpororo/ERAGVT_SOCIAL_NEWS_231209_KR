;Penis(ペニス)の淫語
;**********************************************************
@SLANG_P(ARG)
#FUNCTIONS
#DIMS SLANGWORD_P

;初期化
SLANGWORD_P=


;第一形容詞：状況（汁など）
IF TALENT:백보지 == 1
	SELECTCASE RAND:3
		CASE 2
		SLANGWORD_P+="不毛な"

		CASE 1
		SLANGWORD_P+="백보지の"

		CASEELSE
		SLANGWORD_P+="毛も生えていない"

	ENDSELECT
ENDIF

SELECTCASE TALENT:후타나리
	;定着후타나리
	CASE 4
		;第二形容詞：形状表現
		;第三形容詞：動作表現
		SLANGWORD_P+="定着후타나리"


	;天然후타나리
	CASE 3
		;第二形容詞：形状表現
		;第三形容詞：動作表現
		SLANGWORD_P+="天然후타나리"


	;기생후타나리
	CASE 2
		;第二形容詞：形状表現
		;第三形容詞：動作表現
		SLANGWORD_P+="기생후타나리"


	;後天후타나리
	CASE 1
		;第二形容詞：形状表現
		;第三形容詞：動作表現
		SLANGWORD_P+="後天후타나리"


	;후타나리以外
	CASEELSE
		;第二形容詞：形状表現
		;第三形容詞：動作表現
		;部位呼称
		IF ISMALE()
			IF TALENT:음핵
				SLANGWORD_P+="음핵"
			ELSE
				SELECTCASE RAND:7
					CASE 6
						SLANGWORD_P+="ちんぽ"
					CASE 5
						SLANGWORD_P+="陰茎"
					CASE 4
						SLANGWORD_P+="男性器"
					CASE 3
						SLANGWORD_P+="ちんちん"
					CASE 2
						SLANGWORD_P+="男根"
					CASE 1
						SLANGWORD_P+="ペニス"
					CASEELSE
						SLANGWORD_P+="ちんこ"
				ENDSELECT
			ENDIF
		ENDIF

ENDSELECT







RETURNF SLANGWORD_P

