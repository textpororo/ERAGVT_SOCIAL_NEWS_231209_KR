;CFLAG:279 意中相手の固有番号
;CFLAG:280 意中相手の現在位置

;얀데레専用サーチ処理
;--------------------------------------------------
@YANDERE_SEARCH
;対象サーチ
CFLAG:280 = 0
FOR LOCAL,0,CHARANUM
	SIF CFLAG:LOCAL:240 == CFLAG:279
		CFLAG:280 = LOCAL
NEXT

;얀데레専用初期設定
;--------------------------------------------------
@YANDERE_FIRST_SETTING
;まずは思い人を決めよう
CALL LB
PRINTFORML %CALLNAME%の思い人を選択してください。
FOR LOCAL, 0, CHARANUM
	SIF LOCAL == MASTER || LOCAL == TARGET
		CONTINUE
	VARSET RESULTS
	CALL GET_RELATION, TARGET, LOCAL
	PRINTFORML 　[{LOCAL}] %NAME:LOCAL, 24,LEFT%　%RESULTS%
NEXT
PRINTFORML [99]ランダム
$INPUT_LOOP
INPUT
IF RESULT >= 0 && RESULT <= CHARANUM && RESULT != MASTER && RESULT != TARGET
	CFLAG:279 = RESULT
ELSEIF RESULT == 99
	$LOOP_0
	CFLAG:279 = (RAND:(CHARANUM - 1) + 1)
	SIF CFLAG:279 == 0 || CFLAG:279 == CFLAG:240
		GOTO LOOP_0
ELSE
	GOTO INPUT_LOOP
ENDIF
;相手の名前を心に刻みつけよう
CSTR:20 = %CALLNAME:(CFLAG:279)%
CALL YANDERE_SEARCH
DRAWLINE


