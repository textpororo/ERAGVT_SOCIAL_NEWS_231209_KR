	CALLFORM KOJO_フェイト_BATTLE_CHARA_BATTLE_STYLE_CHANGE


@KOJO_フェイト_BATTLE_CHARA_BATTLE_STYLE_CHANGE
IF ENEMY_TYPE_CHECK_F("MOB") == 1
	SELECTCASE RAND:2
		CASE 0
		CDFLAG:TARGET:근거리:전투스타일=2;装甲
		CDFLAG:TARGET:중거리:전투스타일=2;装甲
		CDFLAG:TARGET:원거리:전투스타일=2;装甲
		CASE 2
		CDFLAG:TARGET:근거리:전투스타일=8;設置
		CDFLAG:TARGET:중거리:전투스타일=8;設置
		CDFLAG:TARGET:원거리:전투스타일=8;設置
		CASEELSE
		CDFLAG:TARGET:근거리:전투스타일=7;知略
		CDFLAG:TARGET:중거리:전투스타일=7;知略
		CDFLAG:TARGET:원거리:전투스타일=7;知略
	ENDSELECT
ELSE
	;敵のHPが1/30かつ3000以下
	IF (FLAG:13 < FLAG:12 /30) && (FLAG:13 <3000)
		CDFLAG:TARGET:근거리:전투스타일=6;全力
		CDFLAG:TARGET:중거리:전투스타일=6;全力
		CDFLAG:TARGET:원거리:전투스타일=6;全力
	;敵のHPが半分以上
	ELSEIF (FLAG:13 > FLAG:12 /2)
		CDFLAG:TARGET:근거리:전투스타일=1;連続
		CDFLAG:TARGET:중거리:전투스타일=1;連続
		CDFLAG:TARGET:원거리:전투스타일=1;連続
	;敵のHPが半分以上で、通常공격
	ELSE
		CDFLAG:TARGET:근거리:전투스타일=0;通常
		CDFLAG:TARGET:중거리:전투스타일=0;通常
		CDFLAG:TARGET:원거리:전투스타일=3;撹乱
	ENDIF
ENDIF


