;--------------------------------------------------
@KOJO_1201_HITOKUTI_SHOP

CALLFORM KOJO_霊夢_HITOKUTI_SHOP
@KOJO_1211_HITOKUTI_SHOP

CALLFORM KOJO_霊夢_HITOKUTI_SHOP
@KOJO_霊夢_HITOKUTI_SHOP
VARSET LOCAL
;ショップ画面で選択されているときに一言しゃべります
;普段の様子や体調、今の心情など
SELECTCASE RAND:10
CASE 9
	PRINTFORML 「ふぁ……、めんどくさいわね」
CASE 8
	FOR LOCAL,1,CHARANUM
		IF CALLNAME:LOCAL == "早苗"
			IF !CFLAG:LOCAL:0
				LOCAL:1 = 1
			ELSE
				LOCAL:1 = 2
			ENDIF
		ENDIF
	NEXT
	IF LOCAL:1 == 1
		PRINTFORML 「早苗、なんか美味しいもの知らない？」
	ELSEIF LOCAL:1 == 2
		PRINTFORML 「どこいったのよ、早苗……」
	ELSE
		PRINTFORML 「たまったもんじゃないわね」
	ENDIF
CASE 7
	FOR LOCAL,1,CHARANUM
		IF CALLNAME:LOCAL == "魔理沙"
			IF !CFLAG:LOCAL:0
				LOCAL:1 = 1
			ELSE
				LOCAL:1 = 2
			ENDIF
		ENDIF
	NEXT
	IF LOCAL:1 == 1
		PRINTFORML 「魔理沙、こっちの物は勝手に借りちゃダメよ」
	ELSEIF LOCAL:1 == 2
		PRINTFORML 「魔理沙……、さっさと帰ってきなさいよ……、バカ」
	ELSE
		PRINTFORML 「たまったもんじゃないわね」
	ENDIF
CASE 6
	PRINTFORML 「……スペカになるくらいの術じゃないと効かないなんてねぇ」
CASE 5
	PRINTFORML 「こっちの菓子は甘すぎるわね、ホント」
CASE 4
	PRINTFORML 「紫のバカの仕業かしらね」
CASE 3
	PRINTFORML 「異変おきてなきゃ良いけど」
CASE 2
	PRINTFORML 「早く帰りたいわね」
CASE 1
	FOR LOCAL,1,CHARANUM
		IF CALLNAME:LOCAL == "早苗"
			IF !CFLAG:LOCAL:0
				SETBIT LOCAL:1,0
			ELSE
				SETBIT LOCAL:1,1
			ENDIF
		ELSEIF CALLNAME:LOCAL == "魔理沙"
			IF !CFLAG:LOCAL:0
				SETBIT LOCAL:1,2
			ELSE
				SETBIT LOCAL:1,3
			ENDIF
		ELSE
			IF !CFLAG:LOCAL:0
				SETBIT LOCAL:1,4
			ELSE
				SETBIT LOCAL:1,5
			ENDIF
		ENDIF
	NEXT
	IF GETBIT(LOCAL:1,0) && GETBIT(LOCAL:1,2)
		PRINTFORML 「……まぁ、魔理沙と早苗が居るだけましね」
	ELSEIF GETBIT(LOCAL:1,0) && GETBIT(LOCAL:1,3)
		PRINTFORML 「早苗だけじゃ心許ないわよ、魔理沙探さないと」
	ELSEIF GETBIT(LOCAL:1,1) && GETBIT(LOCAL:1,2)
		PRINTFORML 「魔理沙だけだと辛いわね、早苗見つけないと」
	ELSEIF GETBIT(LOCAL:1,1) && GETBIT(LOCAL:1,3)
		PRINTFORML 「……早く、……早く二人を見つけないと」
	ELSEIF (GETBIT(LOCAL:1,0) || GETBIT(LOCAL:1,1)) && !(GETBIT(LOCAL:1,2) && GETBIT(LOCAL:1,3))
		IF GETBIT(LOCAL:1,0)
			PRINTFORML 「早苗、なんか美味しいもの知らない？」
		ELSE
			PRINTFORML 「どこいったのよ、早苗……」
		ENDIF
	ELSEIF !(GETBIT(LOCAL:1,0) && GETBIT(LOCAL:1,1)) && (GETBIT(LOCAL:1,2) || GETBIT(LOCAL:1,3))
		IF GETBIT(LOCAL:1,2)
			PRINTFORML 「魔理沙、こっちの物は勝手に借りちゃダメよ」
		ELSE
			PRINTFORML 「魔理沙……、さっさと帰ってきなさいよ……、バカ」
		ENDIF
	ELSEIF GETBIT(LOCAL:1,4)
		PRINTFORML 「ふぁ、あいつら元気ねぇ」
	ELSEIF GETBIT(LOCAL:1,5)
		PRINTFORML 「ほっとくわけには、いかないわよね」
	ENDIF
CASE 0
	PRINTFORML 「妖怪退治が専門の筈なんだけどね……」
ENDSELECT


