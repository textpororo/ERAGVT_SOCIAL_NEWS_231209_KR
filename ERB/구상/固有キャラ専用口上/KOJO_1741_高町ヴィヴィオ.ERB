	CASE 3
		SETCOLOR 0x00ff00
	CASE 2
		SETCOLOR 0x66cccc
	CASE 1
		SETCOLOR 0x0000ff
	CASE 0
		SETCOLOR 0x6600ff
ENDSELECT


