;CFLAG:277 (仮)豹変するタイミング記憶用(bit)
;CFLAG:278 (仮)豹変する性格記憶用(普段性格1～3桁、豹変時性格4～6桁)
;CFLAG:279 얀데레구상용
;CFLAG:280 얀데레구상용

;==================================================
;【独自関数】
;--------------------------------------------------
;豹変する場面の選択肢を表示する関数
;----------------------------------------------------------
;ARG	LOCAL
@GET_CFLAG277_CHOICE_KOJO_4(ARG)
SIF GETBIT(CFLAG:277,ARG)
	SETCOLOR 255,60,220
SELECTCASE ARG
	CASE K4평소
		PRINTFORML [{ARG}] 普段　　　　　
	CASE K4회적
		PRINTFORML [{ARG}] 会敵　　　　　
	CASE K4내습
		PRINTFORML [{ARG}] 襲来　　　　　
	CASE K4전투
		PRINTFORML [{ARG}] 戦闘　　　　　
	CASE K4전투_변신
		PRINTFORML [{ARG}] 戦闘(変身)　　
	CASE K4단련
		PRINTFORML [{ARG}] 鍛錬　　　　　
	CASE K4휴식
		PRINTFORML [{ARG}] 休憩　　　　　
	CASE K4특별활동
		PRINTFORML [{ARG}] 特別活動　　　
	CASE K4특별활동_변신
		PRINTFORML [{ARG}] 特別活動(変身)
	CASE K4정보수집
		PRINTFORML [{ARG}] 情報収集　　　
	CASE K4정보수집_변신
		PRINTFORML [{ARG}] 情報収集(変身)
	CASE K4자유행동
		PRINTFORML [{ARG}] 自由行動　　　
	CASE K4자유행동_변신
		PRINTFORML [{ARG}] 自由行動(変身)
	CASE K4유폐
		PRINTFORML [{ARG}] 幽閉　　　　　
	CASE K4유폐_변신
		PRINTFORML [{ARG}] 幽閉(変身)　　
	CASE K4세뇌
		PRINTFORML [{ARG}] 세뇌　　　　　
	CASE K4흑화
		PRINTFORML [{ARG}] 흑화　　　　
ENDSELECT
RESETCOLOR

;その場面での性格を取得する
;----------------------------------------------------------
;ARG	対象
;ARG:1	CFLAG:277のbit桁
;RESULT:0	性別の番号
;RESULT:1	性格の番号
@GET_SEIKAKU_KOJO_4(ARG,ARG:1)
ARG:1 = GETBIT(CFLAG:277,ARG:1)
RETURN FIGURE_SPLIT(CFLAG:ARG:278,3+ARG:1*3), FIGURE_SPLIT(CFLAG:ARG:278,2+ARG:1*3)*10 + FIGURE_SPLIT(CFLAG:ARG:278,1+ARG:1*3)

;汎用豹変구상ルート処理_普段
;----------------------------------------------------------
@KOJO_4_ROOT_SHOP(ARGS)
CALL GET_SEIKAKU_KOJO_4(TARGET,K4평소)
TRYCALLFORM KOJO_{RESULT:0}_COLOR_{RESULT:1}
TRYCALLFORM KOJO_{RESULT:0}_%ARGS%_{RESULT:1}

