SIF ISMALE()
	CFLAG:278 += 100100
$INPUT_LOOP
SEIKAKU_COUNT = 0
PRINTSINGLE ================================================================================================================================================================
PRINTFORML ※パラメータは「%TALENTNAME:SEIKAKU_CHECK_F(TARGET)%」のまま固定です。
PRINT ※未変身時の性別は「
IF CHARATALENT_F(TARGET,0,"남자") > 0
	PRINT 남자
ELSEIF CHARATALENT_F(TARGET,0,"후타나리") > 0
	PRINT 후타나리
ELSE
	PRINT オンナ
ENDIF
PRINT 」で、変身時の性別は「
IF CHARATALENT_F(TARGET,1,"남자") > 0
	PRINT 남자
ELSEIF CHARATALENT_F(TARGET,1,"후타나리") > 0
	PRINT 후타나리
ELSE
	PRINT オンナ
ENDIF
PRINTL 」に設定されています。
IF PAGE == 0
	PRINTFORML %CALLNAME%の通常時と豹変時の性格を選択してください。
	PRINTL 末尾に☆が付いている性格が구상有りです。
	PRINTL 　□　　通常時 　　　　　　　　　　豹変時
	FOR LOCAL, 10, 50
		SIF TALENTNAME:LOCAL == ""
			CONTINUE
		SIF FIGURE_SPLIT(CFLAG:278,3) == 0 && FIGURE_SPLIT(CFLAG:278,2)*10 + FIGURE_SPLIT(CFLAG:278,1) == LOCAL
			SETCOLOR 255,60,220
		PRINTFORM 　[{LOCAL}] %TALENTNAME:LOCAL, 15,LEFT%(女)
		RESETCOLOR
		SIF FIGURE_SPLIT(CFLAG:278,6) == 0 && FIGURE_SPLIT(CFLAG:278,5)*10 + FIGURE_SPLIT(CFLAG:278,4) == LOCAL
			SETCOLOR 255,60,220
		PRINTFORM 　[{LOCAL*1000}] %TALENTNAME:LOCAL, 15,LEFT%(女)
		TRYCCALLFORM KOJO_0_COLOR_{LOCAL}
			PRINTL ☆
		CATCH
			PRINTL 
		ENDCATCH
		RESETCOLOR
	NEXT
	FOR LOCAL, 10, 50
		SIF TALENTNAME:LOCAL == ""
			CONTINUE
		SIF FIGURE_SPLIT(CFLAG:278,3) == 1 && FIGURE_SPLIT(CFLAG:278,2)*10 + FIGURE_SPLIT(CFLAG:278,1) == LOCAL
			SETCOLOR 255,60,220
		PRINTFORM 　[{LOCAL+100}] %TALENTNAME:LOCAL, 15,LEFT%(男)
		RESETCOLOR
		SIF FIGURE_SPLIT(CFLAG:278,6) == 1 && FIGURE_SPLIT(CFLAG:278,5)*10 + FIGURE_SPLIT(CFLAG:278,4) == LOCAL
			SETCOLOR 255,60,220
		PRINTFORM 　[{(LOCAL+100)*1000}] %TALENTNAME:LOCAL, 15,LEFT%(男)
		TRYCCALLFORM KOJO_1_COLOR_{LOCAL}
			PRINTL ☆
		CATCH
			PRINTL 
		ENDCATCH
		RESETCOLOR
		SEIKAKU_COUNT ++
	NEXT
ELSE
	PRINTFORML %CALLNAME%の性格が豹変する場面を選択してください。
	FOR LOCAL, 0, 55
		CALL GET_CFLAG277_CHOICE_KOJO_4(LOCAL)
	NEXT
ENDIF
PRINTL 
PRINTFORM [-1]ページ変更({PAGE+1}/2)
PRINT 　　
PRINT [99]ランダム
PRINT 　　
PRINTL [100] 결정

INPUT
IF RESULT == -1
	INVERTBIT PAGE,0
	GOTO INPUT_LOOP
ELSEIF RESULT == 100
	PRINTW 決定しました。
ELSEIF PAGE == 0
	IF RESULT == 99
		CFLAG:278 = (RAND:SEIKAKU_COUNT+10)*1000+RAND:SEIKAKU_COUNT+10
		SIF ISMALE()
			CFLAG:278 += 100100
	ELSEIF (RESULT >= 10 && RESULT <= SEIKAKU_COUNT+10) || (RESULT >= 110 && RESULT <= SEIKAKU_COUNT+110)
	CFLAG:278 = FIGURE_SPLIT(CFLAG:278,6)*100000 + FIGURE_SPLIT(CFLAG:278,5)*10000 + FIGURE_SPLIT(CFLAG:278,4)*1000
	CFLAG:278 += RESULT
	ELSEIF ((RESULT >= 10*1000 && RESULT <= (SEIKAKU_COUNT+10)*1000)) || ((RESULT >= 110*1000 && RESULT <= (SEIKAKU_COUNT+110)*1000))
	CFLAG:278 = FIGURE_SPLIT(CFLAG:278,3)*100+FIGURE_SPLIT(CFLAG:278,2)*10+FIGURE_SPLIT(CFLAG:278,1)
	CFLAG:278 += RESULT
	ENDIF
	GOTO INPUT_LOOP
ELSE
	IF RESULT == 99
		CFLAG:277 = 0
		FOR LOCAL, 0, 20
			SIF RAND:2
				SETBIT CFLAG:277, LOCAL
		NEXT
		SIF RAND:2
			SETBIT CFLAG:277, 30
		SIF RAND:2
			SETBIT CFLAG:277, 40
	ELSEIF RESULT >= 0 && RESULT <= 49
		INVERTBIT CFLAG:277, RESULT
	ENDIF
	GOTO INPUT_LOOP
ENDIF

LOCAL = 0
SIF FIGURE_SPLIT(CFLAG:278,3) == 0 && FIGURE_SPLIT(CFLAG:278,2)*10 + FIGURE_SPLIT(CFLAG:278,1) == 21
	LOCAL = 1
SIF FIGURE_SPLIT(CFLAG:278,6) == 0 && FIGURE_SPLIT(CFLAG:278,5)*10 + FIGURE_SPLIT(CFLAG:278,4) == 21
