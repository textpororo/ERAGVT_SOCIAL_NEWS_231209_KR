	ENDIF
ELSE
	IF CFLAG:1 > 0
		CALL GET_SEIKAKU_KOJO_4(TARGET,K4전투_변신)
	ELSE
		CALL GET_SEIKAKU_KOJO_4(TARGET,K4전투)
	ENDIF
