;淫紋の位置決定
;C	下腹部
;V	腹部
;A	右臀部
;B	左乳房
;---------------------------------------------
@TATTOO_POSITION
#DIM CAL_VAR, 3
#DIM LCOUNT

VARSET CAL_VAR
;一か所の場合、最も感覚LVの高い部位になる
;感覚LVには、敏感には1.2倍、鈍感には0.8倍、淫〜系素質には1.5倍の補正をかける
FOR LCOUNT, 0, 감각수
	CAL_VAR:0 = TATTOO_LV_CAL(LCOUNT)
	IF CAL_VAR:0 > CAL_VAR:1
		CAL_VAR:1 = CAL_VAR:0
		CAL_VAR:2 = LCOUNT
	ENDIF
NEXT
SIF CAL_VAR:1 == TATTOO_LV_CAL(1)
	CAL_VAR:2 = 1

RETURN CAL_VAR:2



;淫紋用感覚LV補正
;ARG	感覚指定
;---------------------------------------------
@TATTOO_LV_CAL(ARG)
#FUNCTION
#DIM CAL_VAR

CAL_VAR = ABL:ARG * (TALENT:(ARG * 2 + 100) + 5) / 5 * (5 - TALENT:(ARG * 2 + 101)) / 5 * (TALENT:(ARG + 153) + 2) / 2

RETURNF CAL_VAR



;淫紋の保存処理
;ARG	進行度
;---------------------------------------------
@SAVE_TATTOO, ARG
#DIM LCOUNT

SIF TALENT:촉수의포로 == 0
	RETURN 0

;進行度は汚染度(どれだけ陥落に近付いているか示す内部値)と連動する
ARG = LIMIT(ARG, 0, 100)
;汚染度との連動を切っている場合、5~10%進行する
IF CONFIG_CHECK_MANIAC_F(10) == 0 && CFLAG:0 == 1
	ARG = CFLAG:32 / 10000000000000000
	ARG = LIMIT(ARG + 5 + RAND:6,0,100)
ENDIF
IF CFLAG:32 == 0
	CFLAG:32 = ARG * 10000000000000000
	;淫紋複雑化フラグチェック
	LOCAL = CONFIG_CHECK_MANIAC_F(8)
	;複数箇所フラグチェック
	;淫〜系素質がないと追加で淫紋がつかない
	IF CONFIG_CHECK_MANIAC_F(7) == 1 && (TALENT:음핵 + TALENT:음호 + TALENT:음고 + TALENT:음유) > 0
		FOR LCOUNT, 0, 감각수
			SIF TALENT:(LCOUNT + 153)
				CFLAG:32 += (LOCAL * (RAND:19 + 1) + 1) * POWER(100, LCOUNT)
		NEXT
	ELSE
		CALL TATTOO_POSITION
		CFLAG:32 += ((LOCAL)* (RAND:19 + 1) + 1) * POWER(100, RESULT)
	ENDIF
ELSE
	CFLAG:32 = CFLAG:32 % 10000000000000000
	CFLAG:32 += ARG * 10000000000000000
	;複数箇所フラグチェック
	;淫〜系素質がないと追加で淫紋がつかない
	IF CONFIG_CHECK_MANIAC_F(7) == 1 && (TALENT:음핵 + TALENT:음호 + TALENT:음고 + TALENT:음유) > 0
		CALL TATTOO_ACCESS, "POSITION_BIT"
		LOCAL = RESULT
		;新規に淫紋が出る部位がないかをチェック
		FOR LCOUNT, 0, 감각수
			IF GETBIT(LOCAL, LCOUNT) == 0 && TALENT:(LCOUNT + 153)
				;淫紋複雑化フラグチェック
				CFLAG:32 += ((CONFIG_CHECK_MANIAC_F(8)) * (RAND:19 + 1) + 1) * POWER(100, LCOUNT)
			ENDIF
		NEXT
	ENDIF
ENDIF

;追加淫紋の保存処理
;淫紋番号4
;0:額　1:喉下部　2:背中　3:右頬　4:左頬　5:右大腿部　6:左大腿部
;---------------------------------------------
@SAVE_TATTOO_ADDITIONAL, ARG
#DIM BIT_VAR
#DIM RAND_VAR
#DIM LCOUNT

VARSET BIT_VAR
;淫〜系素質を数え、全てなければ終了
FOR LCOUNT, 0, 감각수
	SIF TALENT:(LCOUNT + 153)
		BIT_VAR += 1
NEXT

SIF BIT_VAR < 감각수
	RETURN 0

;追加淫紋がいくつついてるかを調べ、5つ以上なら終了
CALL TATTOO_ACCESS, "TATTOO_TYPE", 7
BIT_VAR = RESULT
SIF BITNUM(BIT_VAR) >= 5
	RETURN 0

;刻まれる淫紋の重複チェック
DO
	RAND_VAR = RAND:7
	;既にある場合は不可
	SIF BIT_VAR & POWER(2, RAND_VAR)
		RAND_VAR = -1

	;既に左にある場合は不可
	SIF RAND_VAR > 2 && RAND_VAR % 2 && BIT_VAR & POWER(2, RAND_VAR + 1)
		RAND_VAR = -1

	;既に右にある場合は不可
	SIF RAND_VAR > 2 && RAND_VAR % 2 == 0 && BIT_VAR & POWER(2, RAND_VAR - 1)
		RAND_VAR = -1
LOOP RAND_VAR < 0

SETBIT BIT_VAR, RAND_VAR
CFLAG:32 = CFLAG:32 % 1000000000000 + CFLAG:32 / 10000000000000000 * 10000000000000000 + BIT_VAR * 100000000000000



;ARGに立っているビットの数を返す式中関数
;---------------------------------------------
@BITNUM, ARG
#FUNCTION
#DIM CAL_VAR
#DIM RES_VAR

CAL_VAR = ARG
VARSET RES_VAR
DO
	SIF CAL_VAR % 2
		RES_VAR += 1
	CAL_VAR /= 2
LOOP CAL_VAR > 0

RETURNF RES_VAR



;淫紋の各パラメータ取得
;ARGS	セレクタ
;---------------------------------------------
@TATTOO_ACCESS, ARGS, ARG
#DIM LCOUNT
#DIM CAL_VAR
#DIM PROG_VAR
#DIM DES_VAR, 8

FOR LCOUNT, 0, 8
	DES_VAR:LCOUNT = CFLAG:32 / POWER(100, LCOUNT) % 100
NEXT

VARSET CAL_VAR
SELECTCASE ARGS
	;淫紋進行度
	CASE "PROGRESS_VAR"
		RETURN CFLAG:32 / 10000000000000000

	;淫紋の感覚LVによるランク
	CASE "TATTOO_RANK"
		IF ARG < 감각수
			RETURN TATTOO_LV_CAL(ARG) * (CFLAG:32 * 5 / 100000000000000000 + 50) / 800
		ELSE
			VARSET CAL_VAR
			FOR LCOUNT, 0, 감각수
				CAL_VAR += TATTOO_LV_CAL(LCOUNT)
			NEXT
			RETURN CAL_VAR * (CFLAG:32 * 5 / 100000000000000000 + 50) / 2000
		ENDIF

	;淫紋の追加文字列の種類
	CASE "TATTOO_TYPE"
		RETURN DES_VAR:ARG

	;淫紋の数
	CASE "POSITION_NUM"
		RETURN (DES_VAR:0 > 0) + (DES_VAR:1 > 0) + (DES_VAR:2 > 0) + (DES_VAR:3 > 0)

	;淫紋の位置　ビット
	CASE "POSITION_BIT"
		SIF DES_VAR:0
			CAL_VAR += Ｃ
		SIF DES_VAR:1
			CAL_VAR += Ｖ
		SIF DES_VAR:2
			CAL_VAR += Ａ
		SIF DES_VAR:3
			CAL_VAR += Ｂ

		RETURN CAL_VAR

	;淫紋の位置　文字列
	CASE "POSITION_STR"
		IF (DES_VAR:0 > 0) + (DES_VAR:1 > 0) + (DES_VAR:2 > 0) + (DES_VAR:3 > 0) > 1
			IF DES_VAR:2 == 0 && DES_VAR:3 == 0
				LOCALS = 腹部
			ELSEIF DES_VAR:3 == 0
				LOCALS = 下半身
			ELSE
				LOCALS = 体中
			ENDIF
			RESULTS = %LOCALS%
			RETURN 0
		ENDIF

		SIF DES_VAR:0
			LOCALS = 下腹部
		SIF DES_VAR:1
			LOCALS = 腹部
		SIF DES_VAR:2
			LOCALS = 右臀部
		SIF DES_VAR:3
			LOCALS = 左乳房
		RESULTS = %LOCALS%
		RETURN 0
ENDSELECT



