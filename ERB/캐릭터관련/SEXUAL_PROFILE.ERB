;*******************************************************************************************************
;裏プロフィール文字列の作成
;感度
;5		[Adj:1][部位]を[動詞]のが好き 淫語ON
;6		[Adj:1][部位]を[動詞]のが好き
;7		[Adj:1][部位]を[Adv:0][動詞]のが好き
;8		[Adj:1][Adj:0][部位]を[Adv:0][動詞]のが好き
;9		[Adj:1][Adj:0][部位]を[Adv:1][動詞]のが好き
;10		[Adj:2][Adj:1][Adj:0][部位]を[Adv:1][動詞]のが好き
;11		[Adj:3] [Adj:2] [Adj:1] [Adj:0] [部位] を [Adv:1]  [動詞] のが好き
;*******************************************************************************************************
@MAKESEXUALPROFILE, 対象, Lv
#FUNCTIONS

#DIMS 対象
#DIM Lv

#DIMS Adj, 4
#DIMS 部位
#DIMS Adv, 2
#DIMS 動詞

VARSET Adj, ""
VARSET Adv, ""

IF 対象 == "B" || 対象 == "C"|| 対象 == "P"
;BかCの形容詞をつくる
	SIF Lv < 11
		Adj:1 = %MAKE_ADJ_BC(Lv)%
	SIF Lv >= 8
		Adj:0 = %MAKE_ONOMATOPE_BC()%
	IF Lv >= 10
		Adj:2 = %MAKE_ADJ_BC(Lv)%
		WHILE Adj:2 == Adj:1
			Adj:2 = %MAKE_ADJ_BC(Lv)%
		WEND
	ENDIF
ELSEIF 対象 == "V" || 対象 == "A"
;VかAの形容詞をつくる
	SIF Lv < 11
		Adj:1 = %MAKE_ADJ_VA(Lv)%
	SIF Lv >= 8
		Adj:0 = %MAKE_ONOMATOPE_VA()%
	IF Lv >= 10
		Adj:2 = %MAKE_ADJ_VA(Lv)%
		WHILE Adj:2 == Adj:1
			Adj:2 = %MAKE_ADJ_VA(Lv)%
		WEND
	ENDIF
ENDIF

SIF Lv >= 11
	Adj:3 = %MAKE_BAD_REPUTATION()%

IF 対象 == "B"
	;思いつかなかった
	部位 = 乳首
ELSEIF 対象 == "C"
	IF Lv >= 5
		部位 = %MAKE_C_NAME("淫語")%
	ELSE
		部位 = %MAKE_C_NAME()%
	ENDIF
ELSEIF 対象 == "P"
	IF Lv >= 5
		部位 = %MAKE_P_NAME("淫語")%
	ELSE
		部位 = %MAKE_P_NAME()%
	ENDIF
ELSEIF 対象 == "V"
	IF Lv >= 5
		部位 = %MAKE_V_NAME("淫語")%
	ELSE
		部位 = %MAKE_V_NAME()%
	ENDIF
ELSEIF 対象 == "A"
	IF Lv >= 5
		部位 = %MAKE_A_NAME("淫語")%
	ELSE
		部位 = %MAKE_A_NAME()%
	ENDIF
ELSE
	部位 = "エラー"
ENDIF

SIF LV >= 7 && LV < 9
	Adv:0 = %MAKE_ADV_1()%
SIF LV >= 9
	Adv:1 = %MAKE_ADV_2()%
	
IF 対象 == "B" || 対象 == "C" || 対象 == "P"
	動詞 = %MAKE_VERB_BC()%
ELSEIF 対象 == "V"
	IF Lv >= 5
		動詞 = %MAKE_VERB_V("淫語")%
	ELSE
		動詞 = %MAKE_VERB_V()%
	ENDIF
ELSEIF 対象 == "A"
	IF Lv >= 5
		動詞 = %MAKE_VERB_A("淫語")%
	ELSE
		動詞 = %MAKE_VERB_A()%
	ENDIF
ELSE
	動詞 = "エラー"
ENDIF
RETURNF Adj:3+Adj:2+Adj:1+Adj:0+部位+"を"+Adv:1+Adv:0+動詞+"のが好き"

