;*******************************************************************************************************
;뒷 프로필 문자열 생성
;감도
;5		[Adj:1][부위]를 [동사] 게 좋아 (음어ON)
;6		[Adj:1][부위]를 [동사] 게 좋아
;7		[Adj:1][부위]를 [Adv:0][동사] 게 좋아
;8		[Adj:1][Adj:0][부위]를 [Adv:0][동사] 게 좋아
;9		[Adj:1][Adj:0][부위]를 [Adv:1][동사] 게 좋아
;10		[Adj:2][Adj:1][Adj:0][부위]를 [Adv:1][동사] 게 좋아
;11		[Adj:3] [Adj:2] [Adj:1] [Adj:0] [부위] 를 [Adv:1]  [동사] 게 좋아
;*******************************************************************************************************
@MAKESEXUALPROFILE, 대상, Lv
#FUNCTIONS

#DIMS 대상
#DIM Lv

#DIMS Adj, 4
#DIMS 부위
#DIMS Adv, 2
#DIMS 동사

VARSET Adj, ""
VARSET Adv, ""

IF 대상 == "B" || 대상 == "C"|| 대상 == "P"
;B 또는 C(P)의 형용사를 만든다
	SIF Lv < 11
		Adj:1 = %MAKE_ADJ_BC(Lv)%
	SIF Lv >= 8
		Adj:0 = %MAKE_ONOMATOPE_BC()%
	IF Lv >= 10
		Adj:2 = %MAKE_ADJ_BC(Lv)%
		WHILE Adj:2 == Adj:1
			Adj:2 = %MAKE_ADJ_BC(Lv)%
		WEND
	ENDIF
ELSEIF 대상 == "V" || 대상 == "A"
;V 또는 A의 형용사를 만든다
	SIF Lv < 11
		Adj:1 = %MAKE_ADJ_VA(Lv)%
	SIF Lv >= 8
		Adj:0 = %MAKE_ONOMATOPE_VA()%
	IF Lv >= 10
		Adj:2 = %MAKE_ADJ_VA(Lv)%
		WHILE Adj:2 == Adj:1
			Adj:2 = %MAKE_ADJ_VA(Lv)%
		WEND
	ENDIF
ENDIF

SIF Lv >= 11
	Adj:3 = %MAKE_BAD_REPUTATION()%

IF 대상 == "B"
	;생각나지 않았다
	부위 = 유두
ELSEIF 대상 == "C"
	IF Lv >= 5
		부위 = %MAKE_C_NAME("음어")%
	ELSE
		부위 = %MAKE_C_NAME()%
	ENDIF
ELSEIF 대상 == "P"
	IF Lv >= 5
		부위 = %MAKE_P_NAME("음어")%
	ELSE
		부위 = %MAKE_P_NAME()%
	ENDIF
ELSEIF 대상 == "V"
	IF Lv >= 5
		부위 = %MAKE_V_NAME("음어")%
	ELSE
		부위 = %MAKE_V_NAME()%
	ENDIF
ELSEIF 대상 == "A"
	IF Lv >= 5
		부위 = %MAKE_A_NAME("음어")%
	ELSE
		부위 = %MAKE_A_NAME()%
	ENDIF
ELSE
	부위 = "ERROR"
ENDIF

SIF LV >= 7 && LV < 9
	Adv:0 = %MAKE_ADV_1()%
SIF LV >= 9
	Adv:1 = %MAKE_ADV_2()%
	
IF 대상 == "B" || 대상 == "C" || 대상 == "P"
	동사 = %MAKE_VERB_BC()%
ELSEIF 대상 == "V"
	IF Lv >= 5
		동사 = %MAKE_VERB_V("음어")%
	ELSE
		동사 = %MAKE_VERB_V()%
	ENDIF
ELSEIF 대상 == "A"
	IF Lv >= 5
		동사 = %MAKE_VERB_A("음어")%
	ELSE
		동사 = %MAKE_VERB_A()%
	ENDIF
ELSE
	동사 = "ERROR"
ENDIF

;RETURNF Adj:3+Adj:2+Adj:1+Adj:0+부위+"을(를) "+Adv:1+Adv:0+동사+" 게 좋아"

RETURNF Adj:3+Adj:2+Adj:1+Adj:0+조사처리(부위,"를")+" "+Adv:1+Adv:0+동사+" 게 좋아"

