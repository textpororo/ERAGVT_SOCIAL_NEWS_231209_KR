;鍛錬変化時の表示
;-------------------------
@TRAINING_HOSEI_CHILD,ARG
LOCAL:4 = MARK:혈연보정
SIF LOCAL:4 <= 0
	RETURN ARG
SIF LOCAL:4 >= 12
	LOCAL:4 = 12
LOCAL:2 = 3 + LOCAL:4
LOCAL:3 = 2 + LOCAL:4
LOCAL:1 = ARG
FOR LOCAL,0,LOCAL:4
	LOCAL:1 = LOCAL:1 * LOCAL:2
NEXT
FOR LOCAL,0,LOCAL:4
	LOCAL:1 = LOCAL:1 / LOCAL:3
NEXT
RETURN LOCAL:1

