@GROW_HANTEI
#DIM CCOUNT
LOCAL = TARGET
FOR CCOUNT, 0, CHARANUM
	TARGET = CCOUNT
	;육아 중이라면 육아일 증가
	IF CFLAG:0 == 11 || CFLAG:22 > 0
		CFLAG:224 += 1
		;확률로 육아 종료　최단 2일, 최장 5일
		SIF (CFLAG:224 >= 4 && RAND:2 == 0) || CFLAG:224 >= 10
			CALL ADD_CHILD,TARGET
	ENDIF
	IF TALENT:성징정체 > 0
	ELSEIF CFLAG:225 > 14
		CALL CHILD_GROW_2
		CFLAG:225 = 0
	ELSEIF CFLAG:225 == 7
		CALL CHILD_GROW_1
		CFLAG:225 += 1
	ELSEIF CFLAG:225 > 0
		CFLAG:225 += 1
	ENDIF
NEXT
TARGET = LOCAL

;-------------------------
;단련 변화 시의 표시
;-------------------------
@TRAINING_HOSEI_CHILD,ARG
LOCAL:4 = MARK:혈연보정
SIF LOCAL:4 <= 0
	RETURN ARG
SIF LOCAL:4 >= 12
	LOCAL:4 = 12
LOCAL:2 = 3 + LOCAL:4
LOCAL:3 = 2 + LOCAL:4
LOCAL:1 = ARG
FOR LOCAL,0,LOCAL:4
	LOCAL:1 = LOCAL:1 * LOCAL:2
NEXT
FOR LOCAL,0,LOCAL:4
	LOCAL:1 = LOCAL:1 / LOCAL:3
NEXT
RETURN LOCAL:1

