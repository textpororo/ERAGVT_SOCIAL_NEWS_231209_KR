	IF CFLAG:0 == 11 || CFLAG:22 > 0
		CFLAG:224 += 1
		;確率で育児終了　最短2日最長5日
		SIF (CFLAG:224 >= 4 && RAND:2 == 0) || CFLAG:224 >= 10
			CALL ADD_CHILD,TARGET
	ENDIF
	IF TALENT:성징정체 > 0
	ELSEIF CFLAG:225 > 14
		CALL CHILD_GROW_2
		CFLAG:225 = 0
	ELSEIF CFLAG:225 == 7
		CALL CHILD_GROW_1
		CFLAG:225 += 1
	ELSEIF CFLAG:225 > 0
		CFLAG:225 += 1
	ENDIF
NEXT
TARGET = LOCAL

