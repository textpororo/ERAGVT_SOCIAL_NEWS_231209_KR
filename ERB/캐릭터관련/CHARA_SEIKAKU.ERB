;--------------------------------------------------
;キャラの性格に関する処理
;--------------------------------------------------
;性格のチェック
@SEIKAKU_CHECK, ARGS, ARG
LOCAL = 0
LOCAL:1 = 0
LOCAL:2 = 0
LOCAL:3 = 0
FOR LOCAL:2, 10, 50
	SIF TALENTNAME:(LOCAL:2) != ""
		LOCAL:1 += 1
NEXT
FOR LOCAL:3,0,LOCAL:1
	LOCAL = LOCAL:3 + 10
	IF TALENT:ARG:LOCAL == 1
		SELECTCASE ARGS
			CASE "STRING"
				RESULTS = %TALENTNAME:LOCAL%
				RETURN LOCAL
			CASE "PRINT"
				PRINTFORM %TALENTNAME:LOCAL%
				RETURN LOCAL
			CASE "GET_TALENT_VALUE"
				RETURN LOCAL
		ENDSELECT
	ENDIF
NEXT
SELECTCASE ARGS
	CASE "STRING"
		RESULTS = ランダム
	CASE "PRINT"
		PRINTFORM ランダム
		RETURN 0
	CASE "GET_TALENT_VALUE"
		RETURN 0
ENDSELECT
;--------------------------------------------------
;性格のチェック、式中関数版
;ARG = キャラ番号
@SEIKAKU_CHECK_F(ARG)
#FUNCTION
LOCAL = 0
LOCAL:1 = 0
LOCAL:2 = 0
LOCAL:3 = 0
FOR LOCAL:2, 10, 50
	SIF TALENTNAME:(LOCAL:2) != ""
		LOCAL:1 += 1
NEXT
FOR LOCAL:3,0,LOCAL:1
	LOCAL = LOCAL:3 + 10
	SIF TALENT:ARG:LOCAL == 1
			RETURNF LOCAL
NEXT
RETURNF 0

