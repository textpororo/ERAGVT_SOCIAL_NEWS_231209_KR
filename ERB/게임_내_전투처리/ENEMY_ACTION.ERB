IF FLAG:902 && TFLAG:10 == 2
	LOCAL:1 = RAND:100
	IF LOCAL:1 < 15
		TFLAG:10 = 1
	ELSEIF LOCAL:1 < 30
		TFLAG:10 = 3
	ELSEIF LOCAL:1 < 35
		TFLAG:10 = 4
	ELSEIF LOCAL:1 < 40
		TFLAG:10 = 6
	ENDIF
;絡みつく選択率アップの効果
ELSEIF FLAG:907 && RAND:100 < 20
	TFLAG:10 = 2
ENDIF

