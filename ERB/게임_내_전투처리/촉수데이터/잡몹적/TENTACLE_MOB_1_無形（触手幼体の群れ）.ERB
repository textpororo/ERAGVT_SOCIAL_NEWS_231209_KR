;==================================================
;【専用ルーチン（性行為）】
;--------------------------------------------------
@TENTACLE_MOB_1_SEX_ROUTINE
LOCAL = RAND:100
;種付け
IF (LOCAL < 15 || (TFLAG:20 == 3 && LOCAL < 90) || (TCVARn:12 & 배란) > 0) && CHECK_HOLYVIRGIN_F()==0
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		IF ISMALE()
			RETURN 1
		ELSE
			RETURN 3
		ENDIF
	ENDIF
;母乳吸い
ELSEIF LOCAL < 40 && TALENT:모유체질
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		IF ISHOLE() == 0
			RETURN 1
		ELSE
			RETURN 7
		ENDIF
	ENDIF
;全身愛撫
ELSEIF LOCAL < 70
	RETURN 0
;Ｃ攻め強
ELSEIF LOCAL < 80
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		RETURN 1
	ENDIF
;フェラ
ELSE
	IF ISHOLE() == 0
		RETURN 1
	ELSE
		RETURN 11
	ENDIF
ENDIF

