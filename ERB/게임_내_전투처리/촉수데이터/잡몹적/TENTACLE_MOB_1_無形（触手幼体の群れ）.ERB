;==================================================
;【전용 루틴（전투）】
;--------------------------------------------------
@TENTACLE_MOB_1_ATTACK_ROUTINE
LOCAL = RAND:100
;공격 범위가 넓다
SIF RAND:4 == 0
	TFLAG:11 |= 1
SIF RAND:4 == 0
	TFLAG:11 |= 2
SIF RAND:4 == 0
	TFLAG:11 |= 4

IF LOCAL < 45
	RETURN 2
ELSE
	RETURN 0
ENDIF


;==================================================
;【전용 루틴（성행위）】
;--------------------------------------------------
@TENTACLE_MOB_1_SEX_ROUTINE
LOCAL = RAND:100
;씨받이 교배
IF (LOCAL < 15 || (TFLAG:20 == 3 && LOCAL < 90) || (TCVARn:12 & 배란) > 0) && CHECK_HOLYVIRGIN_F()==0
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		IF ISMALE()
			RETURN 1
		ELSE
			RETURN 3
		ENDIF
	ENDIF
;모유 흡입
ELSEIF LOCAL < 40 && TALENT:모유체질
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		IF ISHOLE() == 0
			RETURN 1
		ELSE
			RETURN 7
		ENDIF
	ENDIF
;전신 애무
ELSEIF LOCAL < 70
	RETURN 0
;Ｃ공격（강）
ELSEIF LOCAL < 80
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		RETURN 1
	ENDIF
;펠라
ELSE
	IF ISHOLE() == 0
		RETURN 1
	ELSE
		RETURN 11
	ENDIF
ENDIF

