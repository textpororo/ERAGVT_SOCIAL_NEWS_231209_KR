;==================================================
;【専用ルーチン（性行為）】
;--------------------------------------------------
@TENTACLE_MOB_3_SEX_ROUTINE
LOCAL = RAND:100
;母乳吸い/乳首食み
IF LOCAL < 30  || (TFLAG:20 == 7 && LOCAL < 80)
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		RETURN 7
	ENDIF
;這い回り
ELSEIF LOCAL < 80
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		RETURN 6
	ENDIF
;フェラ
ELSE
	RETURN 11
ENDIF

