;==================================================
;【専用ルーチン（性行為）】
;--------------------------------------------------
;専用ルーチン（性行為）
@TENTACLE_MOB_102_SEX_ROUTINE

;사정値が3/4（FLAG:15 > FLAG:14*3/4 ）かつ체력が1割以下（FLAG:13 < FLAG:12 /10 ）
;A촉수専用SPの浣腸拡張
IF (FLAG:15 > FLAG:14 *3/4) && (FLAG:13 < FLAG:12 /10)
	;衣装の耐久度が高ければ衣装破壊を優先する
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF / 4
		RETURN 14
	ELSE
		;남자かつ男女平等オフならC攻め強に振り替える
		IF ISHOLE() == 0
			RETURN 1
		;소프트 바디슈트の場合は貫通できないのでA攻め強に振り返る
		ELSEIF CFLAG:41 == 299 && CFLAG:1 > 0
			RETURN 5
		ELSE
			;専用コマンド「浣腸拡張」
			RETURN 2000
		ENDIF
	ENDIF
ELSE


	;ランダム係数호출（A攻め強、A攻め弱、CA攻め弱）
	LOCAL = RAND:100

	;애널に潜り込む
	IF LOCAL < 20 + (ISMALE()) * 20 || (TFLAG:20 == 5 && LOCAL < 80)
		IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
			RETURN 14
		ELSE
			IF ISHOLE() == 0
				RETURN 1
			ELSE
				RETURN 5
			ENDIF
		ENDIF

	;CA攻め弱
	ELSEIF LOCAL < 50
		RETURN 0

	;애널攻め弱
	ELSE
		IF ISHOLE() == 0
			RETURN 1
		ELSE
			RETURN 4, FLAG:11, 1
		ENDIF
	ENDIF
ENDIF

RETURN 1

