	RETURN 1
ENDIF


;==================================================
;【전용 루틴（성행위）】
;--------------------------------------------------
@TENTACLE_MOB_601_SEX_ROUTINE
LOCAL = RAND:100

;Ｖ挿入
IF LOCAL < 50 || (TFLAG:20 == 2 && LOCAL < 90) && ISFEMALE() && CHECK_HOLYVIRGIN_F()==0
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		RETURN 3
	ENDIF

;素股
ELSEIF LOCAL < 90 && ISFEMALE()
	RETURN 2

;キック
ELSE
	RETURN 8
ENDIF
RETURN 1

