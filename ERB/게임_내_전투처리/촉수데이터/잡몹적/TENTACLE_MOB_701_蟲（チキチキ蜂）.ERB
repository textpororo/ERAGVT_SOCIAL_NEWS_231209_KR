;==================================================
;【전용 루틴（성행위）】
;--------------------------------------------------
@TENTACLE_MOB_701_SEX_ROUTINE, ARG:0, ARG:1

;기절かつ服の방어力が低い場合で、최면간で繁殖
IF TCVARn:12 & 기절 && CLOTH_OUTER_PER < 90 && CLOTH_INNER_PER < CLOTH_INNER_DEF
	RETURN 3
;服の방어力が低い場合で、毒針で기절させる
ELSEIF CLOTH_OUTER_PER < 90 && CLOTH_INNER_PER < CLOTH_INNER_DEF
	RETURN 9
;服のダメージが少ない間は普通の性行動
ELSE
	LOCAL = RAND:100

	;20% 이하의 공격（겉옷&속옷의 방어 무시, 양성평등）
	;Ｃ공격（약）
	IF LOCAL < 20
		RETURN 0

	;40%-20%=20%의 확률
	;Ｖ공격（약）
	ELSEIF LOCAL < 40
		IF ISMALE() ;남자は別行動に割り振り
			SELECTCASE RAND:4
			CASE 0
				RETURN 0	;C
			CASE 1
				RETURN 4	;A
			CASE 2
				RETURN 6	;B
			CASE 3
				RETURN 10	;羞恥
			ENDSELECT
		ELSE
			RETURN 2
		ENDIF

	;60%-40%=20%의 확률
	;Ａ공격（약）
	ELSEIF LOCAL < 60
		RETURN 4

	;80%-60%=20%의 확률
	;Ｂ공격（약）
	ELSEIF LOCAL < 80
		RETURN 6

	;100%-80%=20%의 확률（양성평등）
	;수치 플레이
	ELSE
		RETURN 10
	ENDIF
ENDIF
RETURN 1


