@TENTACLE_MOB_901_ATTACK_ROUTINE
IF FLAG:13 < FLAG:12/5 && RAND:10 == 0
;체력が1/5より小さくなった かつ 10%
	RETURN 4	;距離を取る
ELSEIF TCVARn:TARGET:10 == 0 || TALENT:TARGET:변신능력 == 0
