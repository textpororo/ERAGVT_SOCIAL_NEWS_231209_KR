	IF RAND:3 == 0
		RETURN 2	;絡みつく
	ELSEIF RAND:6 == 0
		RETURN 6	;邪悪な波動
	ELSE
		RETURN 1	;通常공격
	ENDIF
ELSE
	IF RAND:3 == 0
		RETURN 2	;絡みつく
	ELSEIF (CHARATALENT_F(TARGET,1,"남자")!=1 && CHARATALENT_F(TARGET,1,"후타나리")!=1) && ((CHARATALENT_F(TARGET,2,"남자")==1 || CHARATALENT_F(TARGET,2,"후타나리")==1))
	;女→男・双の場合、「絡みつく」の他には「通常공격」しかしない
		RETURN 1	;通常공격
	ELSEIF RAND:4 == 0
		RETURN 6	;邪悪な波動
	ELSE
		RETURN 1	;通常공격
	ENDIF
ENDIF

;==================================================
;【専用ルーチン（性行為）】
;--------------------------------------------------
@TENTACLE_MOB_901_SEX_ROUTINE
IF TCVARn:TARGET:10 == 0
	;未変身ならとりあえず「エンジェルキッス」で변신한다か試す
	;変身不能な場合はドレインキッスに差し替え
	IF CHARATALENT_F(TARGET,0,"남자") == 0 && CHARATALENT_F(TARGET,1,"남자") > 0 && TALENT:임신 > 0
		RETURN 2001 ;ドレインキッス
	ELSE
		RETURN 2000	;エンジェルキッス
	ENDIF
ELSEIF CLOTH_INNER_PER < CLOTH_INNER_DEF
	IF ISPENIS(TARGET) == 1
	;속옷방어無し かつ ペニス有り
		RETURN 2004	;搾精
	ELSEIF (CHARATALENT_F(TARGET,0,"남자")!=1 && CHARATALENT_F(TARGET,0,"후타나리")!=1 && CHARATALENT_F(TARGET,1,"남자")!=1 && CHARATALENT_F(TARGET,1,"후타나리")!=1) && (TFLAG:20 == 1 || TFLAG:0 == 13 || TFLAG:0 == 14)
	;속옷방어無し かつ女
		RETURN 2002	;기생準備
	ENDIF
ELSEIF (CHARATALENT_F(TARGET,0,"남자")!=1 && CHARATALENT_F(TARGET,0,"후타나리")!=1) && (CHARATALENT_F(TARGET,1,"남자")==1 || CHARATALENT_F(TARGET,1,"후타나리")==1) && (CFLAG:TARGET:1==0 || BASE:TARGET:기력<MAXBASE:TARGET:기력/10)
	;女→男・双 かつ (変身前または기력が1/10以下)
	;変身不能な場合はドレインキッスに差し替え
	IF CHARATALENT_F(TARGET,0,"남자") == 0 && CHARATALENT_F(TARGET,1,"남자") > 0 && TALENT:임신 > 0
		RETURN 2001 ;ドレインキッス
	ELSE
		RETURN 2000	;エンジェルキッス
	ENDIF
ELSEIF ((CHARATALENT_F(TARGET,0,"남자")==1 || CHARATALENT_F(TARGET,0,"후타나리")==1) || (CHARATALENT_F(TARGET,1,"남자")!=1 && CHARATALENT_F(TARGET,1,"후타나리")!=1)) && RAND:4 == 0
	RETURN 2001	;ドレインキッス
ELSEIF RAND:6 == 0 && BASE:TARGET:체력<MAXBASE:TARGET:체력/10
	RETURN 8	;スパンキング(跳ね回る)
ELSE
	SELECTCASE RAND:5
	CASE 0
		RETURN 0	;Ｃ弱
	CASE 1
		RETURN 2	;Ｖ弱
	CASE 2
		RETURN 4	;Ａ弱
	CASE 3
		RETURN 6	;Ｂ弱
	CASE 4
		RETURN 1	;Ｃ強
	ENDSELECT
ENDIF


