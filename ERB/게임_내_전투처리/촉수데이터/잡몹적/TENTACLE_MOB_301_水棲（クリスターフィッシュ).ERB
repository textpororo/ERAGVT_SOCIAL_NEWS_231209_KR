;==================================================
;【전용 루틴（전투）】
;--------------------------------------------------
@TENTACLE_MOB_301_ATTACK_ROUTINE
LOCAL = RAND:100
;공격 범위가 넓다
SIF RAND:4 == 0
	TFLAG:11 |= 1
SIF RAND:4 == 0
	TFLAG:11 |= 2
SIF RAND:4 == 0
	TFLAG:11 |= 4

IF LOCAL < 55
	RETURN 2
ELSE
	RETURN 0
ENDIF


;==================================================
;【전용 루틴（성행위）】
;--------------------------------------------------
@TENTACLE_MOB_301_SEX_ROUTINE
LOCAL = RAND:100
;咥え込み/抱え込み
IF LOCAL < 40 || (TFLAG:20 == 1 && LOCAL < 80)
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		RETURN 1
	ENDIF

;撫で回し
ELSEIF LOCAL < 80
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		RETURN 0
	ENDIF

;연인繋ぎ
ELSE
	RETURN 10

ENDIF



