;==================================================
;【전용 루틴（전투）】
;--------------------------------------------------
@TENTACLE_MOB_2_ATTACK_ROUTINE, ARG:0, ARG:1

LOCAL = RAND:100
IF LOCAL < 45
	RETURN 2
ELSE
	RETURN 0
ENDIF



;==================================================
;【전용 루틴（성행위）】
;--------------------------------------------------
@TENTACLE_MOB_2_SEX_ROUTINE, ARG:0, ARG:1

;V없음
IF ISMALE()
	;필터 유무
	IF ISHOLE() == 0
		SELECTCASE RAND:3
		CASE 0
			RETURN 0	;C
		CASE 1
			RETURN 6	;B
		CASE 2
			RETURN 11	;펠라
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
		CASE 0
			RETURN 0	;C
		CASE 1
			RETURN 6	;B
		CASE 2
			RETURN 4	;A
		CASE 3
			RETURN 11	;펠라
		ENDSELECT
	ENDIF
	
;여성 OR 후타나리(V있음)
ELSE
	LOCAL = RAND:100
	;20% 이하의 공격（겉옷&속옷의 방어 무시, 양성평등）
	;Ｃ공격（약）
	IF LOCAL < 20
		RETURN 0

	;40%-20%=20%의 확률
	;Ｖ공격（약）
	ELSEIF LOCAL < 40
		RETURN 2

	;60%-40%=20%의 확률
	;Ａ공격（약）
	ELSEIF LOCAL < 60
		RETURN 4

	;80%-60%=20%의 확률
	;Ｂ공격（약）
	ELSEIF LOCAL < 80
		RETURN 6

	;100%-80%=20%의 확률（양성평등）
	;펠라
	ELSE
		RETURN 11
	ENDIF
ENDIF




;==================================================
;【성 커맨드 레퍼런스】
;--------------------------------------------------
;ARG=1　스마타 애태우기 성공 시의 행동 지정
;ARG=2　스마타 애태우기 실패 시의 행동 지정
;ARG=3　펠라치오 유발 시의 행동 지정
@TENTACLE_MOB_2_REACTION_REF, ARG = 0
;삽입계 커맨드가 루틴에 없으므로 -1을 반환한다
