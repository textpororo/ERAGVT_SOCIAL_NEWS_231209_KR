ELSE
	RETURN 0
ENDIF



;==================================================
;【専用ルーチン（性行為）】
;--------------------------------------------------
@TENTACLE_MOB_2_SEX_ROUTINE, ARG:0, ARG:1

;Vなし
IF ISMALE()
	;필터の有無
	IF ISHOLE() == 0
		SELECTCASE RAND:3
		CASE 0
			RETURN 0	;C
		CASE 1
			RETURN 6	;B
		CASE 2
			RETURN 11	;フェラ
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
		CASE 0
			RETURN 0	;C
		CASE 1
			RETURN 6	;B
		CASE 2
			RETURN 4	;A
		CASE 3
			RETURN 11	;フェラ
		ENDSELECT
	ENDIF
	
;女性 OR 후타나리(Vあり)
ELSE
	LOCAL = RAND:100
	;20%以下の공격（アウター&インナーの방어無視、男女平等）
	;C攻め弱
	IF LOCAL < 20
		RETURN 0

	;40%-20%=20%の確率
	;V攻め弱
	ELSEIF LOCAL < 40
		RETURN 2

	;60%-40%=20%の確率
	;A攻め弱
	ELSEIF LOCAL < 60
		RETURN 4

	;80%-60%=20%の確率
	;B攻め弱
	ELSEIF LOCAL < 80
		RETURN 6

	;100%-80%=20%の（男女平等）
	;フェラ
	ELSE
		RETURN 11
	ENDIF
ENDIF




;==================================================
;【性コマンドレファレンス】
;--------------------------------------------------
;ARG=1　스마타애태우기成功時の行動指定
;ARG=2　스마타애태우기失敗時の行動指定
;ARG=3　フェラチオ誘発時の行動指定
@TENTACLE_MOB_2_REACTION_REF, ARG = 0
;挿入系コマンドがルーチンに無いので-1を返す
IF ARG == 1
	RETURN -1
ELSEIF ARG == 2
	RETURN -1
ELSEIF ARG == 3
	RETURN 11
ENDIF
RETURN -1

