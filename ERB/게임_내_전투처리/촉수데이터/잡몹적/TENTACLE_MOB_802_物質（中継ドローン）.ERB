	;질내사정 당하는 모습을 보였다
	SIF (STAIN:3 & 4)
		SETBIT TFLAG:21,4
	;애널에 사정당하는 모습을 보였다
	SIF (STAIN:4 & 4)
		SETBIT TFLAG:21,5
	;정액 붓카케 당하는 모습을 보였다
	SIF (STAIN:0 & 4) || (STAIN:1 & 4) || (STAIN:2 & 4)
		SETBIT TFLAG:21,6
	;희롱감이 되는 모습을 보였다
	SETBIT TFLAG:21,7
	;절정하는 모습을 보였다
	SIF NOWEX:0 + NOWEX:1 + NOWEX:2 + NOWEX:3
		SETBIT TFLAG:21,8
ENDIF



