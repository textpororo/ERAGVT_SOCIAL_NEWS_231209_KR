;==================================================
;【전용 루틴（성행위）】
;--------------------------------------------------
@TENTACLE_MOB_802_SEX_ROUTINE, ARG:0, ARG:1

;악질 시민がいると撮影会を行いやすく
IF FLAG:72 == 1 && RAND:5 == 0
	RETURN 10
;自身のHPが高いと子機を飛ばす
;ELSEIF CLOTH_OUTER_PER < 90 && CLOTH_INNER_PER < CLOTH_INNER_DEF
;	RETURN 9
;撮影会の比率を少し上げる
ELSE
	LOCAL = RAND:100

	;15%以下の공격（겉옷&속옷の방어無視、양성평등）
	;Ｃ공격（약）
	IF LOCAL < 13
		RETURN 0

	;15%-13%=12%의 확률
	;Ｖ공격（약）
	ELSEIF LOCAL < 25
		IF ISMALE()
			RETURN 0
		ELSE
			RETURN 2
		ENDIF

	;37%-25%=12%의 확률
	;Ａ공격（약）
	ELSEIF LOCAL < 37
		RETURN 4

	;37%-25%=12%의 확률
	;Ａ공격（약）
	ELSEIF LOCAL < 37
		RETURN 4

	;65%-45%=20%의 확률
	;Ｂ공격（약）
	ELSEIF LOCAL < 70
		RETURN 6

	;100%-70%=30%の（양성평등）
	;수치 플레이
	ELSE
		RETURN 10
	ENDIF
ENDIF	
RETURN 1


