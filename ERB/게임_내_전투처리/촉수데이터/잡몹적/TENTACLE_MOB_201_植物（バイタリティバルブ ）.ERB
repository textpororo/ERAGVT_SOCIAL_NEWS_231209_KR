;==================================================
;【専用ルーチン（戦闘）】
;--------------------------------------------------
@TENTACLE_MOB_201_ATTACK_ROUTINE, ARG:0, ARG:1

LOCAL = RAND:100
;공격範囲が広い
SIF RAND:4 == 0
	TFLAG:11 |= 1
SIF RAND:4 == 0
	TFLAG:11 |= 2
SIF RAND:4 == 0
	TFLAG:11 |= 4

IF LOCAL < 45
	RETURN 2
ELSE
	RETURN 0
ENDIF



;==================================================
;【専用ルーチン（性行為）】
;--------------------------------------------------
@TENTACLE_MOB_201_SEX_ROUTINE, ARG:0, ARG:1

LOCAL = RAND:100

;20%以下の공격（겉옷&속옷の방어無視、男女平等）
;C공격(약)
IF LOCAL < 20
	RETURN 0

;40%-20%=20%の確率
;V공격(약)
ELSEIF LOCAL < 40
	IF ISMALE() ;남자は別行動に割り振り
		SELECTCASE RAND:4
		CASE 0
			RETURN 0
		CASE 1
			RETURN 4
		CASE 2
			RETURN 6
		CASE 3
			RETURN 11
		ENDSELECT
	ELSE
		RETURN 2
	ENDIF

;60%-40%=20%の確率
;A공격(약)
ELSEIF LOCAL < 60
	RETURN 4

;80%-60%=20%の確率
;B공격(약)
ELSEIF LOCAL < 80
	RETURN 6

;100%-80%=20%の（男女平等）
;フェラ
ELSE
	RETURN 11
ENDIF

;==================================================
;【性コマンドレファレンス】
;--------------------------------------------------
;ARG=1　스마타애태우기成功時の行動指定
;ARG=2　스마타애태우기失敗時の行動指定
;ARG=3　フェラチオ誘発時の行動指定
@TENTACLE_MOB_201_REACTION_REF, ARG = 0
IF ARG == 1
	RETURN -1
ELSEIF ARG == 2
	RETURN -1
ELSEIF ARG == 3
	RETURN 11
ENDIF
RETURN -1

