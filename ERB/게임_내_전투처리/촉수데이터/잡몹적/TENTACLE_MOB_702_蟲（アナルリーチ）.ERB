;==================================================
;【専用ルーチン（性行為）】
;--------------------------------------------------
;専用ルーチン（性行為）
@TENTACLE_MOB_702_SEX_ROUTINE

;ランダム係数호출（A攻め強、A攻め弱、CA攻め弱）
LOCAL = RAND:100

	;애널に潜り込む
IF LOCAL < 30 + (ISMALE()) * 20 || (TFLAG:20 == 5 && LOCAL < 80)
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		RETURN 5
	ENDIF

;애널攻め弱
ELSE
	RETURN 4, FLAG:11, 1
ENDIF

RETURN 1


;==================================================
;【性コマンドレファレンス】
