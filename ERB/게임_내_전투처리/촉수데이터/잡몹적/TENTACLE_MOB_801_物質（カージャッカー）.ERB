;==================================================
;【専用ルーチン（性行為）】
;--------------------------------------------------
@TENTACLE_MOB_801_SEX_ROUTINE, ARG:0, ARG:1

IF ISHOLE() == 0
	SELECTCASE RAND:7
		CASE 0
			RETURN 0	;一斉愛撫
		CASE 1
			RETURN 2	;一斉사정
		CASE 3
			RETURN 11	;口辱
		CASE 4
			RETURN 1	;C攻め
		CASE 5
			RETURN 9	;針
		CASE 6
		  RETURN 8	;スパンキング
	ENDSELECT

ELSEIF ISMALE()
	SELECTCASE RAND:8
		CASE 0
			RETURN 0	;一斉愛撫
		CASE 1
			RETURN 2	;一斉사정
		CASE 3
			RETURN 11	;口辱
		CASE 4
			RETURN 1	;C攻め
		CASE 5
			RETURN 9	;針
		CASE 6
		  RETURN 8	;スパンキング
		CASE 7
			RETURN 5	;A攻め
	ENDSELECT

ELSE
	LOCAL = RAND:110

	;全身愛撫
	IF LOCAL < 20
		RETURN 0

	;一斉사정
	ELSEIF LOCAL < 40
		RETURN 2

	;口辱
	ELSEIF LOCAL < 60
		RETURN 11

	;C攻め強
	ELSEIF LOCAL < 70
		RETURN 1

	;種付け
	ELSEIF LOCAL < 80 && CHECK_HOLYVIRGIN_F()==0
		RETURN 3

	;A攻め強
	ELSEIF LOCAL < 90
		RETURN 5

	;針
	ELSEIF LOCAL < 95
		RETURN 9

	;スパンキング
	ELSE
		RETURN 8
	ENDIF
ENDIF
RETURN 1

