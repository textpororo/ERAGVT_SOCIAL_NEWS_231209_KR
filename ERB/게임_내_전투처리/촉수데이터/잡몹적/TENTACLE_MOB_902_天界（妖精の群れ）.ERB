@BUST_SIZE_NAME(ARG,ARG:1)
#FUNCTIONS
IF ARG:1
	IF ISMALE(ARG)
		RETURNF ""
	ELSEIF TALENT:ARG:빈유 == 2
		RETURNF "揺れない"
	ELSEIF TALENT:ARG:빈유
		RETURNF "プルプル震える"
	ELSEIF TALENT:ARG:거유
		RETURNF "大きく揺れる"
	ELSEIF TALENT:ARG:거유 > 2
		RETURNF "ブンブン揺れる"
	ELSE
		RETURNF "揺れる"
	ENDIF
ELSE
	IF TALENT:ARG:보추 > 0
		RETURNF "ぺたんこな"
	ELSEIF ISMALE(ARG)
		RETURNF ""
	ELSEIF TALENT:ARG:빈유 == 2
		RETURNF "平らな"
	ELSEIF TALENT:ARG:빈유
		RETURNF "小振りな"
	ELSEIF TALENT:ARG:거유
		RETURNF "たわわな"
	ELSEIF TALENT:ARG:거유 > 2
		RETURNF "巨大な"
	ELSE
		RETURNF "形のいい"
	ENDIF
ENDIF

@PRINT_CLOTH_CONDITION()
#FUNCTIONS
;겉옷が防護機能を失ってない(下着は傷付いていない)
