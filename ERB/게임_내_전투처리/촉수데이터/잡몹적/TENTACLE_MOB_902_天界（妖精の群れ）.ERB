@TENTACLE_MOB_902_ATTACK_ROUTINE
;공격 범위가 넓다
SIF RAND:4 == 0
	TFLAG:11 |= 1
SIF RAND:4 == 0
	TFLAG:11 |= 2
SIF RAND:4 == 0
	TFLAG:11 |= 4

;기절・쓰러짐してる場合、絶対に範囲に含めるように
IF (TCVARn:12 & 기절) || (TCVARn:12 & 쓰러짐)
	SIF TCVARn:0 == 1
		TFLAG:11 |= 1
	SIF TCVARn:0 == 2
		TFLAG:11 |= 2
	SIF TCVARn:0 == 3
		TFLAG:11 |= 4
ENDIF


;現在체력がRAND最大체력より小さい時
IF FLAG:13 < RAND:(FLAG:12)
	RETURN 4	;距離を取る
ELSEIF RAND:3 == 0
	RETURN 6	;邪悪な波動
ELSEIF RAND:2 == 0
	RETURN 1	;通常공격
ELSE
	RETURN 2	;絡みつく
ENDIF

;==================================================
;【전용 루틴（성행위）】
;--------------------------------------------------
@TENTACLE_MOB_902_SEX_ROUTINE
IF RAND:4 == 0
	IF RAND:3 == 0 && !CHECK_HOLYVIRGIN_F()
		RETURN 3	;Ｖ공격（강）
	ELSEIF RAND:2 == 0
		RETURN 5	;Ａ공격（강）
	ELSE
		RETURN 11	;펠라
	ENDIF
ELSEIF RAND:3 == 0
	IF RAND:2 == 0
		RETURN 1	;Ｃ공격（강）
	ELSE
		RETURN 7	;Ｂ공격（강）
	ENDIF
ELSEIF RAND:2 == 0
	IF RAND:3 == 0
		RETURN 14	;의상을 찢는다
	ELSEIF RAND:3 == 0
		RETURN 8	;스팽킹
	ELSE
		RETURN 9	;바늘
	ENDIF
ELSE
	RETURN 0	;Ｃ공격（약）(전신 애무)
ENDIF

