;==================================================
;【전용 루틴（전투）】
;--------------------------------------------------
@TENTACLE_MOB_201_ATTACK_ROUTINE, ARG:0, ARG:1

LOCAL = RAND:100
;공격 범위가 넓다
SIF RAND:4 == 0
	TFLAG:11 |= 1
SIF RAND:4 == 0
	TFLAG:11 |= 2
SIF RAND:4 == 0
	TFLAG:11 |= 4

IF LOCAL < 45
	RETURN 2
ELSE
	RETURN 0
ENDIF



;==================================================
;【전용 루틴（성행위）】
;--------------------------------------------------
@TENTACLE_MOB_201_SEX_ROUTINE, ARG:0, ARG:1

LOCAL = RAND:100

;20% 이하의 공격（겉옷&속옷의 방어 무시, 양성평등）
;Ｃ공격（약）
IF LOCAL < 20
	RETURN 0

;40%-20%=20%의 확률
;Ｖ공격（약）
ELSEIF LOCAL < 40
	IF ISMALE() ;남자は別行動に割り振り
		SELECTCASE RAND:4
		CASE 0
			RETURN 0
		CASE 1
			RETURN 4
		CASE 2
			RETURN 6
		CASE 3
			RETURN 11
		ENDSELECT
	ELSE
		RETURN 2
	ENDIF

;60%-40%=20%의 확률
;Ａ공격（약）
ELSEIF LOCAL < 60
	RETURN 4

;80%-60%=20%의 확률
;Ｂ공격（약）
ELSEIF LOCAL < 80
	RETURN 6

;100%-80%=20%의 확률（양성평등）
;펠라
ELSE
	RETURN 11
ENDIF

