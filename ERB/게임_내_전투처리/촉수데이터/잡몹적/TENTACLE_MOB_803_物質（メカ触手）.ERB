	SIF TCVARn:0 == 1
		TFLAG:11 |= 1
	SIF TCVARn:0 == 2
		TFLAG:11 |= 2
	SIF TCVARn:0 == 3
		TFLAG:11 |= 4
ENDIF

;現在체력がRAND最大체력より小さい時
