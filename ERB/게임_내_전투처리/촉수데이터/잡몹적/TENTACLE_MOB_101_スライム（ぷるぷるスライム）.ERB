ENDIF


;==================================================
;【전용 루틴（성행위）】
;--------------------------------------------------
@TENTACLE_MOB_101_SEX_ROUTINE
LOCAL = RAND:100

;촉수 밸런스볼（Ｖ삽입）
IF LOCAL < 5 || (TFLAG:20 == 2 && LOCAL < 90) && ISFEMALE() && CHECK_HOLYVIRGIN_F()==0
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		RETURN 3
	ENDIF

;촉수 밸런스볼（Ｖ브러싱）
ELSEIF LOCAL < 40 && ISFEMALE()
	RETURN 2

;촉수 밸런스볼（약）
ELSE
	RETURN 0
ENDIF
RETURN 1

