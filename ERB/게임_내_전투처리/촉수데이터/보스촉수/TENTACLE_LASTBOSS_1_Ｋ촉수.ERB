	RETURN 1
ELSEIF LOCAL < 60
	CALL PRISON_COMABLE, 201
	RETURN 1
ELSEIF LOCAL < 70
	CALL PRISON_COMABLE, 300
	RETURN 1
ELSEIF LOCAL < 75
