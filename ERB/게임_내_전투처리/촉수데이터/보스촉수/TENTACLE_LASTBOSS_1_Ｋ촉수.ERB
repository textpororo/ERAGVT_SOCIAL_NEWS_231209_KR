;--------------------------------------------------
;専用ルーチン（戦闘）
@TENTACLE_LASTBOSS_1_ATTACK_ROUTINE
LOCAL = 0
LOCAL:1 = RAND:100
LOCAL:2 = BASE:방어 - 200
;油断していると距離を取る確率アップ
IF FLAG:17 >= FLAG:16 && LOCAL:1 < 35
	LOCAL = 4
;방어力の高い相手に対して공격の使用頻度ダウン、粘液の使用頻度アップ
ELSEIF LOCAL:2 && LOCAL:1 < (LOCAL:2 / 2)
	IF LOCAL:1 < 5
		LOCAL = 1
	ELSEIF LOCAL:1 < 35
		LOCAL = 3
	ELSE
		LOCAL = 2
	ENDIF
ELSEIF LOCAL:1 < 50
	LOCAL = 1
ELSEIF LOCAL:1 < 74
	LOCAL = 2
ELSEIF LOCAL:1 < 98
	LOCAL = 3
ELSE
	LOCAL = 4
ENDIF
RETURN LOCAL

;--------------------------------------------------
;専用ルーチン（性行為）
@TENTACLE_LASTBOSS_1_SEX_ROUTINE
LOCAL = RAND:100
IF LOCAL < 5
	RETURN 1000
ELSEIF LOCAL < 10
	RETURN 1001
ELSEIF LOCAL < 15
	RETURN 1002
ELSEIF LOCAL < 20
	RETURN 1003
ELSEIF LOCAL < 25
	RETURN 1004
ELSEIF LOCAL < 30
	RETURN 1005
ELSEIF LOCAL < 35
	RETURN 1006
ELSEIF LOCAL < 40
	RETURN 1007
ELSEIF LOCAL < 50
	RETURN 1015
ELSE
	RETURN -1
ENDIF

;--------------------------------------------------
;性コマンドレファレンス
;ARG=1　스마타애태우기成功時の行動指定
;ARG=2　스마타애태우기失敗時の行動指定
;ARG=3　フェラチオ誘発時の行動指定
@TENTACLE_LASTBOSS_1_REACTION_REF, ARG = 0
IF ARG == 1
	LOCAL = RAND:6
	IF LOCAL < 2
		RETURN 3
	ELSEIF LOCAL < 4
		RETURN 5
	ELSEIF LOCAL < 5
		RETURN 1001
	ELSE
		RETURN 1002
	ENDIF
ELSEIF ARG == 2
	RETURN 15
ELSEIF ARG == 3
	RETURN 11
ENDIF
RETURN -1

