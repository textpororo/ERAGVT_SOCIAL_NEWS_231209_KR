;--------------------------------------------------
;전용 루틴（전투）
@TENTACLE_BOSS_7_ATTACK_ROUTINE
LOCAL:0 = RAND:100
IF LOCAL:0 < 20
	RETURN 3
ELSE 
	RETURN 0
ENDIF

;--------------------------------------------------
;전용 루틴（성행위）
@TENTACLE_BOSS_7_SEX_ROUTINE
;삽입계 커맨드가 적기 때문에, 의상 파괴의 우선순위 향상
SIF RAND:100 < 25
	RETURN 14

LOCAL = RAND:100
;황홀だと최면간を仕掛ける確率UP
IF (TCVARn:12 & 황홀) && RAND:100 < 50
	RETURN 1014
ELSEIF LOCAL < 5
	RETURN 0
ELSEIF LOCAL < 15
	RETURN 1006
ELSEIF LOCAL < 30
	RETURN 1007
ELSEIF LOCAL < 40
	RETURN 6
ELSEIF LOCAL < 43
	RETURN 2
ELSEIF LOCAL < 50
	RETURN 3
ELSEIF LOCAL < 60
	RETURN 11
ELSEIF LOCAL < 70
	RETURN 12
ELSEIF LOCAL < 75
	RETURN 13
ELSEIF LOCAL < 85
	RETURN 1014
ELSE
	RETURN -1
ENDIF

;--------------------------------------------------
;성 커맨드 레퍼런스
;ARG=1　스마타 애태우기 성공 시의 행동 지정
;ARG=2　스마타 애태우기 실패 시의 행동 지정
;ARG=3　펠라치오 유발 시의 행동 지정
@TENTACLE_BOSS_7_REACTION_REF, ARG = 0
IF ARG == 1
	LOCAL = RAND:6
	IF LOCAL < 2
		RETURN 3
	ELSEIF LOCAL < 4
		RETURN 5
	ELSEIF LOCAL < 5
		RETURN 1001
	ELSE
		RETURN 1002
	ENDIF
ELSEIF ARG == 2
	RETURN 15
ELSEIF ARG == 3
	RETURN 11
ENDIF
RETURN -1


;--------------------------------------------------
;전용 루틴（유폐）
@TENTACLE_BOSS_7_PRISON_ROUTINE
LOCAL = RAND:100
IF LOCAL < 30
	CALL PRISON_COMABLE, 6
	RETURN 1
ELSEIF LOCAL < 35
	CALL PRISON_COMABLE, 102
	RETURN 1
ELSEIF LOCAL < 40
	CALL PRISON_COMABLE, 201
	RETURN 1
ELSE
	RETURN 0
ENDIF

