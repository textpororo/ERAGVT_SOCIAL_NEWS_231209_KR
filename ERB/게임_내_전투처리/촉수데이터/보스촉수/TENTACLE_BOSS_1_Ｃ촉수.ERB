;--------------------------------------------------
;専用ルーチン（性行為）
@TENTACLE_BOSS_1_SEX_ROUTINE
;挿入系コマンドが少ないため、衣装の破壊の優先度アップ
SIF RAND:100 < 12
	RETURN 14

LOCAL = RAND:100
IF LOCAL < 20
	RETURN 0
ELSEIF LOCAL < 35
	RETURN 1
ELSEIF LOCAL < 40
	RETURN 1000
ELSEIF LOCAL < 45
	RETURN 2
ELSEIF LOCAL < 55
	RETURN 3
ELSEIF LOCAL < 60
	RETURN 11
ELSEIF LOCAL < 75
	RETURN 1008
ELSE
	RETURN -1
ENDIF

;--------------------------------------------------
;性コマンドレファレンス
;ARG=1　스마타애태우기成功時の行動指定
;ARG=2　스마타애태우기失敗時の行動指定
;ARG=3　フェラチオ誘発時の行動指定
@TENTACLE_BOSS_1_REACTION_REF, ARG = 0
IF ARG == 1
	LOCAL = RAND:6
	IF LOCAL < 2
		RETURN 3
	ELSEIF LOCAL < 4
		RETURN 5
	ELSEIF LOCAL < 5
		RETURN 1001
	ELSE
		RETURN 1002
	ENDIF
ELSEIF ARG == 2
	RETURN 15
ELSEIF ARG == 3
	RETURN 11
ENDIF
RETURN -1

;--------------------------------------------------
;専用ルーチン（유폐）
@TENTACLE_BOSS_1_PRISON_ROUTINE
LOCAL = RAND:100
IF LOCAL < 30
	CALL PRISON_COMABLE, 0
	RETURN 1
ELSEIF LOCAL < 40
	CALL PRISON_COMABLE, 100
	RETURN 1
ELSE
	RETURN 0
ENDIF

;--------------------------------------------------
;촉수サイズ補正
;この補正の後に部位ごとの補正が入る
@TENTACLE_BOSS_1_TENTACLE_SIZE,ARG
;C用촉수
LOCAL:0 = 200
LOCAL:4 = RAND:3 + 1
;V用촉수
IF ARG == 3
	LOCAL:1 = 100
	LOCAL:5 = 1
ELSE
	LOCAL:1 = 60
	LOCAL:5 = RAND:5 + 1
ENDIF
;A用촉수
LOCAL:2 = 60
LOCAL:6 = RAND:5 + 1
;B用촉수
LOCAL:3 = 80
LOCAL:7 = RAND:3 + 1
RETURN LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7
