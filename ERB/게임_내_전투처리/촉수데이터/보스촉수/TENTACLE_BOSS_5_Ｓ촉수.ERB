;専用ルーチン（戦闘）
@TENTACLE_BOSS_5_ATTACK_ROUTINE
LOCAL:0 = RAND:100
IF LOCAL:0 < 20
	RETURN 1
ELSE 
	RETURN 0
ENDIF

;--------------------------------------------------
;専用ルーチン（性行為）
@TENTACLE_BOSS_5_SEX_ROUTINE
;挿入系コマンドが少ないため、衣装の破壊の優先度アップ
SIF RAND:100 < 5
	RETURN 14

LOCAL = RAND:100
IF BASE:체력 == 0 && BASE:기력 == 0 && LOCAL < 40
	RETURN 3
ELSEIF LOCAL < 15 && TFLAG:0 > 5
	RETURN 1012
ELSEIF LOCAL < 20
	RETURN 8
ELSEIF LOCAL < 35 && CHECK_HOLYVIRGIN_F()==0
	RETURN 9
ELSEIF LOCAL < 40
	RETURN 1004
ELSEIF LOCAL < 45
	RETURN 2
ELSEIF LOCAL < 55
	RETURN 11
ELSEIF LOCAL < 80
	RETURN 12
ELSEIF LOCAL < 85
	RETURN 13
ELSE
	RETURN -1
ENDIF

;--------------------------------------------------
;性コマンドレファレンス
;ARG=1　스마타애태우기成功時の行動指定
;ARG=2　스마타애태우기失敗時の行動指定
;ARG=3　フェラチオ誘発時の行動指定
@TENTACLE_BOSS_5_REACTION_REF, ARG = 0
IF ARG == 1
	LOCAL = RAND:6
	IF LOCAL < 2
		RETURN 3
	ELSEIF LOCAL < 4
		RETURN 5
	ELSEIF LOCAL < 5
		RETURN 1001
	ELSE
		RETURN 1002
	ENDIF
ELSEIF ARG == 2
	RETURN 15
ELSEIF ARG == 3
	RETURN 12
ENDIF
RETURN -1


;--------------------------------------------------
;専用ルーチン（유폐）
@TENTACLE_BOSS_5_PRISON_ROUTINE
LOCAL = RAND:100
IF LOCAL < 30
	CALL PRISON_COMABLE, 4
	RETURN 1
ELSEIF LOCAL < 40
	CALL PRISON_COMABLE, 300
	RETURN 1
ELSE
	RETURN 0
ENDIF

