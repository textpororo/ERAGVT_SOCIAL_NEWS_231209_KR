;--------------------------------------------------
;전용 루틴（전투）
@TENTACLE_BOSS_5_ATTACK_ROUTINE
LOCAL:0 = RAND:100
IF LOCAL:0 < 20
	RETURN 1
ELSE 
	RETURN 0
ENDIF

;--------------------------------------------------
;전용 루틴（성행위）
@TENTACLE_BOSS_5_SEX_ROUTINE
;삽입계 커맨드가 적기 때문에, 의상 파괴의 우선순위 향상
SIF RAND:100 < 5
	RETURN 14

LOCAL = RAND:100
IF BASE:체력 == 0 && BASE:기력 == 0 && LOCAL < 40
	RETURN 3
ELSEIF LOCAL < 15 && TFLAG:0 > 5
	RETURN 1012
ELSEIF LOCAL < 20
	RETURN 8
ELSEIF LOCAL < 35 && CHECK_HOLYVIRGIN_F()==0
	RETURN 9
ELSEIF LOCAL < 40
	RETURN 1004
ELSEIF LOCAL < 45
	RETURN 2
ELSEIF LOCAL < 55
	RETURN 11
ELSEIF LOCAL < 80
	RETURN 12
ELSEIF LOCAL < 85
	RETURN 13
ELSE
	RETURN -1
ENDIF

;--------------------------------------------------
;성 커맨드 레퍼런스
;ARG=1　스마타 애태우기 성공 시의 행동 지정
;ARG=2　스마타 애태우기 실패 시의 행동 지정
;ARG=3　펠라치오 유발 시의 행동 지정
@TENTACLE_BOSS_5_REACTION_REF, ARG = 0
IF ARG == 1
	LOCAL = RAND:6
	IF LOCAL < 2
		RETURN 3
	ELSEIF LOCAL < 4
		RETURN 5
	ELSEIF LOCAL < 5
		RETURN 1001
	ELSE
		RETURN 1002
	ENDIF
ELSEIF ARG == 2
	RETURN 15
ELSEIF ARG == 3
	RETURN 12
ENDIF
RETURN -1


;--------------------------------------------------
;전용 루틴（유폐）
@TENTACLE_BOSS_5_PRISON_ROUTINE
LOCAL = RAND:100
IF LOCAL < 30
	CALL PRISON_COMABLE, 4
	RETURN 1
ELSEIF LOCAL < 40
	CALL PRISON_COMABLE, 300
	RETURN 1
ELSE
	RETURN 0
ENDIF

