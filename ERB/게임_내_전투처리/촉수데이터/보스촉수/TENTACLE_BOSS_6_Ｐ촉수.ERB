	RETURN 2
ELSE 
	RETURN 0
ENDIF

;--------------------------------------------------
;専用ルーチン（性行為）
@TENTACLE_BOSS_6_SEX_ROUTINE
;挿入系コマンドが少ないため、衣装の破壊の優先度アップ
SIF RAND:100 < 12
	RETURN 14

LOCAL = RAND:100
;성내성０で丸飲み準備、敗北確定嬲り中は確定
IF BASE:성내성 == 0 && (TFLAG:23 == 0 || TFLAG:23 == 100)
	RETURN 1013
ELSEIF TFLAG:23 && RAND:100 < 90 - TFLAG:23 * 5
	RETURN 1013
ELSEIF LOCAL < 20
	RETURN 10
ELSEIF LOCAL < 35
	RETURN 11
ELSEIF LOCAL < 40
	RETURN 1005
ELSEIF LOCAL < 45
	RETURN 2
ELSEIF LOCAL < 50
	RETURN 3
ELSEIF LOCAL < 60
	RETURN 5
ELSEIF LOCAL < 70
	RETURN 12
ELSEIF LOCAL < 75
	RETURN 13
ELSE
	RETURN -1
ENDIF

;--------------------------------------------------
;性コマンドレファレンス
;ARG=1　스마타애태우기成功時の行動指定
;ARG=2　스마타애태우기失敗時の行動指定
;ARG=3　フェラチオ誘発時の行動指定
@TENTACLE_BOSS_6_REACTION_REF, ARG = 0
IF ARG == 1
	LOCAL = RAND:6
	IF LOCAL < 2
		RETURN 3
	ELSEIF LOCAL < 4
		RETURN 5
	ELSEIF LOCAL < 5
		RETURN 1001
	ELSE
		RETURN 1002
	ENDIF
ELSEIF ARG == 2
	RETURN 15
ELSEIF ARG == 3
	RETURN 11
ENDIF
RETURN -1


;--------------------------------------------------
;専用ルーチン（유폐）
@TENTACLE_BOSS_6_PRISON_ROUTINE
LOCAL = RAND:100
IF CHECK_HOLYVIRGIN_F()==0
	IF LOCAL < 30
		CALL PRISON_COMABLE, 5
		RETURN 1
	ELSEIF LOCAL < 35
		CALL PRISON_COMABLE, 101
		RETURN 1
	ELSEIF LOCAL < 40
		CALL PRISON_COMABLE, 200
		RETURN 1
	ELSE
		RETURN 0
	ENDIF
ELSE
	IF LOCAL < 30
		CALL PRISON_COMABLE, 5
		RETURN 1
	ELSEIF LOCAL < 35
		CALL PRISON_COMABLE, 102
		RETURN 1
	ELSEIF LOCAL < 40
		CALL PRISON_COMABLE, 201
		RETURN 1
	ELSE
		RETURN 0
	ENDIF
ENDIF

