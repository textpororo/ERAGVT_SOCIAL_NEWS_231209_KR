	ENDIF
	IF CHOICECOUNT_F() < 100
		FOR LOCAL,0,101 - CHOICECOUNT_F()
			LOCAL:1 = RAND:100
			IF LOCAL:1 < 9
				CALL ADDRANDCHOOSE,0
			ELSEIF LOCAL:1 < 12
				CALL ADDRANDCHOOSE,1
			ELSEIF LOCAL:1 < 21
				CALL ADDRANDCHOOSE,2
			ELSEIF LOCAL:1 < 24
				CALL ADDRANDCHOOSE,3
			ELSEIF LOCAL:1 < 33
