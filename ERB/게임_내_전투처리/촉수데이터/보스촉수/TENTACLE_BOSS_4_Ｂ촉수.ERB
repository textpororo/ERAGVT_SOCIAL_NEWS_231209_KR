;--------------------------------------------------
;専用ルーチン（性行為）
@TENTACLE_BOSS_4_SEX_ROUTINE
;挿入系コマンドが少ないため、衣装の破壊の優先度アップ
SIF RAND:100 < 15
	RETURN 14

LOCAL = RAND:100
IF TFLAG:0 > 10 && LOCAL < 25
	RETURN 1011
ELSEIF LOCAL < 15
	RETURN 6
ELSEIF LOCAL < 30
	RETURN 7
ELSEIF LOCAL < 35
	RETURN 1003
ELSEIF LOCAL < 45
	RETURN 1005
ELSEIF LOCAL < 55
	RETURN 1011
ELSEIF LOCAL < 60
	RETURN 2
ELSEIF LOCAL < 65
	RETURN 3
ELSEIF LOCAL < 70
	RETURN 11
ELSEIF LOCAL < 75
	RETURN 12
ELSE
	RETURN -1
ENDIF


;--------------------------------------------------
;性コマンドレファレンス
;ARG=1　스마타애태우기成功時の行動指定
;ARG=2　스마타애태우기失敗時の行動指定
;ARG=3　フェラチオ誘発時の行動指定
@TENTACLE_BOSS_4_REACTION_REF, ARG = 0
IF ARG == 1
	LOCAL = RAND:6
	IF LOCAL < 2
		RETURN 3
	ELSEIF LOCAL < 4
		RETURN 5
	ELSEIF LOCAL < 5
		RETURN 1001
	ELSE
		RETURN 1002
	ENDIF
ELSEIF ARG == 2
	RETURN 15
ELSEIF ARG == 3
	RETURN 11
ENDIF
RETURN -1


;--------------------------------------------------
;専用ルーチン（유폐）
@TENTACLE_BOSS_4_PRISON_ROUTINE
LOCAL = RAND:100
IF LOCAL < 30
	CALL PRISON_COMABLE, 3
	RETURN 1
ELSEIF LOCAL < 40
	CALL PRISON_COMABLE, 103
	RETURN 1
ELSEIF LOCAL < 50
	CALL PRISON_COMABLE, 105
	RETURN 1
ELSE
	RETURN 0
ENDIF

;--------------------------------------------------
;촉수サイズ補正
;この補正の後に部位ごとの補正が入る
@TENTACLE_BOSS_4_TENTACLE_SIZE,ARG
;C用촉수
LOCAL:0 = 70
LOCAL:4 = RAND:4 + 1
;V用촉수
IF ARG == 1011
	LOCAL:1 = 120
	LOCAL:5 = 1
ELSE
	LOCAL:1 = 80
	LOCAL:5 = RAND:3 + 1
ENDIF
;A用촉수
LOCAL:2 = 70
LOCAL:6 = RAND:4 + 1
;B用촉수
LOCAL:3 = 180
LOCAL:7 = 2
RETURN LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7
