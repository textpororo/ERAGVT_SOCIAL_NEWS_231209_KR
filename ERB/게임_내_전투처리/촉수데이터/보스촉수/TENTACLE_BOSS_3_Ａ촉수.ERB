;--------------------------------------------------
;전용 루틴（전투）
@TENTACLE_BOSS_3_ATTACK_ROUTINE
LOCAL = 0
RETURN LOCAL

;--------------------------------------------------
;전용 루틴（성행위）
@TENTACLE_BOSS_3_SEX_ROUTINE
LOCAL = RAND:100
IF LOCAL < 32
	RETURN 4
ELSEIF LOCAL < 51
	RETURN 5
ELSEIF LOCAL < 59
	RETURN 1002
ELSEIF LOCAL < 64
	RETURN 1010
ELSEIF LOCAL < 69
	RETURN 2
ELSEIF LOCAL < 74
	RETURN 1
ELSEIF CONFIG_CHECK_OTHER_F(4) > 0
	LOCAL = RAND:100
	IF LOCAL < 22
		RETURN 8
	ELSEIF LOCAL < 44
		RETURN 9
	ELSEIF LOCAL < 66
		RETURN 10
	ELSEIF LOCAL < 84
		RETURN 11
	ELSEIF LOCAL < 88
		RETURN 1004
	ELSEIF LOCAL < 92
		RETURN 1006
	ELSEIF LOCAL < 96
		RETURN 1007
	ELSE
		RETURN 1010
	ENDIF
ENDIF

;--------------------------------------------------
;성 커맨드 레퍼런스
;ARG=1　스마타 애태우기 성공 시의 행동 지정
;ARG=2　스마타 애태우기 실패 시의 행동 지정
;ARG=3　펠라치오 유발 시의 행동 지정
@TENTACLE_BOSS_3_REACTION_REF, ARG = 0
IF ARG == 1
	LOCAL = RAND:6
	IF LOCAL < 2
		RETURN 3
	ELSEIF LOCAL < 4
		RETURN 5
	ELSEIF LOCAL < 5
		RETURN 1001
	ELSE
		RETURN 1002
	ENDIF
ELSEIF ARG == 2
	RETURN 15
ELSEIF ARG == 3
	RETURN 11
ENDIF
RETURN -1


;--------------------------------------------------
;전용 루틴（유폐）
@TENTACLE_BOSS_3_PRISON_ROUTINE
LOCAL = RAND:100
IF LOCAL < 45
	CALL PRISON_COMABLE, 2
	RETURN 1
ELSEIF LOCAL < 60
	CALL PRISON_COMABLE, 102
	RETURN 1
ELSEIF LOCAL < 75
	CALL PRISON_COMABLE, 201
	RETURN 1
ELSE
	LOCAL = RAND:100
	IF LOCAL < 35
		CALL PRISON_COMABLE, 4
		RETURN 1
	ELSEIF LOCAL < 70
		CALL PRISON_COMABLE, 6
		RETURN 1
	ELSEIF LOCAL < 85
		CALL PRISON_COMABLE, 300
		RETURN 1
	ELSE
		CALL PRISON_COMABLE, 301
		RETURN 1
	ENDIF
ENDIF

;--------------------------------------------------
;촉수 크기 보정
;이 보정 후에 부위별 보정이 들어간다
@TENTACLE_BOSS_3_TENTACLE_SIZE,ARG
;C용 촉수
LOCAL:0 = 85
LOCAL:4 = RAND:5 + 2
;V용 촉수
LOCAL:1 = 60
LOCAL:5 = RAND:5 + 1
;A용 촉수
IF ARG == 1010 || ARG == -102
	LOCAL:2 = 180
	LOCAL:6 = 1
ELSE
	LOCAL:2 = 90
	LOCAL:6 = RAND:3 + 1
ENDIF
;B용 촉수
LOCAL:3 = 95
LOCAL:7 = RAND:3 + 2
RETURN LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7
