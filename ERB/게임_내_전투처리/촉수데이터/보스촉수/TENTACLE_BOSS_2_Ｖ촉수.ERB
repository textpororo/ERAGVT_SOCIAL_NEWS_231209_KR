;--------------------------------------------------
;전용 루틴（전투）
@TENTACLE_BOSS_2_ATTACK_ROUTINE
LOCAL = 0
RETURN LOCAL

;--------------------------------------------------
;전용 루틴（성행위）
@TENTACLE_BOSS_2_SEX_ROUTINE
;남자인 경우, 범용 행동만 취한다
;오토코노코인 경우에는 범용과 펠라, 이라마치오도 취한다
SIF RAND:3 == 0 && TALENT:오토코노코 > 0
	RETURN 11
SIF RAND:2 == 0 && TALENT:오토코노코 > 0
	RETURN 12
SIF ISMALE()
	RETURN 0
LOCAL = RAND:100

IF LOCAL < 60 && PALAM:윤활 < 2000
	RETURN 2
ELSEIF LOCAL < 10
	RETURN 2
ELSEIF LOCAL < 40
	RETURN 3
ELSEIF LOCAL < 55
	RETURN 1001
ELSEIF LOCAL < 70
	RETURN 1009
ELSEIF LOCAL < 75
	RETURN 11
ELSEIF LOCAL < 80
	RETURN 12
ELSE
	RETURN -1
ENDIF

;--------------------------------------------------
;성 커맨드 레퍼런스
;ARG=1　스마타 애태우기 성공 시의 행동 지정
;ARG=2　스마타 애태우기 실패 시의 행동 지정
;ARG=3　펠라치오 유발 시의 행동 지정
@TENTACLE_BOSS_2_REACTION_REF, ARG = 0
IF ARG == 1
	LOCAL = RAND:6
	IF LOCAL < 2
		RETURN 3
	ELSEIF LOCAL < 4
		RETURN 5
	ELSEIF LOCAL < 5
		RETURN 1001
	ELSE
		RETURN 1002
	ENDIF
ELSEIF ARG == 2
	RETURN 15
ELSEIF ARG == 3
	RETURN 11
ENDIF
RETURN -1

;--------------------------------------------------
;전용 루틴（유폐）
@TENTACLE_BOSS_2_PRISON_ROUTINE
LOCAL = RAND:100
IF LOCAL < 30
	CALL PRISON_COMABLE, 1
	RETURN 1
ELSEIF LOCAL < 40
	CALL PRISON_COMABLE, 101
	RETURN 1
ELSEIF LOCAL < 50
	CALL PRISON_COMABLE, 200
	RETURN 1
ELSEIF LOCAL < 60
	CALL PRISON_COMABLE, 104
	RETURN 1
ELSE
	RETURN 0
ENDIF

;--------------------------------------------------
;촉수 크기 보정
;이 보정 후에 부위별 보정이 들어간다
@TENTACLE_BOSS_2_TENTACLE_SIZE,ARG
;C용 촉수
LOCAL:0 = 60
LOCAL:4 = RAND:25 + 25
;V용 촉수
IF ARG == 1009 || ARG == -101
	LOCAL:1 = 150
	LOCAL:5 = 1
ELSE
	LOCAL:1 = 90
	LOCAL:5 = RAND:3 + 1
ENDIF
;A용 촉수
LOCAL:2 = 60
LOCAL:6 = RAND:5 + 1
;B용 촉수
LOCAL:3 = 100
LOCAL:7 = 1
RETURN LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7

