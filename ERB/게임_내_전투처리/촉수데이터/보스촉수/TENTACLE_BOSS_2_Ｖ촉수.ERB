;--------------------------------------------------
;専用ルーチン（性行為）
@TENTACLE_BOSS_2_SEX_ROUTINE
;남자の場合、汎用行動のみを行う
;보추の場合は汎用とフェラ、イマラチオも
SIF RAND:3 == 0 && TALENT:보추 > 0
	RETURN 11
SIF RAND:2 == 0 && TALENT:보추 > 0
	RETURN 12
SIF ISMALE()
	RETURN 0
LOCAL = RAND:100

IF LOCAL < 60 && PALAM:윤활 < 2000
	RETURN 2
ELSEIF LOCAL < 10
	RETURN 2
ELSEIF LOCAL < 40
	RETURN 3
ELSEIF LOCAL < 55
	RETURN 1001
ELSEIF LOCAL < 70
	RETURN 1009
ELSEIF LOCAL < 75
	RETURN 11
ELSEIF LOCAL < 80
	RETURN 12
ELSE
	RETURN -1
ENDIF

;--------------------------------------------------
;性コマンドレファレンス
;ARG=1　스마타애태우기成功時の行動指定
;ARG=2　스마타애태우기失敗時の行動指定
;ARG=3　フェラチオ誘発時の行動指定
@TENTACLE_BOSS_2_REACTION_REF, ARG = 0
IF ARG == 1
	LOCAL = RAND:6
	IF LOCAL < 2
		RETURN 3
	ELSEIF LOCAL < 4
		RETURN 5
	ELSEIF LOCAL < 5
		RETURN 1001
	ELSE
		RETURN 1002
	ENDIF
ELSEIF ARG == 2
	RETURN 15
ELSEIF ARG == 3
	RETURN 11
ENDIF
RETURN -1

;--------------------------------------------------
;専用ルーチン（幽閉）
@TENTACLE_BOSS_2_PRISON_ROUTINE
LOCAL = RAND:100
IF LOCAL < 30
	CALL PRISON_COMABLE, 1
	RETURN 1
ELSEIF LOCAL < 40
	CALL PRISON_COMABLE, 101
	RETURN 1
ELSEIF LOCAL < 50
	CALL PRISON_COMABLE, 200
	RETURN 1
ELSEIF LOCAL < 60
	CALL PRISON_COMABLE, 104
	RETURN 1
ELSE
	RETURN 0
ENDIF

;--------------------------------------------------
;촉수サイズ補正
;この補正の後に部位ごとの補正が入る
@TENTACLE_BOSS_2_TENTACLE_SIZE,ARG
;C用촉수
LOCAL:0 = 60
LOCAL:4 = RAND:25 + 25
;V用촉수
IF ARG == 1009 || ARG == -101
	LOCAL:1 = 150
	LOCAL:5 = 1
ELSE
	LOCAL:1 = 90
	LOCAL:5 = RAND:3 + 1
ENDIF
;A用촉수
LOCAL:2 = 60
LOCAL:6 = RAND:5 + 1
;B用촉수
LOCAL:3 = 100
LOCAL:7 = 1
RETURN LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7

