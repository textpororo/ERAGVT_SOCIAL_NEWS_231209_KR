;--------------------------------------------------
;촉수の사정に関する処理
;기교の値で촉수の사정 게이지増加量を求める
@TENTACLE_SYASEI_UP, ARG
;기교を見る
IF ABL:기교 == 0
	TIMES ARG, 1.00
ELSEIF ABL:기교 == 1
	TIMES ARG, 1.10
ELSEIF ABL:기교 == 2
	TIMES ARG, 1.20
ELSEIF ABL:기교 == 3
	TIMES ARG, 1.25
ELSEIF ABL:기교 == 4
	TIMES ARG, 1.30
ELSEIF ABL:기교 == 5
	TIMES ARG, 1.35
ENDIF
;V감각を見る
IF (INSERT & 질)
	IF ABL:V감각 == 0
		TIMES ARG, 1.00
	ELSEIF ABL:V감각 == 1
		TIMES ARG, 1.05
	ELSEIF ABL:V감각 == 2
		TIMES ARG, 1.15
	ELSEIF ABL:V감각 == 3
		TIMES ARG, 1.25
	ELSEIF ABL:V감각 == 4
		TIMES ARG, 1.40
	ELSEIF ABL:V감각 == 5
		TIMES ARG, 1.80
	ENDIF
ENDIF
;A감각を見る
IF (INSERT & 애널)
	IF ABL:A감각 == 0
		TIMES ARG, 1.00
	ELSEIF ABL:A감각 == 1
		TIMES ARG, 1.05
	ELSEIF ABL:A감각 == 2
		TIMES ARG, 1.15
	ELSEIF ABL:A감각 == 3
		TIMES ARG, 1.25
	ELSEIF ABL:A감각 == 4
		TIMES ARG, 1.40
	ELSEIF ABL:A감각 == 5
		TIMES ARG, 1.80
	ENDIF
ENDIF
;거유
IF (TFLAG:4 & 가슴)
	IF TALENT:거유 == 5
		TIMES ARG, 1.40
	ELSEIF TALENT:거유 == 4
		TIMES ARG, 1.35
	ELSEIF TALENT:거유 == 3
		TIMES ARG, 1.30
	ELSEIF TALENT:거유 == 2
		TIMES ARG, 1.25
	ELSEIF TALENT:거유 == 1
		TIMES ARG, 1.20
	ENDIF
ENDIF

;ランダム性付与
LOCAL:1 = RAND:100
IF LOCAL:1 < 20
	TIMES ARG, 0.90
ELSEIF LOCAL:1 < 40
	TIMES ARG, 0.95
ELSEIF LOCAL:1 < 60
ELSEIF LOCAL:1 < 80
	TIMES ARG, 1.05
ELSE
	TIMES ARG, 1.10
ENDIF
;가만히있는다
SIF TCVARn:2 == 자세：가만히있는다
	TIMES ARG, 0.50
;봉사한다
SIF TCVARn:2 == 자세：봉사한다
	TIMES ARG, 1.20
FLAG:15 += ARG

;사정チェック
;----------------------------------------------------------
@TENTACLE_SYASEI_CHECK
;敵がペニスを持たない場合
IF ENEMY_TYPE_CHECK_F("AKUOTI") == 0 && FLAG:11 == 901 && FLAG:15 >= FLAG:14
	FONTBOLD
	PRINTL 적 절정
	FONTREGULAR
	CALL TENTACLE_ACCESS, "NAME"
	PRINTFORML は耐え切れずに絶頂に達した！
	PRINTL 

	;방심도 상승
	TFLAG:3 += 100
	;搾精強化機能
	IF CONFIG_CHECK_BALANCE_F(3) > 0 && LOCAL:2 > 0
		IF FLAG:73 > 0
			PRINTFORML %조사처리(PRINT_TRANSCALLNAME(TARGET),"는")%精液からエネルギーを吸収した！
		ELSE
			PRINTFORML %조사처리(PRINT_TRANSCALLNAME(TARGET),"는")%촉수の精液からエネルギーを吸収した！
		ENDIF
		CALL GET_SYUREN, (RAND:10 + RAND:(LOCAL:2 * 3) + (LOCAL:2 * 3) + 2) / 5
		PRINTL 
	ENDIF
	;搾精ダメージの処理
	CALL TENTACLE_SAKUSEI,FLAG:15, 0, 0, 0, 0
	;사정 게이지低下
	FLAG:15 = FLAG:15 - FLAG:14

	;강구속を解く
	SIF (TCVARn:12 & 강구속) > 0
		TCVARn:12 -= 강구속

	RETURN 0, 0, 0, 0
ELSEIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && (ISFEMALE(FLAG:111) && TALENT:(FLAG:111):후타나리 < 1 && TALENT:(FLAG:111):기생 == 0) && FLAG:15 >= FLAG:14
	FONTBOLD
	PRINTL 적 절정
	FONTREGULAR
	PRINTFORM %PRINT_TRANSCALLNAME(FLAG:111)%
	PRINTFORML は耐え切れずに絶頂に達した！
	PRINTL 

	;방심도 상승
	TFLAG:3 += 100
	;搾精強化機能
	IF CONFIG_CHECK_BALANCE_F(3) > 0 && LOCAL:2 > 0
		IF FLAG:73 > 0
			PRINTFORML %조사처리(PRINT_TRANSCALLNAME(TARGET),"는")%精液からエネルギーを吸収した！
		ELSE
			PRINTFORML %조사처리(PRINT_TRANSCALLNAME(TARGET),"는")%촉수の精液からエネルギーを吸収した！
		ENDIF
		CALL GET_SYUREN, (RAND:10 + RAND:(LOCAL:2 * 3) + (LOCAL:2 * 3) + 2) / 5
		PRINTL 
	ENDIF
	;搾精ダメージの処理
	CALL TENTACLE_SAKUSEI,FLAG:15, 0, 0, 0, 0
	;사정 게이지低下
	FLAG:15 = FLAG:15 - FLAG:14

	;강구속を解く
	SIF (TCVARn:12 & 강구속) > 0
		TCVARn:12 -= 강구속

	RETURN 0, 0, 0, 0
;사정箇所が指定されていない場合
ELSEIF (TFLAG:4 == 0 || (TFLAG:4 & 오발)) && FLAG:15 >= FLAG:14
	IF (FLAG:15 >= FLAG:14 * 2 && TFLAG:20 != 15 && TFLAG:20 != 17 && TFLAG:20 != 19) || (TFLAG:4 & 오발)
		;대량사정

		;방심도 상승
		TFLAG:3 += 200

		;사정場所チェック
		CALL TENTACLE_SYASEI_POINT, 2
		;搾精ダメージの処理
		CALL TENTACLE_SAKUSEI,FLAG:15 * 4, RESULT:0, RESULT:1, RESULT:2, RESULT:3
		;사정 게이지低下
		FLAG:15 = 0

		;강구속を解く
		SIF (TCVARn:12 & 강구속) > 0
			TCVARn:12 -= 강구속

		RETURN RESULT:0, RESULT:1, RESULT:2, RESULT:3
	ELSE
		;지문: 사정我慢
		CALL MESSAGE_SEX_TENTACLE_SYASEI_GAMAN
		;사정値 50
		CALL TENTACLE_SYASEI_UP, 50
			RETURN 0, 0, 0, 0
	ENDIF
ELSEIF FLAG:15 >= FLAG:14 * 2
	;대량사정

	;방심도 상승
	TFLAG:3 += 200

	;사정場所チェック
	CALL TENTACLE_SYASEI_POINT, 2
	;搾精ダメージの処理
	CALL TENTACLE_SAKUSEI,FLAG:15 * 2, RESULT:0, RESULT:1, RESULT:2, RESULT:3
	;사정 게이지低下
	FLAG:15 = 0

	;강구속を解く
	SIF (TCVARn:12 & 강구속) > 0
		TCVARn:12 -= 강구속

	RETURN RESULT:0, RESULT:1, RESULT:2, RESULT:3
ELSEIF FLAG:15 >= FLAG:14
	;通常사정

	;방심도 상승
	TFLAG:3 += 100

	;사정場所チェック
	CALL TENTACLE_SYASEI_POINT, 1
	;搾精ダメージの処理
	CALL TENTACLE_SAKUSEI,FLAG:15, RESULT:0, RESULT:1, RESULT:2, RESULT:3
	;사정 게이지低下
	FLAG:15 = FLAG:15 - FLAG:14

	;강구속を解く
	SIF (TCVARn:12 & 강구속) > 0
		TCVARn:12 -= 강구속

	RETURN RESULT:0, RESULT:1, RESULT:2, RESULT:3
ELSE
	TFLAG:5 = 0
	RETURN 0, 0, 0, 0
ENDIF

;--------------------------------------------------
;사정場所チェック ARGは1=通常사정,2=대량사정
@TENTACLE_SYASEI_POINT, ARG
VARSET LOCAL
PRINTL 
IF TFLAG:4 == 0 || (TFLAG:4 & 오발)
