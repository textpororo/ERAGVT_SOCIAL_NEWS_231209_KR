	STAIN:1 |= 4
	;사정個所を記録
	TFLAG:5 |= 오발

	;지문: 촉수사정오발
	CALL MESSAGE_SEX_TENTACLE_SYASEI_EXPLODE
	EXP:정액경험 += ARG

	LOCAL:2 += ARG
ENDIF

IF TFLAG:18 != 균열 && ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && (ISPENIS(FLAG:111)) && ISTENTACLER(FLAG:111) == 0
	LOCAL:3 = 0
ELSE
	LOCAL:3 = 1
ENDIF
IF (TFLAG:4 & 균열) && LOCAL:3
	;汚れ
	STAIN:3 |= 4
	;사정個所を記録
	TFLAG:5 |= 균열
	
	IF ARG == 1
		;지문: 촉수ワレメ사정
		CALL MESSAGE_SEX_TENTACLE_SYASEI_WAREME

		;지문: 세뇌/흑화 캐릭터가 조작 캐릭터의 질내에 사정した
		SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
			CALL MESSAGE_OTHER_SEX_TENTACLE_SYASEI_VAGINA

		;윤활の加算
		LOCAL:0 += 1000
		;굴복の加算
		LOCAL:1 += 500
		;공순の加算
		LOCAL:4 += 100 * ABL:순종
		;욕정の加算
		LOCAL:5 += 100 * ABL:욕망

		EXP:정액경험 += ARG
		LOCAL:2 += ARG
	ELSEIF ARG == 2
		;지문: 촉수ワレメ대량사정
		CALL MESSAGE_SEX_TENTACLE_SYASEI_WAREME_HI

		;지문: 세뇌/흑화 캐릭터가 조작 캐릭터의 질내に대량사정した
		SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
			CALL MESSAGE_OTHER_SEX_TENTACLE_SYASEI_VAGINA_HI

		;윤활の加算
		LOCAL:0 += 2000
		;굴복の加算
		LOCAL:1 += 1000
		;공순の加算
		LOCAL:4 += 200 * ABL:순종
		;욕정の加算
		LOCAL:5 += 200 * ABL:욕망

		EXP:정액경험 += ARG
		LOCAL:2 += ARG
	ENDIF
ENDIF

IF TFLAG:18 != 질 && ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && (ISPENIS(FLAG:111)) && ISTENTACLER(FLAG:111) == 0
	LOCAL:3 = 0
ELSE
	LOCAL:3 = 1
ENDIF
IF (TFLAG:4 & 질) && LOCAL:3
	;汚れ
	STAIN:3 |= 4
	;사정個所を記録
	TFLAG:5 |= 질
	;질내사정量を記録
	TFLAG:7 += ARG

	IF ARG == 1
		;지문: 촉수질내사정
		CALL MESSAGE_SEX_TENTACLE_SYASEI_VAGINA

		;임신確率 出産後の中だし回数と今までの출산경험に応じ可能性が増える
		CALL NINSIN_HANTEI,ARG,3
		;지문: 세뇌/흑화 캐릭터가 조작 캐릭터의 질내에 사정した
		SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
			CALL MESSAGE_OTHER_SEX_TENTACLE_SYASEI_VAGINA

		;윤활の加算
		LOCAL:0 += 2000
		;굴복の加算
		LOCAL:1 += 1500
		;공순の加算
		LOCAL:4 += 200 * ABL:순종
		;욕정の加算
		LOCAL:5 += 200 * ABL:욕망 + 2000

		EXP:정액경험 += ARG
		LOCAL:2 += ARG
	ELSEIF ARG == 2
		;지문: 촉수질내대량사정
		CALL MESSAGE_SEX_TENTACLE_SYASEI_VAGINA_HI
		;임신確率 通常사정の5倍
		CALL NINSIN_HANTEI,ARG,15
		;지문: 세뇌/흑화 캐릭터가 조작 캐릭터의 질내に대량사정した
		SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
