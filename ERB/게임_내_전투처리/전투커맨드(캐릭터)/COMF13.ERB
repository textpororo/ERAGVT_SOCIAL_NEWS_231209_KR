;봉사한다
@COM13

;거리 표시
CALL PRINT_DISTANCE
PRINTL 

;사정 부위 플래그:손을 세운다
TFLAG:4 = TFLAG:4 | 손
;기교가 2 이상이면 입을 세운다
SIF ABL:기교 >= 2
	TFLAG:4 = TFLAG:4 | 입
;강구속 중에는 사정 부위 플래그를 접는다
SIF (TCVARn:12 & 강구속)
	TFLAG:4 = 0

;방어 플래그：봉사한다로 한다
TCVARn:2 = 자세：봉사한다

;방심도 상승
TFLAG:3 += 100

;사정 기교 의존
IF ABL:기교 < 2
	LOCAL = 70
ELSEIF ABL:기교 == 2
	LOCAL = 100
ELSEIF ABL:기교 == 3
	LOCAL = 140
ELSEIF ABL:기교 == 4
	LOCAL = 190
ELSEIF ABL:기교 >= 5
	LOCAL = 250
ENDIF
CALL TENTACLE_SYASEI_UP, LOCAL

;지문: 봉사한다
CALL MESSAGE_BATTLE_CHARA_HOUSI
;지문: 세뇌/흑화 캐릭터가 조작 캐릭터의「봉사한다」를 봤다
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
	CALL MESSAGE_OTHER_BATTLE_CHARA_HOUSI

;행동 포인트를 +1
EX:행동포인트 += 1

RETURN 1
