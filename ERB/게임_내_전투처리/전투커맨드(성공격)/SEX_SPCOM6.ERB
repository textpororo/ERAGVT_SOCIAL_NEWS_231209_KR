;自慰強制

;性コマンド分類
@SEX_TYPE_COM1006
RETURN 통상

;本家eraのように自慰と強制自慰の区別はいまのところ無し
@SEX_SPCOM6

;取得パラメータ計算
;ベース値の明示的初期化
REPEAT 12
	LOCAL:COUNT = 0
REND

;油断度 상승
TFLAG:3 += 50

;사정値 50
CALL TENTACLE_SYASEI_UP, 50

;촉수サイズの설정
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 1006


;メイン部位絶頂見込みの予測
CALL PALAM_VABCestimate(LOCAL,-1)

;지문: 세뇌/흑화 캐릭터가 操作キャラに強制自慰をさせた
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
	CALL MESSAGE_OTHER_SEX_SPCOM6
	
;지문: SPCOM6
CALL MESSAGE_SEX_SPCOM6

;結界による촉수本数の変動
CALL SET_TENTACLE_POOL

;行動予約なし
TFLAG:17 = -1
;촉수の行動を記録
TFLAG:20 = 1006

;ベースと計算は自慰の専用処理で行うのでここには記述しない
CALL SELF_KIND, TARGET, 0
