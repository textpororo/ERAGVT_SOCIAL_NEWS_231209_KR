;両穴ピストン（ピストン系統からの派生限定行動）

;性コマンド分類
@SEX_TYPE_COM19
RETURN 삽입

@SEX_COM19,ARG = 0,ARG:1 = 0
;EX_COM	追加責め実行部位（ビット）
;	1：Ｃ　2：Ｖ　4：Ａ　8：Ｂ
;追加責め
CALL SEX_COMEX_RANDOM, Ｖ+Ａ, ARG:1
EX_COM = RESULT:0
SH_COM = RESULT:1

INSERT |= 질
INSERT |= 애널
;状態異常：강구속
SIF (TCVARn:12 & 강구속) == 0
	CALL STATE_CHANGE_PKOUSOKU, 100

;흑화 캐릭터のペニス位置をセット
IF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && (ISPENIS(FLAG:111))
	IF RAND:2 == 0 || TCVARn:2 == 자세：Ｖ방어
		TFLAG:18 = 질
	ELSE
		TFLAG:18 = 애널
	ENDIF
ENDIF

;取得パラメータ計算
;ベース値の明示的초기화
REPEAT 12
	LOCAL:COUNT = 0
REND

;油断度 상승
TFLAG:3 += 8

;사정値 600
CALL TENTACLE_SYASEI_UP, 600

;촉수サイズの설정
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 19


;快V
IF ABL:V감각 == 0
	LOCAL:1 = 30
ELSEIF ABL:V감각 == 1
	LOCAL:1 = 75
ELSEIF ABL:V감각 == 2
	LOCAL:1 = 150
ELSEIF ABL:V감각 == 3
	LOCAL:1 = 300
ELSEIF ABL:V감각 == 4
	LOCAL:1 = 750
ELSEIF ABL:V감각 >= 5
	LOCAL:1 = 1500
ENDIF

;快A　감각が低いうちは少なめ
IF CONFIG_CHECK_BALANCE_F(7) == 0
	IF ABL:A감각 == 0
		LOCAL:2 = 6
	ELSEIF ABL:A감각 == 1
		LOCAL:2 = 35
	ELSEIF ABL:A감각 == 2
		LOCAL:2 = 150
	ELSEIF ABL:A감각 == 3
		LOCAL:2 = 300
	ELSEIF ABL:A감각 == 4
		LOCAL:2 = 750
	ELSEIF ABL:A감각 >= 5
		LOCAL:2 = 1500
	ENDIF
;컨피그オンで쾌A増加
ELSEIF CONFIG_CHECK_BALANCE_F(7) == 0
	IF ABL:A감각 == 0
		LOCAL:2 = 30
	ELSEIF ABL:A감각 == 1
		LOCAL:2 = 75
	ELSEIF ABL:A감각 == 2
		LOCAL:2 = 150
	ELSEIF ABL:A감각 == 3
		LOCAL:2 = 300
	ELSEIF ABL:A감각 == 4
		LOCAL:2 = 750
	ELSEIF ABL:A감각 >= 5
		LOCAL:2 = 1500
	ENDIF
ENDIF

;メイン部位絶頂見込みの予測
CALL PALAM_VABCestimate(LOCAL,1,2,-1)



IF ENEMY_TYPE_CHECK_F("MOB") == 1
	TRYCALLFORM MESSAGE_MOB_{ARG}_COM19, EX_COM, SH_COM
	;처녀なら고통が大量に入る
	IF TALENT:처녀 > 0 && TCVARn:2 != 자세：Ｖ방어 && BASE:V결계내구력 <= 0
		LOCAL:10 = 10000
		TALENT:처녀 = -1
		CFLAG:206 = 2
		TCVARn:1 = 1
	ELSE
		LOCAL:10 = 0
	ENDIF
ELSE
	;처녀なら고통が大量に入る
	IF TALENT:처녀 > 0 && TCVARn:2 != 자세：Ｖ방어 && BASE:V결계내구력 <= 0
		LOCAL:10 = 10000
		CALL LOSTVIRGIN
	ELSE
		LOCAL:10 = 0
	ENDIF
	;지문: 세뇌/흑화 캐릭터가 操作キャラに両穴ピストンをした
	SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
		CALL MESSAGE_OTHER_SEX_COM19
	;지문: COM17
	CALL MESSAGE_SEX_COM19, EX_COM, SH_COM
ENDIF

;結界による촉수本数の変動
CALL SET_TENTACLE_POOL
;服へのダメージ
CALL CLOTH_BATTLE_DAMAGE, 3

;ベース
EXP:V경험 += 1
EXP:A경험 += 1
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 0 && INCEST_F(TARGET, FLAG:10 * 100 + FLAG:11, 1) > 0
	EXP:근친교배경험 += 1
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && INCEST_F(TARGET, FLAG:111) > 0
	EXP:근친교배경험 += 1



;공순
IF ABL:순종 <= 2
	LOCAL:5 = 30
ELSEIF ABL:순종 == 3
	LOCAL:5 = 75
ELSEIF ABL:순종 == 4
	LOCAL:5 = 150
ELSEIF ABL:순종 >= 5
	LOCAL:5 = 300
ENDIF

;습득
IF ABL:봉사정신 == 0
	LOCAL:6 = 15
ELSEIF ABL:봉사정신 == 1
	LOCAL:6 = 35
ELSEIF ABL:봉사정신 == 2
	LOCAL:6 = 70
ELSEIF ABL:봉사정신 == 3
	LOCAL:6 = 135
ELSEIF ABL:봉사정신 == 4
	LOCAL:6 = 300
ELSEIF ABL:봉사정신 >= 5
	LOCAL:6 = 600
ENDIF

;굴복　A감각に応じて
IF ABL:A감각 == 0
	LOCAL:8 = 20
ELSEIF ABL:A감각 == 1
	LOCAL:8 = 40
ELSEIF ABL:A감각 == 2
	LOCAL:8 = 100
ELSEIF ABL:A감각 == 3
	LOCAL:8 = 200
ELSEIF ABL:A감각 == 4
	LOCAL:8 = 400
ELSEIF ABL:A감각 >= 5
	LOCAL:8 = 900 + ABL:A감각 * 20
ENDIF

;윤활が低いと고통が入る
IF PALAM:윤활 < 1000
	LOCAL:10 += 1000
ELSEIF PALAM:윤활 < 2000
	LOCAL:10 += 200
ENDIF

;V감각が低いと공포が入る
IF ABL:V감각 == 0
	LOCAL:11 = 300
ELSEIF ABL:V감각 == 1
	LOCAL:11 = 75
ELSEIF ABL:V감각 == 2
	LOCAL:11 = 30
ELSEIF ABL:V감각 == 3
	LOCAL:11 = 15
ELSE
ENDIF
;A감각が低いと공포が入る
IF ABL:A감각 == 0
	LOCAL:11 = 150
ELSEIF ABL:A감각 == 1
	LOCAL:11 = 75
ELSEIF ABL:A감각 == 2
	LOCAL:11 = 35
ELSEIF ABL:A감각 == 3
	LOCAL:11 = 15
ELSE
ENDIF

;追加責め実行
IF EX_COM
	CALL SEX_COMEX, Ｖ+Ａ, 1, EX_COM | SH_COM
	REPEAT 12
		LOCAL:COUNT += RESULT:COUNT
	REND
ENDIF

;대량사정可能ならフィニッシュに移行
IF FLAG:15 >= FLAG:14 * 2
	TFLAG:17 = 20
;それ以外なら両穴ピストン続行
ELSE
	TFLAG:17 = 19
ENDIF
;촉수の行動を記録
TFLAG:20 = 19

;습득基本値
LOCAL:6 += 350

;욕정基本値
LOCAL:7 += 200

;굴복基本値
LOCAL:8 += 500

;치정基本値
LOCAL:9 += 300

;고통基本値
LOCAL:10 += 150

;공포基本値
LOCAL:11 += 200

;체력消費基本値
LOCAL:12 += 5

;計算へ
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12
