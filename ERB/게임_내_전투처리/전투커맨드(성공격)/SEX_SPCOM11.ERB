;甘えん坊授乳プレイ

;性コマンド分類
@SEX_TYPE_COM1011
RETURN 삽입

@SEX_SPCOM11
;EX_COM	追加責め実行部位（ビット）
;	1：Ｃ　2：Ｖ　4：Ａ　8：Ｂ
;追加責め
EX_COM = 10
SH_COM = 0

;사정部位フラグ:膣を立てる
TFLAG:4 = TFLAG:4 | 질
INSERT = INSERT | 질

;取得パラメータ計算
;ベース値の明示的初期化
REPEAT 12
	LOCAL:COUNT = 0
REND

;油断度 상승
TFLAG:3 += 100

;사정値 800
CALL TENTACLE_SYASEI_UP, 800

;촉수サイズの設定
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 1011


;快V
IF ABL:V감각 == 0
	LOCAL:1 = 2000
ELSEIF ABL:V감각 == 1
	LOCAL:1 = 4000
ELSEIF ABL:V감각 == 2
	LOCAL:1 = 8000
ELSEIF ABL:V감각 == 3
	LOCAL:1 = 10000
ELSEIF ABL:V감각 == 4
	LOCAL:1 = 20000
ELSEIF ABL:V감각 >= 5
	LOCAL:1 = 40000
ENDIF

;快B
IF ABL:B감각 == 0
	LOCAL:3 = 3000
ELSEIF ABL:B감각 == 1
	LOCAL:3 = 4500
ELSEIF ABL:B감각 == 2
	LOCAL:3 = 6000
ELSEIF ABL:B감각 == 3
	LOCAL:3 = 8000
ELSEIF ABL:B감각 == 4
	LOCAL:3 = 16000
ELSEIF ABL:B감각 >= 5
	LOCAL:3 = 20000
ENDIF

;メイン部位絶頂見込みの予測
CALL PALAM_VABCestimate(LOCAL,1,3,-1)


;지문: SPCOM11
CALL MESSAGE_SEX_SPCOM11, EX_COM, SH_COM

;처녀なら고통が大量に入る
IF TALENT:처녀 == 1 && CHECK_HOLYVIRGIN_F()==0
	LOCAL:10 = 10000
	TALENT:처녀 = -1
	CFLAG:206 = 3
	TCVARn:1 = 1
ELSE
	LOCAL:10 = 0
ENDIF

;結界による촉수本数の変動
CALL SET_TENTACLE_POOL
;服へのダメージ
CALL CLOTH_BATTLE_DAMAGE, 1

;ベース
EXP:V경험 += 5
EXP:봉사쾌락경험 += 1
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 0 && INCEST_F(TARGET, FLAG:10 * 100 + FLAG:11, 1) > 0
	EXP:근친교배경험 += 1
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && INCEST_F(TARGET, FLAG:111) > 0
	EXP:근친교배경험 += 1



;모유체질なら確実に모유する
SIF TALENT:모유체질
	BASE:모유 += MAXBASE:모유

;공순
IF ABL:순종 == 2
	LOCAL:5 = 200
ELSEIF ABL:순종 == 3
	LOCAL:5 = 500
ELSEIF ABL:순종 == 4
	LOCAL:5 = 1000
ELSEIF ABL:순종 >= 5
	LOCAL:5 = 2000
ENDIF
LOCAL:5 += MIN(EXP:출산경험 * 100, 2000)

;습득
IF ABL:봉사정신 == 0
	LOCAL:6 = 10
ELSEIF ABL:봉사정신 == 1
	LOCAL:6 = 20
ELSEIF ABL:봉사정신 == 2
	LOCAL:6 = 50
ELSEIF ABL:봉사정신 == 3
	LOCAL:6 = 100
ELSEIF ABL:봉사정신 == 4
	LOCAL:6 = 200
ELSEIF ABL:봉사정신 >= 5
	LOCAL:6 = 500
ENDIF

;윤활が低いと고통が入る
SIF PALAM:윤활 < 2000
	LOCAL:10 += 1000

;V감각が低いと공포が入る
IF ABL:V감각 == 0
	LOCAL:11 = 2000
ELSEIF ABL:V감각 == 1
	LOCAL:11 = 1000
ELSEIF ABL:V감각 == 2
	LOCAL:11 = 500
ELSEIF ABL:V감각 == 3
	LOCAL:11 = 200
ELSEIF ABL:V감각 == 4
	LOCAL:11 = 100
ELSE
ENDIF

;성내성が残り50%以上なら派生しにくい
IF PERCENT_CAL_F(BASE:성내성, MAXBASE:성내성) >= 50 && RAND:2 == 0
;ＶＡ挿入成功＆사정 게이지不足なら両穴ピストン＋강구속状態
ELSEIF (EX_COM & Ｖ) && (EX_COM & Ａ) && FLAG:15 < FLAG:14 && BASE:V결계내구력 == 0 && BASE:A결계내구력 == 0 && TCVARn:2 != 자세：Ｖ방어 && (ENEMY_TYPE_CHECK_F("MOB") == 0 && ENEMY_TYPE_CHECK_F("CITIZEN") == 0) && ISFEMALE()
	TFLAG:17 = 19
	;状態異常：강구속
	CALL STATE_CHANGE_PKOUSOKU, 100
;Ｖ挿入成功＆사정 게이지不足なら種付けピストン＋강구속状態
ELSEIF FLAG:15 < FLAG:14 && BASE:V결계내구력 == 0 && TCVARn:2 != 자세：Ｖ방어 && (ENEMY_TYPE_CHECK_F("MOB") == 0 && ENEMY_TYPE_CHECK_F("CITIZEN") == 0) && ISFEMALE() && TALENT:처녀 < 1
	TFLAG:17 = 15
	;状態異常：강구속
	CALL STATE_CHANGE_PKOUSOKU, 100
ELSE
	;行動予約なし
	TFLAG:17 = -1
ENDIF
;촉수の行動を記録
TFLAG:20 = 1011

;습득基本値
LOCAL:6 += 2000

;욕정基本値
LOCAL:7 += 500

;굴복基本値
LOCAL:8 += 2000

;치정基本値
LOCAL:9 += 2000

;고통基本値
LOCAL:10 += 0

;공포基本値
LOCAL:11 += 4000

;체력消費基本値
LOCAL:12 += 200

;計算へ
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12

