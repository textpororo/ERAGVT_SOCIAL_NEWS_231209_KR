;針

;性コマンド分類
@SEX_TYPE_COM9
RETURN 통상

@SEX_COM9,ARG = 0,ARG:1 = 0
;追加責め
EX_COM = 0
SH_COM = 0

;取得パラメータ計算
;ベース値の明示的초기화
REPEAT 12
	LOCAL:COUNT = 0
REND

;油断度 상승
TFLAG:3 += 25

;사정値 25
CALL TENTACLE_SYASEI_UP, 25

;촉수サイズの설정
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 9

;メイン部位絶頂見込みの予測
CALL PALAM_VABCestimate(LOCAL,-1)


IF ENEMY_TYPE_CHECK_F("MOB") == 1
	TRYCALLFORM MESSAGE_MOB_{ARG}_COM9, EX_COM, SH_COM
ELSE
	;지문: 세뇌/흑화 캐릭터가 操作キャラに針をした
	SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
		CALL MESSAGE_OTHER_SEX_COM9
	;지문: COM9
	CALL MESSAGE_SEX_COM9
ENDIF

;結界による촉수本数の変動
CALL SET_TENTACLE_POOL
;服へのダメージ
CALL CLOTH_BATTLE_DAMAGE, 10

;ベース
SIF ABL:마조끼
	EXP:고통쾌락경험 += 1

;굴복　마조끼で変化
IF ABL:마조끼 == 0
	LOCAL:8 = 200
	LOCAL:11 = 10000
ELSEIF ABL:마조끼 == 1
	LOCAL:8 = 400
	LOCAL:11 = 4000
ELSEIF ABL:마조끼 == 2
	LOCAL:8 = 1000
	LOCAL:11 = 2000
ELSEIF ABL:마조끼 == 3
	LOCAL:8 = 2000
	LOCAL:11 = 1000
ELSEIF ABL:마조끼 == 4
	LOCAL:8 = 4000
	LOCAL:11 = 400
ELSEIF ABL:마조끼 >= 5
	LOCAL:11 = 200
	LOCAL:8 = 10000
ENDIF

;공포　순종と마조끼で大まかに
IF ABL:순종 < 3
	IF ABL:마조끼 < 3
		LOCAL:11 = 5000
	ELSE
		LOCAL:11 = 500
	ENDIF
ELSE
	IF ABL:마조끼 < 3
		LOCAL:11 = 500
	ELSE
		LOCAL:11 = 50
	ENDIF
ENDIF

;行動予約なし
TFLAG:17 = -1
;촉수の行動を記録
TFLAG:20 = 9

;습득基本値
LOCAL:6 += 0

;욕정基本値
LOCAL:7 += 0

;굴복基本値
LOCAL:8 += 500

;치정基本値
LOCAL:9 += 0

;고통基本値
LOCAL:10 += 5000

;공포基本値
LOCAL:11 += 2500

;체력消費基本値
LOCAL:12 += 300

;計算へ
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12
