;A拡張攻め

;性コマンド分類
@SEX_TYPE_COM1010
RETURN 삽입

@SEX_SPCOM10
;EX_COM	追加責め実行部位（ビット）
;	1：Ｃ　2：Ｖ　4：Ａ　8：Ｂ
;追加責め
EX_COM = 4
SH_COM = 0

;사정部位フラグ:애널を立てる
TFLAG:4 = TFLAG:4 | 애널
INSERT = INSERT | 애널

;取得パラメータ計算
;ベース値の明示的初期化
REPEAT 12
	LOCAL:COUNT = 0
REND

;油断度 상승
TFLAG:3 += 100

;사정値 800
CALL TENTACLE_SYASEI_UP, 800

;我慢：대량사정でなければ사정部位플래그를折る
SIF FLAG:15 < FLAG:14 * 2
	TFLAG:4 = 0

;촉수サイズの設定
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 1010

;快A
IF ABL:A감각 == 0
	LOCAL:2 = 1000
ELSEIF ABL:A감각 == 1
	LOCAL:2 = 2000
ELSEIF ABL:A감각 == 2
	LOCAL:2 = 4000
ELSEIF ABL:A감각 == 3
	LOCAL:2 = 10000
ELSEIF ABL:A감각 == 4
	LOCAL:2 = 20000
ELSEIF ABL:A감각 >= 5
	LOCAL:2 = 40000
ENDIF
;拡張経験が少ない場合は快Aが入りにくい
IF EXP:A확장경험 < 3
	TIMES LOCAL:2, 0.10
ELSEIF EXP:A확장경험 < 5
	TIMES LOCAL:2, 0.50
ELSEIF EXP:A확장경험 < 8
	TIMES LOCAL:2, 0.80
ELSE
ENDIF

;メイン部位絶頂見込みの予測
CALL PALAM_VABCestimate(LOCAL,2,-1)


;지문: SPCOM10
CALL MESSAGE_SEX_SPCOM10, EX_COM, SH_COM

;結界による촉수本数の変動
CALL SET_TENTACLE_POOL
;服へのダメージ
CALL CLOTH_BATTLE_DAMAGE, 2

;ベース
EXP:A경험 += 5
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 0 && INCEST_F(TARGET, FLAG:10 * 100 + FLAG:11, 1) > 0
	EXP:근친교배경험 += 1
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && INCEST_F(TARGET, FLAG:111) > 0
	EXP:근친교배경험 += 1




;굴복
;拡張経験に応じて
IF EXP:A확장경험 < 3
	LOCAL:8 = 2000
ELSEIF EXP:A확장경험 < 5
	LOCAL:8 = 4000
ELSEIF EXP:A확장경험 < 8
	LOCAL:8 = 8000
ELSE
	LOCAL:8 = 20000
ENDIF

;치정
;拡張経験が高いと치정が入る
IF EXP:A확장경험 < 3
	LOCAL:9 = 200
ELSEIF EXP:A확장경험 < 5
	LOCAL:9 = 400
ELSEIF EXP:A확장경험 < 8
	LOCAL:9 = 800
ELSE
	LOCAL:9 = 2000
ENDIF

;고통
;拡張経験が低いと入る
IF EXP:A확장경험 < 3
	LOCAL:10 += 4000
ELSEIF EXP:A확장경험 < 5
	LOCAL:10 += 3200
ELSEIF EXP:A확장경험 < 8
	LOCAL:10 += 1600
ELSE
	LOCAL:10 += 400
ENDIF

;공포
;拡張経験が低いと入る
IF EXP:A확장경험 < 3
	LOCAL:11 = 8000
ELSEIF EXP:A확장경험 < 5
	LOCAL:11 = 6400
ELSEIF EXP:A확장경험 < 8
	LOCAL:11 = 3200
ELSE
	LOCAL:11 = 800
ENDIF

;습득
IF ABL:봉사정신 == 0
	LOCAL:6 = 10
ELSEIF ABL:봉사정신 == 1
	LOCAL:6 = 20
ELSEIF ABL:봉사정신 == 2
	LOCAL:6 = 50
ELSEIF ABL:봉사정신 == 3
	LOCAL:6 = 100
ELSEIF ABL:봉사정신 == 4
	LOCAL:6 = 200
ELSEIF ABL:봉사정신 >= 5
	LOCAL:6 = 500
ENDIF

;윤활が低いと고통が入る
SIF PALAM:윤활 < 2000
	LOCAL:10 += 2000

;行動予約なし
TFLAG:17 = -1
;촉수の行動を記録
TFLAG:20 = 1010

;습득基本値
LOCAL:6 += 2000

;욕정基本値
LOCAL:7 += 0

;굴복基本値
LOCAL:8 += 4000

;치정基本値
LOCAL:9 += 500

;고통基本値
LOCAL:10 += 2000

;공포基本値
LOCAL:11 += 1000

;체력消費基本値
LOCAL:12 += 50

;計算へ
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12

