;両穴フィニッシュ（ＶＡ系からの派生限定行動）

;性コマンド分類
@SEX_TYPE_COM20
RETURN 삽입

@SEX_COM20,ARG = 0,ARG:1 = 0
;EX_COM	追加責め実行部位（ビット）
;	1：Ｃ　2：Ｖ　4：Ａ　8：Ｂ
;追加責め
CALL SEX_COMEX_RANDOM, Ｖ+Ａ, ARG:1
EX_COM = RESULT:0
SH_COM = RESULT:1

;사정部位フラグ:膣と애널を立てる
TFLAG:4 = TFLAG:4 | 질
TFLAG:4 = TFLAG:4 | 애널
INSERT = INSERT | 질
INSERT = INSERT | 애널
;촉수を使っているなら手と口を立てる
IF (FLAG:110 == 1) && ISTENTACLER(FLAG:111) == 0 && (TALENT:(FLAG:111):후타나리 > 0 || ISMALE(FLAG:111))
ELSE
	TFLAG:4 = TFLAG:4 | 손
	TFLAG:4 = TFLAG:4 | 입
ENDIF
;Ｂ追加攻めがあるなら胸を立てる
SIF (EX_COM & Ｂ)
	TFLAG:4 = TFLAG:4 | 가슴

;흑화 캐릭터のペニス位置をセット
IF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && (ISPENIS(FLAG:111))
	IF RAND:2 == 0
		TFLAG:18 = 질
	ELSE
		TFLAG:18 = 애널
	ENDIF
ENDIF

;取得パラメータ計算
;ベース値の明示的初期化
REPEAT 12
	LOCAL:COUNT = 0
REND

;油断度の上昇
TFLAG:3 += 300

;사정値 1200
CALL TENTACLE_SYASEI_UP, 1200

;촉수サイズの設定
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 20


;快V
IF ABL:V감각 == 0
	LOCAL:1 = 3450
ELSEIF ABL:V감각 == 1
	LOCAL:1 = 4125
ELSEIF ABL:V감각 == 2
	LOCAL:1 = 6000
ELSEIF ABL:V감각 == 3
	LOCAL:1 = 8250
ELSEIF ABL:V감각 == 4
	LOCAL:1 = 10500
ELSEIF ABL:V감각 >= 5
	LOCAL:1 = 15000
ENDIF

;快A
IF CONFIG_CHECK_BALANCE_F(7) == 0
	IF ABL:A감각 == 0
		LOCAL:2 = 690
	ELSEIF ABL:A감각 == 1
		LOCAL:2 = 2065
	ELSEIF ABL:A감각 == 2
		LOCAL:2 = 6000
	ELSEIF ABL:A감각 == 3
		LOCAL:2 = 8250
	ELSEIF ABL:A감각 == 4
		LOCAL:2 = 10500
	ELSEIF ABL:A감각 >= 5
		LOCAL:2 = 15000
	ENDIF
;컨피그オンで쾌A増加
ELSEIF CONFIG_CHECK_BALANCE_F(7) > 0
	IF ABL:A감각 == 0
		LOCAL:2 = 3450
	ELSEIF ABL:A감각 == 1
		LOCAL:2 = 4125
	ELSEIF ABL:A감각 == 2
		LOCAL:2 = 6000
	ELSEIF ABL:A감각 == 3
		LOCAL:2 = 8250
	ELSEIF ABL:A감각 == 4
		LOCAL:2 = 10500
	ELSEIF ABL:A감각 >= 5
		LOCAL:2 = 15000
	ENDIF
ENDIF
;メイン部位絶頂見込みの予測
CALL PALAM_VABCestimate(LOCAL,1,2,-1)



IF ENEMY_TYPE_CHECK_F("MOB") == 1
	TRYCALLFORM MESSAGE_MOB_{ARG}_COM20, EX_COM, SH_COM
ELSE
	;지문: 세뇌/흑화 캐릭터가 操作キャラに両穴フィニッシュをした
	SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
		CALL MESSAGE_OTHER_SEX_COM20
	;지문: COM20
	CALL MESSAGE_SEX_COM20, EX_COM, SH_COM
ENDIF

;結界による촉수本数の変動
CALL SET_TENTACLE_POOL
;服へのダメージ
CALL CLOTH_BATTLE_DAMAGE, 3

;ベース
EXP:V경험 += 1
EXP:A경험 += 1
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 0 && INCEST_F(TARGET, FLAG:10 * 100 + FLAG:11, 1) > 0
	EXP:근친교배경험 += 1
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && INCEST_F(TARGET, FLAG:111) > 0
	EXP:근친교배경험 += 1


;공순
IF ABL:순종 <= 2
	LOCAL:5 = 400
ELSEIF ABL:순종 == 3
	LOCAL:5 = 1000
ELSEIF ABL:순종 == 4
	LOCAL:5 = 2000
ELSEIF ABL:순종 >= 5
	LOCAL:5 = 4000
ENDIF

;습득
IF ABL:봉사정신 == 0
	LOCAL:6 = 15
ELSEIF ABL:봉사정신 == 1
	LOCAL:6 = 35
ELSEIF ABL:봉사정신 == 2
	LOCAL:6 = 80
ELSEIF ABL:봉사정신 == 3
	LOCAL:6 = 135
ELSEIF ABL:봉사정신 == 4
	LOCAL:6 = 300
ELSEIF ABL:봉사정신 >= 5
	LOCAL:6 = 600
ENDIF

;굴복　A감각に応じて
IF ABL:A감각 == 0
	LOCAL:8 = 15
ELSEIF ABL:A감각 == 1
	LOCAL:8 = 30
ELSEIF ABL:A감각 == 2
	LOCAL:8 = 75
ELSEIF ABL:A감각 == 3
	LOCAL:8 = 150
ELSEIF ABL:A감각 == 4
	LOCAL:8 = 300
ELSEIF ABL:A감각 >= 5
	LOCAL:8 = 650 + ABL:A감각 * 15
ENDIF

;윤활が低いと고통が入る
IF PALAM:윤활 < 1000
	LOCAL:10 += 1500
ELSEIF PALAM:윤활 < 2000
	LOCAL:10 += 300
ENDIF

;V感覚が低いと공포が入る
IF ABL:V감각 == 0
	LOCAL:11 = 300
ELSEIF ABL:V감각 == 1
	LOCAL:11 = 75
ELSEIF ABL:V감각 == 2
	LOCAL:11 = 30
ELSEIF ABL:V감각 == 3
	LOCAL:11 = 15
ELSE
ENDIF
;A感覚が低いと공포が入る
IF ABL:A감각 == 0
	LOCAL:11 = 150
ELSEIF ABL:A감각 == 1
	LOCAL:11 = 75
ELSEIF ABL:A감각 == 2
	LOCAL:11 = 35
ELSEIF ABL:A감각 == 3
	LOCAL:11 = 15
ELSE
ENDIF

;追加責め実行
IF EX_COM
	CALL SEX_COMEX, Ｖ+Ａ, 1, EX_COM | SH_COM
	REPEAT 12
		LOCAL:COUNT += RESULT:COUNT
	REND
ENDIF

;行動予約なし
TFLAG:17 = -1
;촉수の行動を記録
TFLAG:20 = 20

;강구속状態を解除
SIF (TCVARn:12 & 강구속)
	TCVARn:12 -= 강구속

;습득基本値
LOCAL:6 += 100

;욕정基本値
LOCAL:7 += 2000

;굴복基本値
LOCAL:8 += 8000

;치정基本値
LOCAL:9 += 2000

;고통基本値
LOCAL:10 += 0

;공포基本値
LOCAL:11 += 3000

;체력消費基本値
LOCAL:12 += 1000

;計算へ
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12
