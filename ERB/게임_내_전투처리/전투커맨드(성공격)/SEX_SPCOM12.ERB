;ヘソ快楽攻め

;性コマンド分類
@SEX_TYPE_COM1012
RETURN 통상

@SEX_SPCOM12
;取得パラメータ計算
;ベース値の明示的初期化
REPEAT 12
	LOCAL:COUNT = 0
REND

;油断度の上昇
TFLAG:3 += 100

;사정値 100
CALL TENTACLE_SYASEI_UP, 100

;촉수サイズの設定
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 1012

;メイン部位絶頂見込みの予測
CALL PALAM_VABCestimate(LOCAL,-1)


;지문: SPCOM12
CALL MESSAGE_SEX_SPCOM12

;結界による촉수本数の変動
CALL SET_TENTACLE_POOL
;服へのダメージ
CALL CLOTH_BATTLE_DAMAGE, 5

;ベース
;굴복　마조끼で変化
IF ABL:마조끼 == 0
	LOCAL:8 = 2000
ELSEIF ABL:마조끼 == 1
	LOCAL:8 = 4000
ELSEIF ABL:마조끼 == 2
	LOCAL:8 = 6000
ELSEIF ABL:마조끼 == 3
	LOCAL:8 = 8000
ELSEIF ABL:마조끼 == 4
	LOCAL:8 = 16000
ELSEIF ABL:마조끼 >= 5
	LOCAL:8 = 24000
ENDIF

;공포　순종と마조끼で大まかに
IF ABL:순종 < 4
	IF ABL:마조끼 < 4
		LOCAL:11 = 20000
	ELSE
		LOCAL:11 = 2000
	ENDIF
ELSE
	IF ABL:마조끼 < 4
		LOCAL:11 = 2000
	ELSE
		LOCAL:11 = 200
	ENDIF
ENDIF

;行動予約なし
TFLAG:17 = -1
;촉수の行動を記録
TFLAG:20 = 1012

;습득基本値
LOCAL:6 += 0

;욕정基本値
LOCAL:7 += 10000

;굴복基本値
LOCAL:8 += 5000

;치정基本値
LOCAL:9 += 0

;고통基本値
LOCAL:10 += 0

;공포基本値
LOCAL:11 += 2000

;체력消費基本値
LOCAL:12 += 1050

;計算へ
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12
