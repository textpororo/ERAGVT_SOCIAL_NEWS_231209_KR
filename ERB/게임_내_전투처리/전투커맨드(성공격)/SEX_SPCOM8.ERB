;尿道攻め

;性コマンド分類
@SEX_TYPE_COM1008
RETURN 통상

@SEX_SPCOM8
;EX_COM	追加責め実行部位（ビット）
;	1：Ｃ　2：Ｖ　4：Ａ　8：Ｂ
;追加責め
EX_COM = 1
SH_COM = 0

;取得パラメータ計算
;ベース値の明示的초기화
REPEAT 12
	LOCAL:COUNT = 0
REND

;油断度 상승
TFLAG:3 += 50

;사정値 50
CALL TENTACLE_SYASEI_UP, 50

;촉수サイズの설정
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 1008


;快C
IF ABL:C감각 == 0
	LOCAL:0 = 2000
ELSEIF ABL:C감각 == 1
	LOCAL:0 = 4000
ELSEIF ABL:C감각 == 2
	LOCAL:0 = 8000
ELSEIF ABL:C감각 == 3
	LOCAL:0 = 10000
ELSEIF ABL:C감각 == 4
	LOCAL:0 = 20000
ELSEIF ABL:C감각 >= 5
	LOCAL:0 = 40000
ENDIF
;ペニス持ちには効果大
SIF ISMANLY() || TALENT:후타나리 > 0
	LOCAL:0 += 1500

;メイン部位絶頂見込みの予測
CALL PALAM_VABCestimate(LOCAL,0,-1)


;지문: SPCOM8
CALL MESSAGE_SEX_SPCOM8, EX_COM, SH_COM

;結界による촉수本数の変動
CALL SET_TENTACLE_POOL
;服へのダメージ
CALL CLOTH_BATTLE_DAMAGE, 1

;ベース
EXP:방뇨경험 += 1
EXP:노출쾌락경험 += 1



;굴복
;노출벽に応じて
IF ABL:노출벽 < 3
	LOCAL:8 = 2000
ELSEIF ABL:노출벽 == 3
	LOCAL:8 = 4000
ELSEIF ABL:노출벽 == 4
	LOCAL:8 = 8000
ELSE
	LOCAL:8 = 20000
ENDIF

;치정
;노출벽に応じて
IF ABL:노출벽 < 3
	LOCAL:9 = 2000
ELSEIF ABL:노출벽 == 3
	LOCAL:9 = 800
ELSEIF ABL:노출벽 == 4
	LOCAL:9 = 400
ELSE
	LOCAL:9 = 200
ENDIF

;공포
;방뇨경험이 低いと入る
IF EXP:방뇨경험 == 0
	LOCAL:11 = 8000
ELSEIF EXP:방뇨경험 == 1
	LOCAL:11 = 6400
ELSEIF EXP:방뇨경험 == 2
	LOCAL:11 = 3200
ELSE
	LOCAL:11 = 800
ENDIF

;行動予約なし
TFLAG:17 = -1
;촉수の行動を記録
TFLAG:20 = 1008

;습득基本値
LOCAL:6 += 500

;욕정基本値
LOCAL:7 += 0

;굴복基本値
LOCAL:8 += 2000

;치정基本値
LOCAL:9 += 400

;고통基本値
LOCAL:10 += 500

;공포基本値
LOCAL:11 += 4000

;체력消費基本値
LOCAL:12 += 50

;計算へ
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12

