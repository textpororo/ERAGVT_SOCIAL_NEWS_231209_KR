;Ｃ공격Ａ공격（ＳＰ）

;성 커맨드 분류
@SEX_TYPE_COM1000
RETURN 통상

@SEX_SPCOM0
;EX_COM	추가 고문 실행 부위（비트）
;	1：Ｃ　2：Ｖ　4：Ａ　8：Ｂ
;추가 고문
CALL SEX_COMEX_RANDOM, Ｃ
EX_COM = RESULT:0
SH_COM = RESULT:1

;취득 파라미터 계산
;BASE값의 명시적 초기화
REPEAT 12
	LOCAL:COUNT = 0
REND

;방심도 상승
TFLAG:3 += 100

;사정치 100
CALL TENTACLE_SYASEI_UP, 100

;촉수 크기 설정
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 1000


;쾌Ｃ
IF ABL:Ｃ감각 == 0
	LOCAL:0 = 2000
ELSEIF ABL:Ｃ감각 == 1
	LOCAL:0 = 4000
ELSEIF ABL:Ｃ감각 == 2
	LOCAL:0 = 10000
ELSEIF ABL:Ｃ감각 == 3
	LOCAL:0 = 20000
ELSEIF ABL:Ｃ감각 == 4
	LOCAL:0 = 40000
ELSEIF ABL:Ｃ감각 >= 5
	LOCAL:0 = 100000
ENDIF
;페니스가 있다면 효과가 크다
SIF ISMANLY() || TALENT:후타나리 > 0
	LOCAL:0 += 1500

;추가 고문 실행
IF EX_COM
	CALL SEX_COMEX, Ｃ, 2, EX_COM | SH_COM
	REPEAT 12
		LOCAL:COUNT += RESULT:COUNT
	REND
ENDIF

;메인 부위 절정 예정의 예측
CALL PALAM_VABCestimate(LOCAL,0,-1)


;지문: 세뇌/흑화 캐릭터가 조작 캐릭터에게 Ｃ공격Ａ공격（ＳＰ）을 했다
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
	CALL MESSAGE_OTHER_SEX_SPCOM0

;지문: SPCOM0
CALL MESSAGE_SEX_SPCOM0, EX_COM, SH_COM

;결계에 따른 촉수 개수의 변동
CALL SET_TENTACLE_POOL
;의상에의 피해
CALL CLOTH_BATTLE_DAMAGE, 8

;BASE
EXP:고통쾌락경험 += 1



;Ｃ감각이 낮으면 고통과 공포가 들어간다
IF ABL:Ｃ감각 < 4
	LOCAL:10 = 1000
	LOCAL:11 = 1000
ENDIF


;행동 예약 없음
TFLAG:17 = -1
;촉수의 행동을 기록
TFLAG:20 = 1000

;습득 기본값
LOCAL:6 += 500

;욕정 기본값
LOCAL:7 += 0

;굴복 기본값
LOCAL:8 += 2000

;치정 기본값
LOCAL:9 += 0

;고통 기본값
LOCAL:10 += 500

;공포 기본값
LOCAL:11 += 1000

;체력 소비 기본값
LOCAL:12 += 200

;계산으로
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12
