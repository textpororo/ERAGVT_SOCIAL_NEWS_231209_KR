;手淫 弄るのと同時に弄られる

;性コマンド分類
@SEX_TYPE_COM10
RETURN 통상

@SEX_COM10,ARG = 0,ARG:1 = 0
;EX_COM	追加責め実行部位（ビット）
;	1：Ｃ　2：Ｖ　4：Ａ　8：Ｂ
;追加責め（特定部位なし）
CALL SEX_COMEX_RANDOM, 0, ARG:1
EX_COM = RESULT:0
SH_COM = RESULT:1

;사정部位フラグ:手を立てる
TFLAG:4 = TFLAG:4 | 손
;我慢：確率で사정部位플래그를折る
SIF RAND:100 < 75
	TFLAG:4 = 0

;悪堕ちキャラのペニス位置をセット
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && (ISPENIS(FLAG:111))
	TFLAG:18 = 손

;取得パラメータ計算
;ベース値の明示的初期化
REPEAT 12
	LOCAL:COUNT = 0
REND

;油断度の上昇
TFLAG:3 += 25

;사정値 150
CALL TENTACLE_SYASEI_UP, 150

;촉수サイズの設定
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 10

;メイン部位絶頂見込みの予測
CALL PALAM_VABCestimate(LOCAL,-1)


IF ENEMY_TYPE_CHECK_F("MOB") == 1
	TRYCALLFORM MESSAGE_MOB_{ARG}_COM10, EX_COM, SH_COM
ELSE
	;지문: 洗脳/悪堕ちキャラが操作キャラに手淫をさせた
	SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
		CALL MESSAGE_OTHER_SEX_COM10
	;지문: COM10
	CALL MESSAGE_SEX_COM10, EX_COM, SH_COM
ENDIF

;結界による촉수本数の変動
CALL SET_TENTACLE_POOL
;服へのダメージ
CALL CLOTH_BATTLE_DAMAGE, 5

;ベース
EXP:봉사쾌락경험 += 1

;공순
IF ABL:순종 <= 2
	LOCAL:5 = 200
ELSEIF ABL:순종 == 3
	LOCAL:5 = 500
ELSEIF ABL:순종 == 4
	LOCAL:5 = 1000
ELSEIF ABL:순종 >= 5
	LOCAL:5 = 2000
ENDIF


;습득
IF ABL:봉사정신 == 0
	LOCAL:6 = 50
ELSEIF ABL:봉사정신 == 1
	LOCAL:6 = 100
ELSEIF ABL:봉사정신 == 2
	LOCAL:6 = 200
ELSEIF ABL:봉사정신 == 3
	LOCAL:6 = 500
ELSEIF ABL:봉사정신 == 4
	LOCAL:6 = 1000
ELSEIF ABL:봉사정신 >= 5
	LOCAL:6 = 2000
ENDIF

;봉사정신に応じてボーナス
IF ABL:봉사정신 < 3
ELSEIF ABL:봉사정신 < 4
	LOCAL:5 += 500
	LOCAL:6 += 200
ELSEIF ABL:봉사정신 < 5
	LOCAL:5 += 1000
	LOCAL:6 += 500
ELSE
	LOCAL:5 += 2000
	LOCAL:6 += 1000
ENDIF

;追加責め実行
IF EX_COM
	CALL SEX_COMEX, 0, 0, EX_COM | SH_COM
	REPEAT 12
		LOCAL:COUNT += RESULT:COUNT
	REND
ENDIF

;行動予約なし
TFLAG:17 = -1
;촉수の行動を記録
TFLAG:20 = 10

;습득基本値
LOCAL:6 += 2000

;욕정基本値
LOCAL:7 += 0

;굴복基本値
LOCAL:8 += 400

;치정基本値
LOCAL:9 += 100

;고통基本値
LOCAL:10 += 0

;공포基本値
LOCAL:11 += 0

;체력消費基本値
LOCAL:12 += 50

;計算へ
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12
