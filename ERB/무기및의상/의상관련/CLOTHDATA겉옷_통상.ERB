SIF CUSTOM:3 == 2
	PRINT [초미니]
SIF CUSTOM:3 == 3
	PRINT [부르마]

SIF CUSTOM:3 != 1 && CUSTOM:4 == 3
	PRINT [절대영역]

SIF CUSTOM:4 == 0
	PRINT [삭스]
SIF CUSTOM:4 == 1
	PRINT [하이 삭스]
SIF CUSTOM:4 == 2
	PRINT [루즈 삭스]
SIF CUSTOM:4 == 3
	PRINT [니 하이 삭스]
SIF CUSTOM:4 == 4
	PRINT [검은 타이츠]
SIF CUSTOM:4 == 5
	PRINT [붕대]

PRINTL 
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 1
	PRINTL 　변신 시 추가：[호화로운 장식][경장갑]
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 2
	PRINTL 　변신 시 추가：[서클릿][롱글러브][롱 부츠]
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 3
	PRINTL 　변신 시 추가：[경장갑][견부 대수(大袖)][수갑(手甲)]
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 4
	PRINTL 　변신 시 추가：[기동병장 윙][허리 무장 포드]

@CLOTH_CUSTOMIZE_OPTION_101,ARG
#DIM CUSTOM, 5
#DIM CUSTOM_NUM = 5
#DIM ID
RESULT = 0
ID = (CFLAG:ARG:1 > 0) * 100 + 1
REPEAT CUSTOM_NUM
	CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:ID, COUNT + 1)
REND

TRYCALLFORM CLOTH_CUSTOMIZE_OPTION_DRAW_{(CFLAG:ARG:1 == 0 ? 100 # 0)+ID}(CUSTOM,ID,ARG)
DRAWLINE

CALL CLOTH_CUSTOMIZE_OPTION_COMMON(ID,ARG,"채색","천의 면적","소매 길이","치마 기장","삭스","@@")
$INPUT_LOOP
CALL CLOTH_CUSTOMIZE_OPTION_COMMON(-999)

IF RESULT == 0
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 채색할 색상을 선택해 주십시오.
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:0,"하양","검정","핑크","@@")
	CUSTOM:0 = RESULT
ELSEIF RESULT == 10
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 천의 면적을 선택해 주십시오.
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:1,"통상","작음@경1","매우 작음@경2","@@")
	CUSTOM:1 = RESULT
ELSEIF RESULT == 20
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 소매 길이를 선택해 주십시오.
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:2,"반소매","긴소매@중1","헐렁헐렁@중1","@@")
	CUSTOM:2 = RESULT
ELSEIF RESULT == 30
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 치마 기장을 선택해 주십시오.
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:3,"미니스커트","롱스커트@중2보2","엉덩이가 보일 듯한 초미니@경2백2","스커트가 아닌 부르마@경1백1","@@")
	CUSTOM:3 = RESULT
ELSEIF RESULT == 40
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 삭스를 선택해 주십시오.
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:4,"삭스","하이 삭스","루즈 삭스","니 하이 삭스","검은 타이츠@중1보1","붕대","@@")
	CUSTOM:4 = RESULT
ELSEIF RESULT == 50 && CFLAG:ARG:1 > 0
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 추가 장비를 선택해 주십시오.
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CFLAG:ARG:(900 + ID),"추가 장비 없음","정통파@（호화로운 장식＋경장갑）","클래식@（서클릿＋롱글러브＋하이 부츠）","사무라이 세라복@（경장갑＋견부 대수(大袖)＋수갑(手甲)）","기동병장: 유격사양@（기동병장 윙＋허리 무장 포드）","@@")
	CFLAG:ARG:(900 + ID) = RESULT
ELSEIF RESULT == 90
	CALL CLOTH_CUSTOMIZE_OPTION_INITIALIZE(ARG,ID,CUSTOM)
ELSEIF RESULT == 91 && CFLAG:ARG:40 == CFLAG:ARG:41
	CALL CLOTH_CUSTOMIZE_OPTION_COPY91(ARG,ID,CUSTOM_NUM,CUSTOM)
ELSEIF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
	CALL CLOTH_CUSTOMIZE_OPTION_COPY92(ARG,ID)
	RETURN -1
ELSEIF RESULT == 99
	RETURN 99
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF
CALL CLOTH_CUSTOMIZE_OPTION_SAVE(ARG,ID,CUSTOM_NUM,CUSTOM)

;노출 정도
LOCAL = 0
SIF CUSTOM:1 == 1
	LOCAL -= 1
SIF CUSTOM:1 == 2
	LOCAL -= 2
SIF CUSTOM:3 == 2
	LOCAL -= 2
SIF CUSTOM:3 == 3
	LOCAL -= 1

SIF CUSTOM:2 == 1 || CUSTOM:2 == 2
	LOCAL += 1
SIF CUSTOM:3 == 1
	LOCAL += 2
SIF CUSTOM:4 == 4
	LOCAL += 1

;보정 효과
;경장
SIF LOCAL < 0
	EQUIP:ARG:ID -= LOCAL * POWER(10, 9)
;중장
SIF LOCAL > 0
	EQUIP:ARG:ID += LOCAL * POWER(10, 10)
;속옷 보호 DOWN
SIF CUSTOM:3 == 2
	EQUIP:ARG:ID += 2 * POWER(10, 11)
SIF CUSTOM:3 == 3
	EQUIP:ARG:ID += 1 * POWER(10, 11)
;속옷 보호 UP
SIF CUSTOM:3 == 1
	EQUIP:ARG:ID += 2 * POWER(10, 12)
SIF CUSTOM:4 == 4
	EQUIP:ARG:ID += 1 * POWER(10, 12)

RETURN -1


