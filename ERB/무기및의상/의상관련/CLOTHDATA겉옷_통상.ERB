#DIM CUSTOM_NUM = 5
#DIM ID
RESULT = 0
ID = (CFLAG:ARG:1 > 0) * 100 + 1
REPEAT CUSTOM_NUM
	CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:ID, COUNT + 1)
REND

TRYCALLFORM CLOTH_CUSTOMIZE_OPTION_DRAW_{(CFLAG:ARG:1 == 0 ? 100 # 0)+ID}(CUSTOM,ID,ARG)
DRAWLINE

CALL CLOTH_CUSTOMIZE_OPTION_COMMON(ID,ARG,"カラーリング","布の面積","袖の長さ","スカート丈","ソックス","@@")
$INPUT_LOOP
CALL CLOTH_CUSTOMIZE_OPTION_COMMON(-999)

IF RESULT == 0
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL カラーリングを選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:0,"白","검정","핑크","@@")
	CUSTOM:0 = RESULT
ELSEIF RESULT == 10
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 布の面積を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:1,"通常","少なめ@軽1","かなり少ない@軽2","@@")
	CUSTOM:1 = RESULT
ELSEIF RESULT == 20
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 袖の長さを選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:2,"半袖","長袖@重1","ぶかぶか@重1","@@")
	CUSTOM:2 = RESULT
ELSEIF RESULT == 30
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL スカート丈を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:3,"ミニスカート","ロングスカート@重2護2","お尻が見えそうな超ミニ@軽2晒2","スカートではなくブルマ@軽1晒1","@@")
	CUSTOM:3 = RESULT
ELSEIF RESULT == 40
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL ソックスを選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:4,"ソックス","ハイソックス","ルーズソックス","ニーハイ","黒タイツ@重1護1","包帯","@@")
	CUSTOM:4 = RESULT
ELSEIF RESULT == 50 && CFLAG:ARG:1 > 0
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 追加装備を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CFLAG:ARG:(900 + ID),"追加装備なし","オーソドックス@（豪華な装飾＋軽装甲）","クラシック@（サークレット＋ロンググローブ＋ハイブーツ）","サムライセーラー@（軽装甲＋肩部大袖＋手甲）","機動兵装: 遊撃仕様@（機動兵装ウィング＋腰部武装ポッド）","@@")
	CFLAG:ARG:(900 + ID) = RESULT
ELSEIF RESULT == 90
	CALL CLOTH_CUSTOMIZE_OPTION_INITIALIZE(ARG,ID,CUSTOM)
ELSEIF RESULT == 91 && CFLAG:ARG:40 == CFLAG:ARG:41
	CALL CLOTH_CUSTOMIZE_OPTION_COPY91(ARG,ID,CUSTOM_NUM,CUSTOM)
ELSEIF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
	CALL CLOTH_CUSTOMIZE_OPTION_COPY92(ARG,ID)
	RETURN -1
ELSEIF RESULT == 99
	RETURN 99
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF
CALL CLOTH_CUSTOMIZE_OPTION_SAVE(ARG,ID,CUSTOM_NUM,CUSTOM)

;露出度合い
LOCAL = 0
SIF CUSTOM:1 == 1
	LOCAL -= 1
SIF CUSTOM:1 == 2
	LOCAL -= 2
SIF CUSTOM:3 == 2
	LOCAL -= 2
SIF CUSTOM:3 == 3
	LOCAL -= 1

SIF CUSTOM:2 == 1 || CUSTOM:2 == 2
	LOCAL += 1
SIF CUSTOM:3 == 1
	LOCAL += 2
SIF CUSTOM:4 == 4
	LOCAL += 1

;補正効果
;軽装
SIF LOCAL < 0
	EQUIP:ARG:ID -= LOCAL * POWER(10, 9)
;重装
SIF LOCAL > 0
	EQUIP:ARG:ID += LOCAL * POWER(10, 10)
;下着保護ダウン
SIF CUSTOM:3 == 2
	EQUIP:ARG:ID += 2 * POWER(10, 11)
SIF CUSTOM:3 == 3
	EQUIP:ARG:ID += 1 * POWER(10, 11)
;下着保護アップ
SIF CUSTOM:3 == 1
	EQUIP:ARG:ID += 2 * POWER(10, 12)
SIF CUSTOM:4 == 4
	EQUIP:ARG:ID += 1 * POWER(10, 12)

RETURN -1


