	PRINT [반소매]
SIF CUSTOM:2 == 1
	PRINT [긴소매]
SIF CUSTOM:2 == 2
	PRINT [헐렁한 긴소매]
SIF CUSTOM:2 == 3
	PRINT [민소매]

