	;※비키니라면 민소매, 거기에 레오타드화 중이라면 하카마, 앞치마＋레오타드화 중이라면 앞치마로 변경
	IF RESULT == 3
		CUSTOM:3 = 2
		SIF CUSTOM:4 == 3
			CUSTOM:4 = 0
		SIF CUSTOM:4 == 5
			CUSTOM:4 = 4
	ENDIF
ELSEIF RESULT == 30
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 소매 길이를 선택해 주십시오.
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:3,"긴소매","반소매@경1","민소매@경2","@@")
	CUSTOM:3 = RESULT
ELSEIF RESULT == 40
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 하반신의 착의를 선택해 주십시오.
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:4,"하카마","미니스커트@경1백2","엉덩이가 보일 듯한 초미니@경2백3","레오타드화@"+(CUSTOM:2 == 3 ? "!!비키니라면 선택할 수 없습니다." # "경2속옷겸용"),"앞치마@경1백1","앞치마＋레오타드화@"+(CUSTOM:2 == 3 ? "!!비키니라면 선택할 수 없습니다." # "경1속옷겸용"),"없음@경3백4","@@")
	CUSTOM:4 = RESULT
ELSEIF RESULT == 50
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 삭스를 선택해 주십시오.
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:5,"버선","니 하이 삭스","일본식 니 하이 삭스","붕대","망사 스타킹","타이츠@보1","팬티 스타킹@보1","없음","@@")
	CUSTOM:5 = RESULT
ELSEIF RESULT == 60
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 장식품을 선택해 주십시오.
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:6,"없음","머플러","@@")
	CUSTOM:6 = RESULT
ELSEIF RESULT == 70 && CFLAG:ARG:1 > 0
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 추가 장비를 선택해 주십시오.
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CFLAG:ARG:(900 + ID),"추가 장비 없음","정통파@（호화로운 장식＋경장갑）","@@")
	CFLAG:ARG:(900 + ID) = RESULT

ELSEIF RESULT == 90
	CALL CLOTH_CUSTOMIZE_OPTION_INITIALIZE(ARG,ID,CUSTOM)
ELSEIF RESULT == 91 && CFLAG:ARG:40 == CFLAG:ARG:41
	CALL CLOTH_CUSTOMIZE_OPTION_COPY91(ARG,ID,CUSTOM_NUM,CUSTOM)
ELSEIF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
	CALL CLOTH_CUSTOMIZE_OPTION_COPY92(ARG,ID)
	RETURN -1
ELSEIF RESULT == 99
	RETURN 99
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF


CALL CLOTH_CUSTOMIZE_OPTION_SAVE(ARG,ID,CUSTOM_NUM,CUSTOM)

;노출 정도
LOCAL = 0
SIF CUSTOM:1 <= 3 && CUSTOM:1 >= 7
	LOCAL -= 1
SIF CUSTOM:2 == 1 || CUSTOM:2 == 2
	LOCAL -= 1
SIF CUSTOM:2 == 3
	LOCAL -= 3
SIF CUSTOM:3 == 1
	LOCAL -= 1
SIF CUSTOM:3 == 2
	LOCAL -= 2
SIF CUSTOM:4 == 1 || CUSTOM:4 == 4 || CUSTOM:4 == 5
	LOCAL -= 1
SIF CUSTOM:4 == 2 || CUSTOM:4 == 3
	LOCAL -= 2
SIF CUSTOM:4 == 6
	LOCAL -= 3

SIF CUSTOM:1 == 2
	LOCAL += 1

;보정 효과
;경장
SIF LOCAL < 0
	EQUIP:ARG:ID -= LOCAL * POWER(10, 9)
;중장
SIF LOCAL > 0
	EQUIP:ARG:ID += LOCAL * POWER(10, 10)
;속옷 보호 DOWN
SIF CUSTOM:4 == 1
	EQUIP:ARG:ID += 2 * POWER(10, 11)
SIF CUSTOM:4 == 2
	EQUIP:ARG:ID += 3 * POWER(10, 11)
SIF CUSTOM:4 == 4
	EQUIP:ARG:ID += 1 * POWER(10, 11)
SIF CUSTOM:4 == 6
	EQUIP:ARG:ID += 4 * POWER(10, 11)
;속옷 보호 UP
SIF CUSTOM:5 == 5 || CUSTOM:5 == 6
	EQUIP:ARG:ID += 1 * POWER(10, 12)
;속옷겸용
SIF CUSTOM:2 == 3 || CUSTOM:4 == 3 || CUSTOM:4 == 5
	EQUIP:ARG:ID += 1 * POWER(10, 13)

RETURN -1

