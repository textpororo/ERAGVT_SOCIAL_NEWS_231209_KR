@CLOTH_CUSTOMIZE_OPTION_DRAW_153(CUSTOM,ID,ARG)
#DIM REF CUSTOM,0
#DIM ID,1
SIF CUSTOM:1 == 0
	PRINT [頭巾]
SIF CUSTOM:1 == 1
	PRINT [鉢金]
SIF CUSTOM:1 == 2
	PRINT [頭巾][鉢金]
SIF CUSTOM:1 == 3
	PRINT [かんざし]
SIF CUSTOM:1 == 4
	PRINT [リボン]
SIF CUSTOM:1 == 5
	PRINT [髪飾り]
SIF CUSTOM:1 == 6
	PRINT [カチューシャ]

SIF CUSTOM:0 == 0
	LOCALS=:黒
SIF CUSTOM:0 == 1
	LOCALS=:紅
SIF CUSTOM:0 == 2
	LOCALS=:紫
SIF CUSTOM:0 == 3
	LOCALS=:桃
SIF CUSTOM:0 == 4
	LOCALS=:水色
SIF CUSTOM:0 == 5
	LOCALS=:橙
SIF CUSTOM:0 == 6
	LOCALS=:翠
SIF CUSTOM:0 == 7
	LOCALS=:黄
SIF CUSTOM:0 == 8
	LOCALS=:白
SIF CUSTOM:0 == 9
	LOCALS=:紺


SIF CUSTOM:2 != 3
	PRINTFORM [닌자복%LOCALS%]

IF CUSTOM:2 == 1
	IF TALENT:ARG:거유 > 0
		PRINT [胸の谷間]
	ELSE
		PRINT [開いた胸元]
	ENDIF
ENDIF
SIF CUSTOM:2 == 2
	PRINT [へそ出し]
SIF CUSTOM:2 == 3
	PRINTFORM [ビキニ%LOCALS%]

SIF CUSTOM:3 == 0
	PRINT [長袖]
SIF CUSTOM:3 == 1
	PRINT [半袖]
SIF CUSTOM:3 == 2 && CUSTOM:2 != 3
	PRINT [袖なし]

SIF CUSTOM:4 == 0
	PRINT [袴]
SIF CUSTOM:4 == 1
	PRINT [ミニスカート]
SIF CUSTOM:4 == 2
	PRINT [超ミニ]
SIF CUSTOM:4 == 3
	PRINT [レオタード]
SIF CUSTOM:4 == 4
	PRINT [前垂れ]
SIF CUSTOM:4 == 5
	PRINT [前垂れ][レオタード]

SIF (CUSTOM:5 == 1 || CUSTOM:5 == 2) && CUSTOM:4 != 0
	PRINT [絶対領域]

SIF CUSTOM:5 == 0
	PRINT [足袋]
SIF CUSTOM:5 == 1
	PRINT [ニーハイ]
SIF CUSTOM:5 == 2
	PRINT [和風ニーハイ]
SIF CUSTOM:5 == 3
	PRINT [包帯]
SIF CUSTOM:5 == 4
	PRINT [網タイツ]
SIF CUSTOM:5 == 5
	PRINT [タイツ]
SIF CUSTOM:5 == 6
	PRINT [パンスト]

SIF CUSTOM:6 == 1
	PRINT [マフラー]

PRINTL 
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 1
	PRINTL 　変身時追加：[豪華な装飾][軽装甲]

@CLOTH_CUSTOMIZE_OPTION_153,ARG
#DIM CUSTOM, 7
#DIM CUSTOM_NUM = 7
#DIM ID
RESULT = 0
ID = (CFLAG:ARG:1 > 0) * 100 + 53
REPEAT CUSTOM_NUM
	CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:ID, COUNT + 1)
REND


TRYCALLFORM CLOTH_CUSTOMIZE_OPTION_DRAW_{(CFLAG:ARG:1 == 0 ? 100 # 0)+ID}(CUSTOM,ID,ARG)
DRAWLINE

CALL CLOTH_CUSTOMIZE_OPTION_COMMON(ID,ARG,"基調色","頭部","上半身","袖"+(CUSTOM:2 == 3 ? "!!ビキニを設定中は変更できません" # ""),"下半身","ソックス","装飾品","@@")
$INPUT_LOOP
CALL CLOTH_CUSTOMIZE_OPTION_COMMON(-999)

IF RESULT == 0
	PRINTL 基調色を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:0,"검정","紅","보라","분홍","水色","주황","翠","노랑","白","紺","@@")
	CUSTOM:0 = RESULT
ELSEIF RESULT == 10
	PRINTL 頭部の装備を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:1,"頭巾","鉢金","頭巾＋鉢金@重1","かんざし@軽1","リボン@軽1","髪飾り@軽1","カチューシャ@軽1","無し@軽1","@@")
	CUSTOM:1 = RESULT
ELSEIF RESULT == 20
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 上半身の着衣を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:2,"通常","着崩し@軽1","へそ出し@軽1","ビキニ@軽3속옷겸용","@@")
	CUSTOM:2 = RESULT
	;※ビキニなら袖なし、更にレオタード化中なら袴、前垂れ＋レオタード化中なら前垂れに変更
	IF RESULT == 3
		CUSTOM:3 = 2
		SIF CUSTOM:4 == 3
			CUSTOM:4 = 0
		SIF CUSTOM:4 == 5
			CUSTOM:4 = 4
	ENDIF
ELSEIF RESULT == 30
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 袖の長さを選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:3,"長袖","半袖@軽1","袖なし@軽2","@@")
	CUSTOM:3 = RESULT
ELSEIF RESULT == 40
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 下半身の着衣を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:4,"袴","ミニスカート@軽1晒2","お尻が見えそうな超ミニ@軽2晒3","レオタード化@"+(CUSTOM:2 == 3 ? "!!ビキニだと選択できません" # "軽2속옷겸용"),"前垂れ@軽1晒1","前垂れ＋レオタード化@"+(CUSTOM:2 == 3 ? "!!ビキニだと選択できません" # "軽1속옷겸용"),"無し@軽3晒4","@@")
	CUSTOM:4 = RESULT
ELSEIF RESULT == 50
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL ソックスを選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:5,"足袋","ニーハイ","和風ニーハイ","包帯","網タイツ","タイツ@護1","パンスト@護1","無し","@@")
	CUSTOM:5 = RESULT
ELSEIF RESULT == 60
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 装飾品を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:6,"なし","マフラー","@@")
	CUSTOM:6 = RESULT
ELSEIF RESULT == 70 && CFLAG:ARG:1 > 0
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 追加装備を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CFLAG:ARG:(900 + ID),"追加装備なし","オーソドックス@（豪華な装飾＋軽装甲）","@@")
	CFLAG:ARG:(900 + ID) = RESULT

ELSEIF RESULT == 90
	CALL CLOTH_CUSTOMIZE_OPTION_INITIALIZE(ARG,ID,CUSTOM)
ELSEIF RESULT == 91 && CFLAG:ARG:40 == CFLAG:ARG:41
	CALL CLOTH_CUSTOMIZE_OPTION_COPY91(ARG,ID,CUSTOM_NUM,CUSTOM)
ELSEIF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
	CALL CLOTH_CUSTOMIZE_OPTION_COPY92(ARG,ID)
	RETURN -1
ELSEIF RESULT == 99
	RETURN 99
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF


CALL CLOTH_CUSTOMIZE_OPTION_SAVE(ARG,ID,CUSTOM_NUM,CUSTOM)

;露出度合い
LOCAL = 0
SIF CUSTOM:1 <= 3 && CUSTOM:1 >= 7
	LOCAL -= 1
SIF CUSTOM:2 == 1 || CUSTOM:2 == 2
	LOCAL -= 1
SIF CUSTOM:2 == 3
	LOCAL -= 3
SIF CUSTOM:3 == 1
	LOCAL -= 1
SIF CUSTOM:3 == 2
	LOCAL -= 2
SIF CUSTOM:4 == 1 || CUSTOM:4 == 4 || CUSTOM:4 == 5
	LOCAL -= 1
SIF CUSTOM:4 == 2 || CUSTOM:4 == 3
	LOCAL -= 2
SIF CUSTOM:4 == 6
	LOCAL -= 3

SIF CUSTOM:1 == 2
	LOCAL += 1

;補正効果
;軽装
SIF LOCAL < 0
	EQUIP:ARG:ID -= LOCAL * POWER(10, 9)
;重装
SIF LOCAL > 0
	EQUIP:ARG:ID += LOCAL * POWER(10, 10)
;下着保護ダウン
SIF CUSTOM:4 == 1
	EQUIP:ARG:ID += 2 * POWER(10, 11)
SIF CUSTOM:4 == 2
	EQUIP:ARG:ID += 3 * POWER(10, 11)
SIF CUSTOM:4 == 4
	EQUIP:ARG:ID += 1 * POWER(10, 11)
SIF CUSTOM:4 == 6
	EQUIP:ARG:ID += 4 * POWER(10, 11)
;下着保護アップ
SIF CUSTOM:5 == 5 || CUSTOM:5 == 6
	EQUIP:ARG:ID += 1 * POWER(10, 12)
;속옷겸용
SIF CUSTOM:2 == 3 || CUSTOM:4 == 3 || CUSTOM:4 == 5
	EQUIP:ARG:ID += 1 * POWER(10, 13)

RETURN -1

