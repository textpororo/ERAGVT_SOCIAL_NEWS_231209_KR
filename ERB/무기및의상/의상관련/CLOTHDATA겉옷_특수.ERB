;----------------------------------------------------------
;固有カスタマイズ
@CLOTH_CUSTOMIZE_OPTION_DRAW_199(CUSTOM,ID,ARG)
#DIM REF CUSTOM,0
#DIM ID,1
SIF CUSTOM:0 == 1
	PRINT [ボディスーツ]
SIF CUSTOM:0 == 2
	PRINT [ワンピース]
SIF CUSTOM:0 == 3
	PRINT [レオタード]
SIF CUSTOM:0 == 4
	PRINT [메이드복]
SIF CUSTOM:0 == 5
	PRINT [무당복]

PRINTFORM [強化Lv{CUSTOM:1}]

SIF CUSTOM:2 == 1
	PRINT [공격 강화]
SIF CUSTOM:2 == 2
	PRINT [방어 강화]
SIF CUSTOM:2 == 3
	PRINT [민첩 강화]

SIF CUSTOM:3 == 1
	PRINT [성내성消費軽減Lv1]
SIF CUSTOM:3 == 2
	PRINT [성내성消費軽減Lv2]
SIF CUSTOM:3 == 3
	PRINT [성내성消費半減]
SIF CUSTOM:3 == 4
	PRINT [常時暴走化]

PRINTL 
SIF CFLAG:ARG:1 > 0 && CFLAG:ARG:(900 + ID) == 1
	PRINTL 　変身時追加：[豪華な装飾][軽装甲]

@CLOTH_CUSTOMIZE_OPTION_199,ARG
#DIM CUSTOM, 7
#DIM CUSTOM_NUM = 7
#DIM ID
RESULT = 0
ID = (CFLAG:ARG:1 > 0) * 100 + 99
REPEAT CUSTOM_NUM
	CUSTOM:COUNT = FIGURE_SPLIT(EQUIP:ARG:ID, COUNT + 1)
REND

TRYCALLFORM CLOTH_CUSTOMIZE_OPTION_DRAW_{(CFLAG:ARG:1 == 0 ? 100 # 0)+ID}(CUSTOM,ID,ARG)
DRAWLINE

CALL CLOTH_CUSTOMIZE_OPTION_COMMON(ID,ARG,"形状"+(FLAG:54 == 0 ? "!!研究레벨が足りません" # ""),"能力強化"+(FLAG:54 == 0 ? "!!研究레벨が足りません" # ""),"個別能力強化"+(FLAG:54 == 0 ? "!!研究레벨が足りません" # ""),"성내성消費軽減"+(FLAG:54 == 0 ? "!!研究레벨が足りません" # ""),"@@")
$INPUT_LOOP
CALL CLOTH_CUSTOMIZE_OPTION_COMMON(-999)

IF RESULT == 0
	PRINTL 形状を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:0,"デフォルト","ボディスーツ","ワンピース","レオタード","메이드복","무당복","@@")
	CUSTOM:0 = RESULT
ELSEIF RESULT == 10
	PRINTL 能力強化Lvを選択してください
	PRINTL Lvが高いほど공격、방어、민첩補正とデメリットが強化されます
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:1,"Lv0(デフォルト)","Lv1@"+(FLAG:54 < 1 ? "!!研究레벨が足りません" # ""),"Lv2@"+(FLAG:54 < 2 ? "!!研究레벨が足りません" # ""),"Lv3@"+(FLAG:54 < 3 ? "!!研究레벨が足りません" # ""),"Lv4@"+(FLAG:54 < 4 ? "!!研究레벨が足りません" # ""),"Lv5@"+(FLAG:54 < 5 ? "!!研究레벨が足りません" # ""),"@@")
	CUSTOM:1 = RESULT
ELSEIF RESULT == 20
	PRINTL 強化する項目を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:2,"デフォルト","공격補正強化＆성내성消費微増@"+(FLAG:54 < 3 ? "!!研究레벨が足りません" # ""),"방어補正強化＆성내성消費微増@"+(FLAG:54 < 3 ? "!!研究레벨が足りません" # ""),"민첩補正強化＆성내성消費微増@"+(FLAG:54 < 3 ? "!!研究레벨が足りません" # ""),"@@")
	CUSTOM:2 = RESULT
ELSEIF RESULT == 30
	PRINTL 項目を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CUSTOM:3,"デフォルト","성내성消費軽減＆暴走強化Lv1@"+(FLAG:54 < 2 ? "!!研究레벨が足りません" # ""),"성내성消費軽減＆暴走強化Lv2@"+(FLAG:54 < 4 ? "!!研究레벨が足りません" # ""),"성내성消費半減＆低確率で暴走@"+(FLAG:54 < 3 ? "!!研究레벨が足りません" # ""),"常時暴走化＆能力補正強化@"+(FLAG:54 < 5 ? "!!研究레벨が足りません" # ""),"@@")
	CUSTOM:3 = RESULT
ELSEIF RESULT == 40 && CFLAG:ARG:1 > 0
	CLEARLINE 1
	CALL SHORTLINE
	PRINTL 追加装備を選択してください
	CALL CLOTH_CUSTOMIZE_OPTION_PRINT(CFLAG:ARG:(900 + ID),"追加装備なし","オーソドックス@（豪華な装飾＋軽装甲）","@@")
	CFLAG:ARG:(900 + ID) = RESULT

ELSEIF RESULT == 90
	CALL CLOTH_CUSTOMIZE_OPTION_INITIALIZE(ARG,ID,CUSTOM)
ELSEIF RESULT == 91 && CFLAG:ARG:40 == CFLAG:ARG:41
	CALL CLOTH_CUSTOMIZE_OPTION_COPY91(ARG,ID,CUSTOM_NUM,CUSTOM)
ELSEIF RESULT == 92 && CFLAG:ARG:40 == CFLAG:ARG:41
	CALL CLOTH_CUSTOMIZE_OPTION_COPY92(ARG,ID)
	RETURN -1
ELSEIF RESULT == 99
	RETURN 99
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF


CALL CLOTH_CUSTOMIZE_OPTION_SAVE(ARG,ID,CUSTOM_NUM,CUSTOM)
RETURN -1
