CALL CLOTH_CUSTOMIZE_OPTION_COMMON(ID,ARG,"カラーリング","デザイン","トップスの種類","ショーツの種類","@@")
$INPUT_LOOP
CALL CLOTH_CUSTOMIZE_OPTION_COMMON(-999)

IF RESULT == 0
	PRINTL カラーリングを選択してください
	CALL INNER_CUSTOMIZE_OPTION_PRINT(CUSTOM:0,"ホワイト","ブラック","ピンク","レッド","ブルー","グリーン","イエロー","オレンジ","パープル","ネイビー","@@")
	CUSTOM:0 = RESULT
ELSEIF RESULT == 10
	PRINTL デザインを選択してください
	CALL INNER_CUSTOMIZE_OPTION_PRINT(CUSTOM:1,"無地","ブランドロゴ@油2","レース@油2","チェック@油1","ドット@油1","ボーダー@油1","プリント@油1","@@")
	CUSTOM:1 = RESULT
ELSEIF RESULT == 20
	PRINTL トップスを選択してください
	CALL INNER_CUSTOMIZE_OPTION_PRINT(CUSTOM:2,"スポーツブラ","ブラトップ","ブラキャミ","チューブトップ","ハーフトップ","@@")
	CUSTOM:2 = RESULT
ELSEIF RESULT == 30
	PRINTL ショーツを選択してください
	CALL INNER_CUSTOMIZE_OPTION_PRINT(CUSTOM:3,"ビキニ","スタンダード","ローライズ@軽1油1晒1","スポーツタイプ@重1油2護2","ボーイレッグ@重1油2護2","@@")
	CUSTOM:3 = RESULT
ELSEIF RESULT == 90
	CALL CLOTH_CUSTOMIZE_OPTION_INITIALIZE(ARG,ID,CUSTOM)
ELSEIF RESULT == 99
	RETURN 99
ELSE
	CLEARLINE 1
	GOTO INPUT_LOOP
ENDIF
CALL CLOTH_CUSTOMIZE_OPTION_SAVE(ARG,ID,CUSTOM_NUM,CUSTOM)

;補正効果
LOCAL = 0
SIF CUSTOM:3 == 2
	LOCAL -= 1

SIF CUSTOM:3 == 3
	LOCAL += 1
SIF CUSTOM:3 == 4
	LOCAL += 1

;軽装
SIF LOCAL < 0
	EQUIP:ARG:ID -= LOCAL * POWER(10, 9)
;重装
SIF LOCAL > 0
	EQUIP:ARG:ID += LOCAL * POWER(10, 10)

;油断度ダウン
SIF CUSTOM:2 == 1
	EQUIP:ARG:ID += 2 * POWER(10, 13)
SIF CUSTOM:3 == 3
	EQUIP:ARG:ID += 2 * POWER(10, 13)
SIF CUSTOM:3 == 4
	EQUIP:ARG:ID += 2 * POWER(10, 13)

;油断度アップ
SIF CUSTOM:1 == 2
	EQUIP:ARG:ID += 2 * POWER(10, 14)
SIF CUSTOM:1 == 3
	EQUIP:ARG:ID += 1 * POWER(10, 14)
SIF CUSTOM:1 == 4
	EQUIP:ARG:ID += 1 * POWER(10, 14)
SIF CUSTOM:1 == 5
	EQUIP:ARG:ID += 1 * POWER(10, 14)
SIF CUSTOM:1 == 6
	EQUIP:ARG:ID += 1 * POWER(10, 14)
SIF CUSTOM:3 == 2
	EQUIP:ARG:ID += 1 * POWER(10, 14)

;挿入抵抗ダウン
SIF CUSTOM:3 == 2
	EQUIP:ARG:ID += 1 * POWER(10, 11)

;挿入抵抗アップ
SIF CUSTOM:3 == 3
	EQUIP:ARG:ID += 2 * POWER(10, 12)
SIF CUSTOM:3 == 4
	EQUIP:ARG:ID += 2 * POWER(10, 12)

RETURN -1

