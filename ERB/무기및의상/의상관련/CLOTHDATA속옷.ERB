SIF CUSTOM:3 == 4
	EQUIP:ARG:ID += 2 * POWER(10, 14)
SIF CUSTOM:3 == 6
	EQUIP:ARG:ID += 3 * POWER(10, 14)
SIF CUSTOM:3 == 7
	EQUIP:ARG:ID += 4 * POWER(10, 14)
SIF CUSTOM:4 == 1
